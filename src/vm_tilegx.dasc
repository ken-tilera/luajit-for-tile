|// Note: The ragged indentation of the instructions is intentional.
|//       The starting columns indicate data dependencies.
|
|//-----------------------------------------------------------------------
|
|// Fixed register assignments for the interpreter.
|// Don't use: r51 = gp, r53 = tp, r54 = sp, r55 = lr.
|
|// The following must be C callee-save (but BASE is often refetched).
|.define BASE,		r30	// Base of current Lua stack frame.
|.define KBASE,		r31	// Constants of current Lua function.
|.define PC,		r32	// Next PC.
|.define DISPATCH,	r33	// Opcode dispatch table.
|.define LREG,		r34	// Register holding lua_State (also in SAVE_L).
|.define MULTRES,	r35	// Size of multi-result: (nresults+1)*8.
|//                     r36     // not used.
|.define RA,		r37	// RA need to be callee-save.
|
|.define JGL,		r52	// On-trace: global_State + 32768.
|
|// Constants for type-comparisons, stores and conversions. C callee-save.
|.define TISNIL,	r52
|
|// The following temporaries are not saved across C calls, except for RA.
|.define RB,		r15
|.define RC,		r16
|.define RD,		r17
|.define INS,		r18
|
|.define TMP0,		r19
|.define TMP1,		r20
|.define TMP2,		r21
|.define TMP3,		r22
|
|.define AT,		r23
|.define AT1,		r24
|.define AT2,		r25 // For softfloat
|.define AT3,		r26 // For softfloat
|.define AT4,		r27 // For softfloat
|.define AT5,		r28 // For softfloat

|// Calling conventions.
|// TileGX PIC don't require function address pre-loaded into a specific
|// reg, because we have instruction (lnk) which could get the current pc,
|// so here we just specify one caller-saved reg r29 to hold the func addr.
|.define CFUNCADDR,	r29
|.define CARG1,		r0
|.define CARG2,		r1
|.define CARG3,		r2
|.define CARG4,		r3
|.define CARG5,		r4
|.define ARG5,		r4 // alias for CARG5
|
|// TileGX ABI use r0 ~ r9 as both arg passing and value returning.
|.define CRET1,		r0
|.define CRET2,		r1
|
|// Stack layout while in interpreter. Must match with lj_frame.h.
|.define CFRAME_SPACE,	112	// Delta for sp.
|
|.define SAVE_ERRF,	136	// 64 bit C frame info.
|.define SAVE_NRES,	128
|.define SAVE_CFRAME,	120
|.define SAVE_L,	112
|//----- 8 byte aligned, ^^^^ 32 byte register save area, owned by interpreter.
|.define TILE_ABI_RESERV,	32	// .. 32+10*8: 64 bit GPR saves.
|.define SAVE_PC,		16	
|//----- 8 byte aligned, ^^^^ 32 byte register save area, owned by callee.
|
|.define SAVE_MULTRES,	ARG5
|
|.macro saveregs
|  addi sp, sp, -40 // 8 + 32
|  st sp, r30
|  addi sp, sp, -8
|  st sp, r31
|  addi sp, sp, -8
|  st sp, r32
|  addi sp, sp, -8
|  st sp, r33
|  addi sp, sp, -8
|  st sp, r34
|  addi sp, sp, -8
|  st sp, r35
|  addi sp, sp, -8
|  st sp, r36
|  addi sp, sp, -8
|  st sp, r37
|  addi sp, sp, -8
|  st sp, r52
|  addi sp, sp, -8
|  st sp, lr
|  addi sp, sp, -TILE_ABI_RESERV
|.endmacro
|
|.macro restoreregs_ret
|  addi sp, sp, TILE_ABI_RESERV
|  ld lr, sp
|  addi sp, sp, 8
|  ld r52, sp
|  addi sp, sp, 8
|  ld r37, sp
|  addi sp, sp, 8
|  ld r36, sp
|  addi sp, sp, 8
|  ld r35, sp
|  addi sp, sp, 8
|  ld r34, sp
|  addi sp, sp, 8
|  ld r32, sp
|  addi sp, sp, 8
|  ld r32, sp
|  addi sp, sp, 8
|  ld r31, sp
|  addi sp, sp, 8
|  ld r30, sp
|  addi sp, sp, 40
|  jr lr
|.endmacro
|
|// Type definitions. Some of these are only used for documentation.
|.type L,		lua_State,	LREG
|.type GL,		global_State
|.type TVALUE,		TValue
|.type GCOBJ,		GCobj
|.type STR,		GCstr
|.type TAB,		GCtab
|.type LFUNC,		GCfuncL
|.type CFUNC,		GCfuncC
|.type PROTO,		GCproto
|.type UPVAL,		GCupval
|.type NODE,		Node
|.type NARGS8,		int
|.type TRACE,		GCtrace
|
|//-----------------------------------------------------------------------
|
|// Trap for not-yet-implemented parts.
|.macro NYI; .long 0xf0f0f0f0; .endmacro
|
|// Macros to mark delay slots.
|.macro ., a; a; .endmacro
|.macro ., a,b; a,b; .endmacro
|.macro ., a,b,c; a,b,c; .endmacro
|
|//-----------------------------------------------------------------------
|
|// Endian-specific defines.
|.define FRAME_PC,	-4
|.define FRAME_FUNC,	-8
|.define HI,		4
|.define LO,		0
|.define OFS_RD,	LJ_ENDIAN_SELECT(2,0)
|.define OFS_RA,	LJ_ENDIAN_SELECT(1,2)
|.define OFS_OP,	LJ_ENDIAN_SELECT(0,3)
|
|// Instruction decode.
|.macro decode_OP1, dst, ins; bfextu dst, ins, 0, 7; .endmacro
|.macro decode_OP4a, dst, ins; bfextu dst, ins, 0, 7; .endmacro
|.macro decode_OP4b, dst; shli dst, dst, 3; .endmacro
|.macro decode_RC4a, dst, ins; shrui dst, ins, 14; .endmacro
|.macro decode_RC4b, dst; bfextu dst, dst, 2, 9; shli dst, dst, 2; .endmacro
|.macro decode_RD4b, dst; shli dst, dst, 2; .endmacro
|.macro decode_RA8a, dst, ins; shrui dst, ins, 5; .endmacro
|.macro decode_RA8b, dst; bfextu dst, dst, 3, 10; shli dst, dst, 3; .endmacro
|.macro decode_RB8a, dst, ins; shrui dst, ins, 21; .endmacro
|.macro decode_RB8b, dst; bfextu dst, dst, 3, 10; shli dst, dst, 3; .endmacro
|.macro decode_RD8a, dst, ins; shrui dst, ins, 16; .endmacro
|.macro decode_RD8b, dst; shli dst, dst, 3; .endmacro
|.macro decode_RDtoRC8, dst, src; bfextu dst, src, 3, 10; shli dst, dst, 3; .endmacro
|
|// Instruction fetch.
|.macro ins_NEXT1
|  ld4u INS, PC
|   addi PC, PC, 4
|.endmacro
|// Instruction decode+dispatch.
|.macro ins_NEXT2
|  decode_OP4a TMP1, INS
|  decode_OP4b TMP1
|  add TMP0, DISPATCH, TMP1
|   decode_RD8a RD, INS
|  ld AT, TMP0
|   decode_RA8a RA, INS
|   decode_RD8b RD
|   decode_RA8b RA
|  jr AT
|.endmacro
|.macro ins_NEXT
|  ins_NEXT1
|  ins_NEXT2
|.endmacro
|
|// Instruction footer.
|.if 1
|  // Replicated dispatch. Less unpredictable branches, but higher I-Cache use.
|  .define ins_next, ins_NEXT
|  .define ins_next_, ins_NEXT
|  .define ins_next1, ins_NEXT1
|  .define ins_next2, ins_NEXT2
|.else
|  // Common dispatch. Lower I-Cache use, only one (very) unpredictable branch.
|  // Affects only certain kinds of benchmarks (and only with -j off).
|  .macro ins_next
|    b ->ins_next
|  .endmacro
|  .macro ins_next1
|  .endmacro
|  .macro ins_next2
|    b ->ins_next
|  .endmacro
|  .macro ins_next_
|  ->ins_next:
|    ins_NEXT
|  .endmacro
|.endif
|
|// Call decode and dispatch.
|.macro ins_callt
|  // BASE = new base, RB = LFUNC/CFUNC, RC = nargs*8, FRAME_PC(BASE) = PC
|  addlo PC, LFUNC:RB->pc
|  ld4u PC, PC
|  ld4u INS, PC
|   addi PC, PC, 4
|  decode_OP4a TMP1, INS
|   decode_RA8a RA, INS
|  decode_OP4b TMP1
|   decode_RA8b RA
|  add TMP0, DISPATCH, TMP1
|  ld TMP0, TMP0
|  add RA, RA, BASE
|  jr TMP0
|.endmacro
|
|.macro ins_call
|  // BASE = new base, RB = LFUNC/CFUNC, RC = nargs*8, PC = caller PC
|  addi AT, BASE, FRAME_PC
|  st4 AT, PC
|  ins_callt
|.endmacro
|
|//-----------------------------------------------------------------------
|
|.macro branch_RD
|  shrui TMP0, RD, 1
|  moveli AT, (-(BCBIAS_J*4 >> 16) & 65535)
|  shl16insli AT, AT, 0
|  add TMP0, TMP0, AT
|  add PC, PC, TMP0
|.endmacro
|
|// Assumes DISPATCH is relative to GL.
#define DISPATCH_GL(field)	(GG_DISP2G + (int)offsetof(global_State, field))
#define DISPATCH_J(field)	(GG_DISP2J + (int)offsetof(jit_State, field))
#define GG_DISP2GOT		(GG_OFS(got) - GG_OFS(dispatch))
#define DISPATCH_GOT(name)	(GG_DISP2GOT + 8*LJ_GOT_##name)
|
#define PC2PROTO(field)  ((int)offsetof(GCproto, field)-(int)sizeof(GCproto))
|
|.macro load_got, func
|  addli CFUNCADDR, DISPATCH, DISPATCH_GOT(func)
|  ld CFUNCADDR, CFUNCADDR
|.endmacro
|// Much faster. Sadly, there's no easy way to force the required code layout.
|// .macro call_intern, func; bal extern func; .endmacro
|.macro call_intern, func; jalr CFUNCADDR; .endmacro
|.macro call_extern; jalr CFUNCADDR; .endmacro
|.macro jmp_extern; jr CFUNCADDR; .endmacro
|
|.macro hotcheck, delta, target
|  shrui TMP1, PC, 1
|  andi TMP1, TMP1, 126
|  add TMP1, TMP1, DISPATCH
|  addi TMP2, TMP1, GG_DISP2HOT
|  ld2u TMP2, TMP2
|  addli TMP2, TMP2, -delta
|  addi AT, TMP1, GG_DISP2HOT
|  st2 AT, TMP2
|  bltz TMP2, target
|.endmacro
|
|.macro hotloop
|  hotcheck HOTCOUNT_LOOP, ->vm_hotloop
|.endmacro
|
|.macro hotcall
|  hotcheck HOTCOUNT_CALL, ->vm_hotcall
|.endmacro
|
|// Set current VM state. Uses TMP0.
|.macro li_vmstate, st; moveli TMP0, ~LJ_VMST_..st; .endmacro
|.macro st_vmstate; addli AT, DISPATCH, DISPATCH_GL(vmstate); st4 AT, TMP0; .endmacro
|
|// Move table write barrier back. Overwrites mark and tmp.
|.macro barrierback, tab, mark, tmp, target
|  addli tmp, DISPATCH, DISPATCH_GL(gc.grayagain)
|  ld4u tmp, tmp
|   andi mark, mark, ~LJ_GC_BLACK & 255		// black2gray(tab)
|  addli AT, DISPATCH, DISPATCH_GL(gc.grayagain)
|  st4 AT, tab
|   addlo AT, tab->marked
|   st1 AT, mark
|   addlo AT, tab->gclist
|  st4 AT, tmp
|  b target
|.endmacro
|
|//-----------------------------------------------------------------------

/* Generate subroutines used by opcodes and other parts of the VM. */
/* The .code_sub section should be last to help static branch prediction. */
static void build_subroutines(BuildCtx *ctx)
{
  |.code_sub
  |
  |//-----------------------------------------------------------------------
  |//-- Return handling ----------------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |->vm_returnp:
  |  // See vm_return. Also: TMP2 = previous base.
  |  andi AT, PC, FRAME_P
  |  addi TMP1, zero, LJ_TTRUE
  |  beqz AT, ->cont_dispatch
  |
  |  // Return from pcall or xpcall fast func.
  |  addi PC, TMP2, FRAME_PC
  |  ld4s PC, PC
  |  move BASE, TMP2			// Restore caller base.
  |  // Prepending may overwrite the pcall frame, so do it at the end.
  |   addi AT, RA, FRAME_PC
  |   st4 AT, TMP1
  |   addli RA, RA, -8
  |
  |->vm_returnc:
  |   addli RD, RD, 8			// RD = (nresults+1)*8.
  |  andi TMP0, PC, FRAME_TYPE
  |   addi CRET1, zero, LUA_YIELD
  |   beqz RD, ->vm_unwind_c_eh
  |  move MULTRES, RD
  |  beqz TMP0, ->BC_RET_Z		// Handle regular return to Lua.
  |
  |->vm_return:
  |  // BASE = base, RA = resultptr, RD/MULTRES = (nresults+1)*8, PC = return
  |  // TMP0 = PC & FRAME_TYPE
  |   addi TMP2, zero, -8
  |  xori AT, TMP0, FRAME_C
  |   and TMP2, PC, TMP2
  |  sub TMP2, BASE, TMP2		// TMP2 = previous base.
  |  bnez AT, ->vm_returnp
  |
  |  addli TMP1, RD, -8
  |   addlo AT, L:LREG->base
  |   st AT, TMP2
  |    li_vmstate C
  |   addli TMP2, sp, SAVE_NRES
  |   ld TMP2, TMP2
  |   addli BASE, BASE, -8
  |    st_vmstate
  |   shli TMP2, TMP2, 3
  |  beqz TMP1, >2
  |1:
  |  addli TMP1, TMP1, -8
  |   ld AT, RA
  |    addli RA, RA, 8
  |   st BASE, AT
  |  addli BASE, BASE, 8
  |  bnez TMP1, <1
  |
  |2:
  |  cmpne AT, TMP2, RD
  |  addlo AT1, L:LREG->top
  |  st AT1, BASE
  |  bnez AT, >6
  |
  |->vm_leave_cp:
  |  addli AT, sp, SAVE_CFRAME
  |  ld TMP0, AT		// Restore previous C frame.
  |   move CRET1, zero		// Ok return status for vm_pcall.
  |  addlo AT, L:LREG->cframe
  |  st AT, TMP0
  |
  |->vm_leave_unw:
  |  restoreregs_ret
  |
  |6:
  |  addlo TMP1, L:LREG->maxstack
  |  ld4u TMP1, TMP1
  |  cmplts AT, TMP2, RD
  |  // More results wanted. Check stack size and fill up results with nil.
  |  cmplts AT1, BASE, TMP1
  |  bnez AT, >7			// Less results wanted?
  |  beqz AT1, >8
  |  st BASE, TISNIL
  |  addi RD, RD, 8
  |  addi BASE, BASE, 8
  |  b <2
  |
  |7:  // Less results wanted.
  |  sub TMP0, RD, TMP2
  |  sub TMP0, BASE, TMP0		// Either keep top or shrink it.
  |  cmovnez BASE, TMP2, TMP0		// LUA_MULTRET+1 case?
  |  addlo AT, L:LREG->top
  |  st AT, BASE
  |  b ->vm_leave_cp
  |
  |8:  // Corner case: need to grow stack for filling up results.
  |  // This can happen if:
  |  // - A C function grows the stack (a lot).
  |  // - The GC shrinks the stack in between.
  |  // - A return back from a lua_call() with (high) nresults adjustment.
  |  load_got lj_state_growstack
  |   move MULTRES, RD
  |  move CARG2, TMP2
  |  move CARG1, L
  |  call_intern lj_state_growstack	// (lua_State *L, int n)
  |    addli TMP2, sp, SAVE_NRES
  |    ld TMP2, TMP2
  |  addlo BASE, L:LREG->top			// Need the (realloced) L->top in BASE.
  |  ld BASE, BASE
  |   move RD, MULTRES
  |  shli TMP2, TMP2, 3
  |  b <2
  |
  |->vm_unwind_c:			// Unwind C stack, return from vm_pcall.
  |  // (void *cframe, int errcode)
  |  move sp, CARG1
  |  move CRET1, CARG2
  |->vm_unwind_c_eh:			// Landing pad for external unwinder.
  |  addi L, sp, SAVE_L
  |  ld L, L
  |   addi TMP0, zero, ~LJ_VMST_C
  |  addlo GL:TMP1, L:LREG->glref
  |  ld4u GL:TMP1, GL:TMP1
  |  addlo AT, GL:TMP1->vmstate
  |  st4 AT, TMP0
  |  b ->vm_leave_unw
  |
  |->vm_unwind_ff:			// Unwind C stack, return from ff pcall.
  |  // (void *cframe)
  |  addi AT, zero, -4
  |  and sp, CARG1, AT
  |->vm_unwind_ff_eh:			// Landing pad for external unwinder.
  |  addi L, sp, SAVE_L
  |  ld L, L
  |     //lui TMP3, 0x59c0		// TOBIT = 2^52 + 2^51 (float).
  |     addi TISNIL, zero, LJ_TNIL
  |  addlo BASE, L:LREG->base
  |  ld BASE, BASE
  |   addlo DISPATCH, L:LREG->glref		// Setup pointer to dispatch table.
  |   ld4u DISPATCH, DISPATCH
  |     //mtc1 TMP3, TOBIT
  |  addi TMP1, zero, LJ_TFALSE
  |    li_vmstate INTERP
  |  addi PC, BASE, FRAME_PC
  |  ld4s PC, PC
  |     //cvt.d.s TOBIT, TOBIT
  |  addli RA, BASE, -8			// Results start at BASE-8.
  |   addli DISPATCH, DISPATCH, GG_G2DISP
  |  addi AT, RA, HI
  |  st4 AT, TMP1
  |    st_vmstate
  |  addi RD, zero, 16				// 2 results: false + error message.
  |  b ->vm_returnc
  |
  |//-----------------------------------------------------------------------
  |//-- Grow stack for calls -----------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |->vm_growstack_c:			// Grow stack for C function.
  |  addi CARG2, zero, LUA_MINSTACK
  |  b >2
  |
  |->vm_growstack_l:			// Grow stack for Lua function.
  |  // BASE = new base, RA = BASE+framesize*8, RC = nargs*8, PC = first PC
  |  add RC, BASE, RC
  |   sub RA, RA, BASE
  |  addlo AT, L:LREG->base
  |  st AT, BASE
  |   addli PC, PC, 4			// Must point after first instruction.
  |  addlo AT, L:LREG->top
  |  st AT, RC
  |   shrui CARG2, RA, 3
  |2:
  |  // L->base = new base, L->top = top
  |  load_got lj_state_growstack
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move CARG1, L
  |  call_intern lj_state_growstack	// (lua_State *L, int n)
  |  addlo BASE, L:LREG->base
  |  ld BASE, BASE
  |  addlo RC, L:LREG->top
  |  ld RC, RC
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |  sub RC, RC, BASE
  |  // BASE = new base, RB = LFUNC/CFUNC, RC = nargs*8, FRAME_PC(BASE) = PC
  |  ins_callt				// Just retry the call.
  |
  |//-----------------------------------------------------------------------
  |//-- Entry points into the assembler VM ---------------------------------
  |//-----------------------------------------------------------------------
  |
  |->vm_resume:				// Setup C frame and resume thread.
  |  // (lua_State *L, TValue *base, int nres1 = 0, ptrdiff_t ef = 0)
  |  saveregs
  |  move L, CARG1
  |    addlo AT, L:LREG->glref		// Setup pointer to dispatch table.
  |    ld4u DISPATCH, AT
  |  move BASE, CARG2
  |    addlo AT, L:LREG->status
  |    ld1u TMP1, AT
  |   addi AT, sp, SAVE_L
  |   st AT, L
  |  addi PC, zero, FRAME_CP
  |  addli TMP0, sp, CFRAME_RESUME
  |    addli DISPATCH, DISPATCH, GG_G2DISP
  |   addli AT, sp, SAVE_NRES
  |   st AT, zero
  |   addli AT, sp, SAVE_ERRF
  |   st AT, zero
  |  addlo AT, L:LREG->cframe
  |  st AT, TMP0
  |   addi AT, sp, SAVE_CFRAME
  |   st AT, zero
  |   addi AT, sp, SAVE_PC
  |  st AT, CARG1
  |    beqz TMP1, >3
  |
  |  // Resume after yield (like a return).
  |  move RA, BASE
  |   addlo BASE, L:LREG->base
  |   ld BASE, BASE
  |   addlo TMP1, L:LREG->top
  |   ld TMP1, TMP1
  |  addi PC, BASE, FRAME_PC
  |  ld4s PC, PC
  |     //lui TMP3, 0x59c0		// TOBIT = 2^52 + 2^51 (float).
  |   sub RD, TMP1, BASE
  |     //mtc1 TMP3, TOBIT
  |    addlo AT, L:LREG->status
  |    st1 AT, r0
  |     //cvt.d.s TOBIT, TOBIT
  |    li_vmstate INTERP
  |   addli RD, RD, 8
  |    st_vmstate
  |   move MULTRES, RD
  |  andi TMP0, PC, FRAME_TYPE
  |    addi TISNIL, zero, LJ_TNIL
  |  beqz TMP0, ->BC_RET_Z
  |  b ->vm_return
  |
  |->vm_pcall:				// Setup protected C frame and enter VM.
  |  // (lua_State *L, TValue *base, int nres1, ptrdiff_t ef)
  |  saveregs
  |  addli AT, sp, SAVE_ERRF
  |  st AT, CARG4
  |  addi PC, zero, FRAME_CP
  |  b >1
  |
  |->vm_call:				// Setup C frame and enter VM.
  |  // (lua_State *L, TValue *base, int nres1)
  |  saveregs
  |  addi PC, zero, FRAME_C
  |
  |1:  // Entry point for vm_pcall above (PC = ftype).
  |  addlo TMP1, L:CARG1->cframe
  |  ld TMP1, TMP1
  |   addli AT, sp, SAVE_NRES
  |   st AT, CARG3
  |    move L, CARG1
  |   addi AT, sp, SAVE_L
  |   st AT, CARG1
  |    move BASE, CARG2
  |  addlo AT, L:LREG->cframe			// Add our C frame to cframe chain.
  |  st AT, sp
  |    addlo DISPATCH, L:LREG->glref		// Setup pointer to dispatch table.
  |    ld4u DISPATCH, DISPATCH
  |   addi AT, sp, SAVE_PC
  |   st AT, CARG1
  |  addi AT, sp, SAVE_CFRAME
  |  st AT, TMP1
  |    addli DISPATCH, DISPATCH, GG_G2DISP
  |
  |3:  // Entry point for vm_cpcall/vm_resume (BASE = base, PC = ftype).
  |  addlo AT, L:LREG->base		
  |  ld TMP2, AT			// TMP2 = old base (used in vmeta_call).
  |  addlo AT, L:LREG->top		
  |   ld TMP1, AT
  |  add PC, PC, BASE
  |   sub NARGS8:RC, TMP1, BASE
  |  sub PC, PC, TMP2			// PC = frame delta + frame type
  |    li_vmstate INTERP
  |     moveli TISNIL, LJ_TNIL
  |    st_vmstate
  |
  |->vm_call_dispatch:
  |  // TMP2 = old base, BASE = new base, RC = nargs*8, PC = caller PC
  |  addi TMP0, BASE, FRAME_PC
  |  ld4s TMP0, TMP0
  |  moveli AT, LJ_TFUNC
  |  cmpne AT1, TMP0, AT
  |  addi TMP0, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, TMP0
  |  bnez AT1, ->vmeta_call
  |
  |->vm_call_dispatch_f:
  |  ins_call
  |  // BASE = new base, RB = func, RC = nargs*8, PC = caller PC
  |
  |->vm_cpcall:				// Setup protected C frame, call C.
  |  // (lua_State *L, lua_CFunction func, void *ud, lua_CPFunction cp)
  |  saveregs
  |  move L, CARG1
  |   addlo TMP0, L:CARG1->stack
  |   ld4u TMP0, TMP0
  |  addi AT, sp, SAVE_L
  |  st AT, CARG1
  |   addlo TMP1, L:CARG1->top
  |   ld TMP1, TMP1
  |  addi AT, sp, SAVE_PC
  |  st AT, CARG1			// Any value outside of bytecode is ok.
  |   sub TMP0, TMP0, TMP1		// Compute -savestack(L, L->top).
  |   addlo AT, L:CARG1->cframe
  |    ld TMP1, AT
  |    st AT, sp			// Add our C frame to cframe chain.
  |   addli AT, sp, SAVE_NRES		// Neg. delta means cframe w/o frame.
  |    st AT, TMP0
  |  addli AT, sp, SAVE_ERRF		// No error function.
  |   st AT, r0
  |  addi AT, sp, SAVE_CFRAME
  |  st AT, TMP1
  |  jalr CARG4			// (lua_State *L, lua_CFunction func, void *ud)
  |  move BASE, CRET1
  |   addlo DISPATCH, L:LREG->glref	// Setup pointer to dispatch table.
  |   ld4u DISPATCH, DISPATCH
  |    addi PC, zero, FRAME_CP
  |  addli DISPATCH, DISPATCH, GG_G2DISP
  |  bnez CRET1, <3			// Else continue with the call.
  |  b ->vm_leave_cp			// No base? Just remove C frame.
  |
  |//-----------------------------------------------------------------------
  |//-- Metamethod handling ------------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |// The lj_meta_* functions (except for lj_meta_cat) don't reallocate the
  |// stack, so BASE doesn't need to be reloaded across these calls.
  |
  |//-- Continuation dispatch ----------------------------------------------
  |
  |->cont_dispatch:
  |  // BASE = meta base, RA = resultptr, RD = (nresults+1)*8
  |  addi TMP0, BASE, -16+LO
  |  ld4s TMP0, TMP0
  |   move RB, BASE
  |   move BASE, TMP2			// Restore caller BASE.
  |    addi LFUNC:TMP1, TMP2, FRAME_FUNC
  |    ld4s LFUNC:TMP1, LFUNC:TMP1
  |.if FFI
  |  cmpltui AT, TMP0, 2
  |.endif
  |     addi PC, RB, -16+HI
  |     ld4s PC, PC
  |   add TMP2, RA, RD
  |    addlo TMP1, LFUNC:TMP1->pc
  |    ld4u TMP1, TMP1
  |  addi AT, TMP2, -8+HI
  |  st4 AT, TISNIL
  |.if FFI
  |  bnez AT, >1
  |.endif
  |  // BASE = base, RA = resultptr, RB = meta base
  |  addi AT, TMP1, PC2PROTO(k)
  |  ld4u KBASE, AT
  |  jr TMP0				// Jump to continuation.
  |
  |.if FFI
  |1:
  |  // cont = 0: tailcall from C function.
  |  addli TMP1, RB, -16
  |  bnez TMP0, ->cont_ffi_callback	// cont = 1: return from FFI callback.
  |  sub RC, TMP1, BASE
  |  b ->vm_call_tail
  |.endif
  |
  |->cont_cat:				// RA = resultptr, RB = meta base
  |  addi INS, PC, -4
  |  ld4u INS, INS
  |   addli CARG2, RB, -16
  |   ld AT, RA
  |  decode_RB8a MULTRES, INS
  |   decode_RA8a RA, INS
  |  decode_RB8b MULTRES
  |   decode_RA8b RA
  |  add TMP1, BASE, MULTRES
  |   addlo AT1, L:LREG->base
  |   st AT1, BASE
  |   sub CARG3, CARG2, TMP1
  |  st CARG2, AT
  |  cmpne AT1, TMP1, CARG2
  |  bnez AT1, ->BC_CAT_Z
  |  add RA, BASE, RA
  |  st RA, AT
  |  b ->cont_nop
  |
  |//-- Table indexing metamethods -----------------------------------------
  |
  |->vmeta_tgets1:
  |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
  |  addi TMP0, zero, LJ_TSTR
  |  addi AT, CARG3, LO
  |  st4 AT, STR:RC
  |  addi AT, CARG3, HI
  |  st4 AT, TMP0
  |  b >1
  |
  |->vmeta_tgets:
  |  addli CARG2, DISPATCH, DISPATCH_GL(tmptv)
  |  addi TMP0, zero, LJ_TTAB
  |  addi AT, CARG2, LO
  |  st4 AT, TAB:RB
  |   addli CARG3, DISPATCH, DISPATCH_GL(tmptv2)
  |  addi AT, CARG2, HI
  |  st4 AT, TMP0
  |   addi TMP1, zero, LJ_TSTR
  |  addi AT, CARG3, LO
  |   st4 AT, STR:RC
  |  addi AT, CARG3, HI
  |  st4 AT, TMP1
  |  b >1
  |
  |->vmeta_tgetb:			// TMP0 = index
  |  //mtc1 TMP0, f0
  |  //cvt.d.w f0, f0
  |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
  |  //sdc1 f0, 0(CARG3)
  |
  |->vmeta_tgetv:
  |1:
  |  load_got lj_meta_tget
  |  addlo AT, L:LREG->base
  |  st AT, BASE
  |  st sp, PC
  |  move CARG1, L
  |  call_intern lj_meta_tget		// (lua_State *L, TValue *o, TValue *k)
  |  // Returns TValue * (finished) or NULL (metamethod).
  |  addli TMP1, BASE, -FRAME_CONT
  |  beqz CRET1, >3
  |  ld AT, CRET1
  |  ins_next1
  |   st RA, AT
  |  ins_next2
  |
  |3:  // Call __index metamethod.
  |  // BASE = base, L->top = new base, stack = cont/func/t/k
  |  addlo BASE, L:LREG->top
  |  ld BASE, BASE
  |  addi AT, BASE, -16 + HI
  |  st4 AT, PC
  |   sub PC, BASE, TMP1
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |  addi NARGS8:RC, zero, 16			// 2 args for func(t, k).
  |  b ->vm_call_dispatch_f
  |
  |//-----------------------------------------------------------------------
  |
  |->vmeta_tsets1:
  |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
  |  addi TMP0, zero, LJ_TSTR
  |  addi AT, CARG3, LO
  |  st4 AT, STR:RC
  |  addi AT, CARG3, HI
  |  st4 AT, TMP0
  |  b >1
  |
  |->vmeta_tsets:
  |  addli CARG2, DISPATCH, DISPATCH_GL(tmptv)
  |  addi TMP0, zero, LJ_TTAB
  |  addi AT, CARG2, LO
  |  st4 AT, TAB:RB
  |   addli CARG3, DISPATCH, DISPATCH_GL(tmptv2)
  |  addi AT, CARG2, HI
  |  st4 AT, TMP0
  |   addi TMP1, zero, LJ_TSTR
  |  addi AT, CARG3, LO
  |   st4 AT, STR:RC
  |  addi AT, CARG3, HI
  |  st4 AT, TMP1
  |  b >1
  |
  |->vmeta_tsetb:			// TMP0 = index
  |  //mtc1 TMP0, f0
  |  //cvt.d.w f0, f0
  |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
  |  //sdc1 f0, 0(CARG3)
  |
  |->vmeta_tsetv:
  |1:
  |  load_got lj_meta_tset
  |  addlo AT, L:LREG->base
  |  st AT, BASE
  |  addi AT, sp, SAVE_PC
  |  st AT, PC
  |  move CARG1, L
  |  call_intern lj_meta_tset		// (lua_State *L, TValue *o, TValue *k)
  |  // Returns TValue * (finished) or NULL (metamethod).
  |  ld AT, RA
  |  beqz CRET1, >3
  |  // NOBARRIER: lj_meta_tset ensures the table is not black.
  |  ins_next1
  |   st CRET1, AT
  |  ins_next2
  |
  |3:  // Call __newindex metamethod.
  |  // BASE = base, L->top = new base, stack = cont/func/t/k/(v)
  |  addli TMP1, BASE, -FRAME_CONT
  |  addlo BASE, L:LREG->top
  |  ld BASE, BASE
  |  addi AT, BASE, -16+HI
  |  st4 AT, PC
  |   sub PC, BASE, TMP1
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |  //sdc1 f0, 16(BASE)			// Copy value to third argument.
  |  addi NARGS8:RC, zero, 24			// 3 args for func(t, k, v)
  |  b ->vm_call_dispatch_f
  |
  |//-- Comparison metamethods ---------------------------------------------
  |
  |->vmeta_comp:
  |  // CARG2, CARG3 are already set by BC_ISLT/BC_ISGE/BC_ISLE/BC_ISGT.
  |  load_got lj_meta_comp
  |  addli PC, PC, -4
  |  addlo AT, L:LREG->base
  |  st AT, BASE
  |  addi AT, sp, SAVE_PC
  |  st AT, PC
  |  decode_OP1 CARG4, INS
  |  move CARG1, L
  |  call_intern lj_meta_comp	// (lua_State *L, TValue *o1, *o2, int op)
  |  // Returns 0/1 or TValue * (metamethod).
  |3:
  |  cmpltui AT, CRET1, 2
  |  beqz AT, ->vmeta_binop
  |   //negu TMP2, CRET1
  |4:
  |  addi RD, PC, OFS_RD
  |  ld2u RD, RD
  |   addli PC, PC, 4
  |   //lui TMP1, (-(BCBIAS_J*4 >> 16) & 65535)
  |  shli RD, RD, 2
  |  add RD, RD, TMP1
  |  and RD, RD, TMP2
  |  add PC, PC, RD
  |->cont_nop:
  |  ins_next
  |
  |->cont_ra:				// RA = resultptr
  |  addi TMP1, PC, -4+OFS_RA
  |  ld1u TMP1, TMP1
  |   ld AT, RA
  |  shli TMP1, TMP1, 3
  |  add TMP1, BASE, TMP1
  |  st TMP1, AT
  |  b ->cont_nop
  |
  |->cont_condt:			// RA = resultptr
  |  addi TMP0, RA, HI
  |  ld4s TMP0, TMP0
  |  cmpltui AT, TMP0, LJ_TISTRUECOND
  |  //negu TMP2, AT			// Branch if result is true.
  |  b <4
  |
  |->cont_condf:			// RA = resultptr
  |  addi TMP0, RA, HI
  |  ld4s TMP0, TMP0
  |  cmpltui AT, TMP0, LJ_TISTRUECOND
  |  addli TMP2, AT, -1		// Branch if result is false.
  |  b <4
  |
  |->vmeta_equal:
  |  // CARG2, CARG3, CARG4 are already set by BC_ISEQV/BC_ISNEV.
  |  load_got lj_meta_equal
  |  addli PC, PC, -4
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move CARG1, L
  |  call_intern lj_meta_equal  // (lua_State *L, GCobj *o1, *o2, int ne)
  |  // Returns 0/1 or TValue * (metamethod).
  |  b <3
  |
  |->vmeta_equal_cd:
  |.if FFI
  |  load_got lj_meta_equal_cd
  |  move CARG2, INS
  |  addli PC, PC, -4
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move CARG1, L
  |  call_intern lj_meta_equal_cd	// (lua_State *L, BCIns op)
  |  // Returns 0/1 or TValue * (metamethod).
  |  b <3
  |.endif
  |
  |//-- Arithmetic metamethods ---------------------------------------------
  |
  |->vmeta_unm:
  |  move CARG4, CARG3
  |
  |->vmeta_arith:
  |  load_got lj_meta_arith
  |  decode_OP1 TMP0, INS
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move CARG2, RA
  |  move ARG5, TMP0
  |  move CARG1, L
  |  call_intern lj_meta_arith  // (lua_State *L, TValue *ra,*rb,*rc, BCReg op)
  |  // Returns NULL (finished) or TValue * (metamethod).
  |  beqz CRET1, ->cont_nop
  |
  |  // Call metamethod for binary op.
  |->vmeta_binop:
  |  // BASE = old base, CRET1 = new base, stack = cont/func/o1/o2
  |  sub TMP1, CRET1, BASE
  |   addi AT, CRET1, -16+HI
  |   st4 AT, PC
  |   move TMP2, BASE
  |  addli PC, TMP1, FRAME_CONT
  |   move BASE, CRET1
  |  addi NARGS8:RC, zero, 16			// 2 args for func(o1, o2).
  |  b ->vm_call_dispatch
  |
  |->vmeta_len:
  |  // CARG2 already set by BC_LEN.
#if LJ_52
  |  move MULTRES, CARG1
#endif
  |  load_got lj_meta_len
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move CARG1, L
  |  call_intern lj_meta_len		// (lua_State *L, TValue *o)
  |  // Returns NULL (retry) or TValue * (metamethod base).
#if LJ_52
  |  bnez CRET1, ->vmeta_binop		// Binop call for compatibility.
  |  move CARG1, MULTRES
  |  b ->BC_LEN_Z
#else
  |  b ->vmeta_binop			// Binop call for compatibility.
#endif
  |
  |//-- Call metamethod ----------------------------------------------------
  |
  |->vmeta_call:			// Resolve and call __call metamethod.
  |  // TMP2 = old base, BASE = new base, RC = nargs*8
  |  load_got lj_meta_call
  |   addlo AT, L:LREG->base			// This is the callers base!
  |   st AT, TMP2
  |  addli CARG2, BASE, -8
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  add CARG3, BASE, RC
  |   move MULTRES, NARGS8:RC
  |  move CARG1, L
  |  call_intern lj_meta_call	// (lua_State *L, TValue *func, TValue *top)
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |   addli NARGS8:RC, MULTRES, 8	// Got one more argument now.
  |  ins_call
  |
  |->vmeta_callt:			// Resolve __call for BC_CALLT.
  |  // BASE = old base, RA = new base, RC = nargs*8
  |  load_got lj_meta_call
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |  addli CARG2, RA, -8
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  add CARG3, RA, RC
  |   move MULTRES, NARGS8:RC
  |  move CARG1, L
  |  call_intern lj_meta_call	// (lua_State *L, TValue *func, TValue *top)
  |  addi TMP1, BASE, FRAME_PC
  |  ld4s TMP1, TMP1
  |   addi LFUNC:RB, RA, FRAME_FUNC
  |   ld4s LFUNC:RB, LFUNC:RB
  |  addli NARGS8:RC, MULTRES, 8	// Got one more argument now.
  |  b ->BC_CALLT_Z
  |
  |//-- Argument coercion for 'for' statement ------------------------------
  |
  |->vmeta_for:
  |  load_got lj_meta_for
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |  move CARG2, RA
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  move MULTRES, INS
  |  move CARG1, L
  |  call_intern lj_meta_for	// (lua_State *L, TValue *base)
  |.if JIT
  |  decode_OP1 TMP0, MULTRES
  |  addi AT, zero, BC_JFORI
  |.endif
  |  decode_RA8a RA, MULTRES
  |   decode_RD8a RD, MULTRES
  |  decode_RA8b RA
  |.if JIT
  |  decode_RD8b RD
  |  cmpeq AT1, TMP0, AT
  |  bnez AT1, =>BC_JFORI
  |  b =>BC_FORI
  |.else
  |  decode_RD8b RD
  |  b =>BC_FORI
  |.endif
  |
  |//-----------------------------------------------------------------------
  |//-- Fast functions -----------------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |.macro .ffunc, name
  |->ff_ .. name:
  |.endmacro
  |
  |.macro .ffunc_1, name
  |->ff_ .. name:
  |  addi AT, BASE, HI
  |  ld4s CARG3, AT
  |  addi AT, BASE, LO
  |  ld4s CARG1, AT
  |  beqz NARGS8:RC, ->fff_fallback
  |.endmacro
  |
  |.macro .ffunc_2, name
  |->ff_ .. name:
  |  cmpltui AT, NARGS8:RC, 16
  |   addi CARG3, BASE, HI
  |   ld4s CARG3, CARG3
  |  addi AT, BASE, 8+HI
  |  ld4s CARG4, AT
  |  addi AT, BASE, LO
  |  ld4s CARG1, AT
  |  bnez AT, ->fff_fallback
  |  addi CARG2, BASE, 8+LO
  |  ld4s CARG2, CARG2
  |.endmacro
  |
  |.macro .ffunc_n, name	// Caveat: has delay slot!
  |->ff_ .. name:
  |  addi CARG3, BASE, HI
  |  ld4s CARG3, CARG3
  |  //ldc1 FARG1, 0(BASE)
  |  beqz NARGS8:RC, ->fff_fallback
  |  cmpltui AT, CARG3, LJ_TISNUM
  |  beqz AT, ->fff_fallback
  |.endmacro
  |
  |.macro .ffunc_nn, name	// Caveat: has delay slot!
  |->ff_ .. name:
  |  cmpltui AT, NARGS8:RC, 16
  |   addi CARG3, BASE, HI
  |   ld4s CARG3, CARG3
  |  addi AT, BASE, 8+HI
  |  ld4s CARG4, AT
  |  bnez AT, ->fff_fallback
  |  //ldc1 FARG1, 0(BASE)
  |  //ldc1 FARG2, 8(BASE)
  |  cmpltui TMP0, CARG3, LJ_TISNUM
  |  cmpltui TMP1, CARG4, LJ_TISNUM
  |  and TMP0, TMP0, TMP1
  |  beqz TMP0, ->fff_fallback
  |.endmacro
  |
  |// Inlined GC threshold check. Caveat: uses TMP0 and TMP1 and has delay slot!
  |.macro ffgccheck
  |  addi TMP0, DISPATCH, DISPATCH_GL(gc.total)
  |  ld TMP0, TMP0
  |  addi TMP1, DISPATCH, DISPATCH_GL(gc.threshold)
  |  ld TMP1, TMP1
  |  sub AT, TMP0, TMP1
  |  //FIXME
  |  //bgezal AT, ->fff_gcstep
  |.endmacro
  |
  |//-- Base library: checks -----------------------------------------------
  |
  |.ffunc_1 assert
  |  cmpltui AT, CARG3, LJ_TISTRUECOND
  |  addli RA, BASE, -8
  |  beqz AT, ->fff_fallback
  |  addi PC, BASE, FRAME_PC
  |  ld4s PC, PC
  |  addli RD, NARGS8:RC, 8		// Compute (nresults+1)*8.
  |  add TMP2, RA, NARGS8:RC
  |   addi AT, RA, HI
  |   st4 AT, CARG3
  |  addli TMP1, BASE, 8
  |  addi AT, RA, LO
  |  st4 AT, CARG1
  |  cmpeq AT, BASE, TMP2
  |  bnez AT, ->fff_res		// Done if exactly 1 argument.
  |1:
  |  ld AT, TMP1
  |  addi AT1, TMP1, -8
  |  st AT1, AT
  |  addli TMP1, TMP1, 8
  |  cmpne AT, TMP1, TMP2
  |  bnez AT, <1
  |  b ->fff_res
  |
  |.ffunc type
  |  addi CARG3, BASE, HI
  |  ld4s CARG3, CARG3
  |  addi TMP1, zero, LJ_TISNUM
  |  cmpltui TMP0, CARG3, LJ_TISNUM
  |  beqz NARGS8:RC, ->fff_fallback
  |  cmoveqz TMP1, TMP0, CARG3
  |  nor TMP1, TMP1, zero
  |  shli TMP1, TMP1, 3
  |  add TMP1, CFUNC:RB, TMP1
  |  addlo AT2, CFUNC:TMP1->upvalue
  |  ld AT2, AT2
  |  b ->fff_resn
  |
  |//-- Base library: getters and setters ---------------------------------
  |
  |.ffunc_1 getmetatable
  |  addi AT, zero, LJ_TTAB
  |  addi AT, zero, LJ_TUDATA
  |  cmpne AT1, CARG3, AT
  |  bnez AT1, >6
  |1:  // Field metatable must be at same offset for GCtab and GCudata!
  |  addlo TAB:CARG1, TAB:CARG1->metatable
  |  ld TAB:CARG1, TAB:CARG1
  |2:
  |  addi STR:RC, DISPATCH, DISPATCH_GL(gcroot[GCROOT_MMNAME+MM_metatable])
  |  ld STR:RC, STR:RC
  |  addi CARG3, zero, LJ_TNIL
  |  beqz TAB:CARG1, ->fff_restv
  |  addlo TMP0, TAB:CARG1->hmask
  |  ld TMP0, TMP0
  |   addi CARG3, zero, LJ_TTAB			// Use metatable as default result.
  |  addlo TMP1, STR:RC->hash
  |  ld TMP1, TMP1
  |  addlo NODE:TMP2, TAB:CARG1->node
  |  ld NODE:TMP2, NODE:TMP2
  |  and TMP1, TMP1, TMP0		// idx = str->hash & tab->hmask
  |  shli TMP0, TMP1, 5
  |  shli TMP1, TMP1, 3
  |  sub TMP1, TMP0, TMP1
  |  add NODE:TMP2, NODE:TMP2, TMP1	// node = tab->node + (idx*32-idx*8)
