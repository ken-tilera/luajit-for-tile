  |  addi CARG3, zero, LJ_TNIL
  |  beqz CRET1, ->fff_restv		// End of traversal: return nil.
  |  ldc1 f0, 8(BASE)			// Copy key and value to results.
  |    addli RA, BASE, -8
  |   ldc1 f2, 16(BASE)
  |    li RD, (2+1)*8
  |  sdc1 f0, 0(RA)
  |  sdc1 f2, 8(RA)
