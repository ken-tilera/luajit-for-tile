  |->fff_restv:
  |  // CARG3/CARG1 = TValue result.
  |  ld PC, FRAME_PC(BASE)
  |   st CARG3, -8+HI(BASE)
  |  addli RA, BASE, -8
  |   st CARG1, -8+LO(BASE)
  |->fff_res1:
