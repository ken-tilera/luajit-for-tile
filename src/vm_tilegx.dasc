  |//-- Coroutine library --------------------------------------------------
  |
  |.macro coroutine_resume_wrap, resume
  |.if resume
  |.ffunc_1 coroutine_resume
  |  li AT, LJ_TTHREAD
  |  cmpne CARG3, AT, ->fff_fallback
  |.else
  |.ffunc coroutine_wrap_aux
  |  ld L:CARG1, CFUNC:RB->upvalue[0].gcr
  |.endif
  |  lbu TMP0, L:CARG1->status
  |   ld TMP1, L:CARG1->cframe
  |    ld CARG2, L:CARG1->top
  |    ld TMP2, L:CARG1->base
  |  addli TMP3, TMP0, -LUA_YIELD
  |   xor TMP2, TMP2, CARG2
  |  bgtz TMP3, ->fff_fallback		// st > LUA_YIELD?
  |  or AT, TMP2, TMP0
  |  bnez TMP1, ->fff_fallback		// cframe != 0?
  |  ld TMP0, L:CARG1->maxstack
  |  addi AT1, BASE, FRAME_PC
  |  ld PC, AT1
  |  beqz AT, ->fff_fallback		// base == top && st == 0?
  |  add TMP2, CARG2, NARGS8:RC
  |  cmpltu AT, TMP0, TMP2
  |  st sp, PC
  |  bnez AT, ->fff_fallback		// Stack overflow?
  |   st BASE, L->base
  |1:
  |.if resume
  |  addli BASE, BASE, 8		// Keep resumed thread in stack for GC.
  |  addli NARGS8:RC, NARGS8:RC, -8
  |  addli TMP2, TMP2, -8
  |.endif
  |  st TMP2, L:CARG1->top
  |  add TMP1, BASE, NARGS8:RC
  |  move CARG3, CARG2
  |  st BASE, L->top
  |2:  // Move args to coroutine.
  |   ldc1 f0, 0(BASE)
  |  cmpltu AT, BASE, TMP1
  |  addli BASE, BASE, 8
  |  beqz AT, >3
  |   sdc1 f0, 0(CARG3)
  |  addli CARG3, CARG3, 8
  |  b <2
  |3:
  |  move L:RA, L:CARG1
  |  bal ->vm_resume			// (lua_State *L, TValue *base, 0, 0)
  |  // Returns thread status.
  |4:
  |  ld TMP2, L:RA->base
  |   cmpltui AT, CRET1, LUA_YIELD+1
  |  ld TMP3, L:RA->top
  |    li_vmstate INTERP
  |  ld BASE, L->base
  |    st_vmstate
  | sub RD, TMP3, TMP2
  |   beqz AT, >8
  |   ld TMP0, L->maxstack
  |  add TMP1, BASE, RD
  |  beqz RD, >6			// No results?
  |  cmpltu AT, TMP0, TMP1
  |  add TMP3, TMP2, RD
  |  bnez AT, >9			// Need to grow stack?
  |  st TMP2, L:RA->top			// Clear coroutine stack.
  |  move TMP1, BASE
  |5:  // Move results from coroutine.
  |   ldc1 f0, 0(TMP2)
  |  addli TMP2, TMP2, 8
  |  cmpltu AT, TMP2, TMP3
  |   sdc1 f0, 0(TMP1)
  |  addli TMP1, TMP1, 8
  |  bnez AT, <5
  |6:
  |  andi TMP0, PC, FRAME_TYPE
  |.if resume
  |  li TMP1, LJ_TTRUE
  |   addli RA, BASE, -8
  |  st TMP1, -8+HI(BASE)		// Prepend true to results.
  |  addli RD, RD, 16
  |.else
  |  move RA, BASE
  |  addli RD, RD, 8
  |.endif
  |7:
  |  st sp, PC
  |  move MULTRES, RD
  |  beqz TMP0, ->BC_RET_Z
  |  b ->vm_return
  |
  |8:  // Coroutine returned with error (at co->top-1).
  |.if resume
  |  addli TMP3, TMP3, -8
  |   li TMP1, LJ_TFALSE
  |  ldc1 f0, 0(TMP3)
  |   st TMP3, L:RA->top		// Remove error from coroutine stack.
  |    li RD, (2+1)*8
  |   st TMP1, -8+HI(BASE)		// Prepend false to results.
  |    addli RA, BASE, -8
  |  sdc1 f0, 0(BASE)			// Copy error message.
  |  andi TMP0, PC, FRAME_TYPE
  |  b <7
  |.else
  |  load_got lj_ffh_coroutine_wrap_err
  |  move CARG2, L:RA
  |  move CARG1, L
  |  call_intern lj_ffh_coroutine_wrap_err  // (lua_State *L, lua_State *co)
  |.endif
  |
  |9:  // Handle stack expansion on return from yield.
  |  load_got lj_state_growstack
  |  srl CARG2, RD, 3
  |  move CARG1, L
  |  call_intern lj_state_growstack	// (lua_State *L, int n)
  |  move CRET1, zero
  |  b <4
  |.endmacro
  |
  |  coroutine_resume_wrap 1		// coroutine.resume
  |  coroutine_resume_wrap 0		// coroutine.wrap
  |
  |.ffunc coroutine_yield
  |  ld TMP0, L->cframe
  |   add TMP1, BASE, NARGS8:RC
  |   st BASE, L->base
  |  andi TMP0, TMP0, CFRAME_RESUME
  |   st TMP1, L->top
  |   addi CRET1, zero, LUA_YIELD
  |  beqz TMP0, ->fff_fallback
  |  st r0, L->cframe
  |  addlo AT, L:LREG->status
  |  st1 AT, CRET1
  |  b ->vm_leave_unw
