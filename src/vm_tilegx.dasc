  |    slt AT, AT, r0
  |.if "func" == "ceil"
  |   //lui TMP0, 0xbff0			// Hiword of -1 (double). Preserves -0.
  |.else
  |   //lui TMP0, 0x3ff0			// Hiword of +1 (double).
  |.endif
  |.if "func" == "trunc"
  |   mtc1 TMP0, f5
  |  c.olt.d 0, FRET2, FRET1		// |x| < result?
  |   sub.d FRET2, FRET1, f4
  |  movt.d FRET1, FRET2, 0		// If yes, subtract +1.
  |  neg.d FRET2, FRET1
  |  jr ra
  |.  movn.d FRET1, FRET2, AT		// Merge sign bit back in.
  |.else
  |  neg.d FRET2, FRET1
  |   mtc1 TMP0, f5
  |  movn.d FRET1, FRET2, AT		// Merge sign bit back in.
  |.if "func" == "ceil"
  |  c.olt.d 0, FRET1, FARG1		// x > result?
  |.else
  |  c.olt.d 0, FARG1, FRET1		// x < result?
  |.endif
  |   sub.d FRET2, FRET1, f4		// If yes, subtract +-1.
  |  jr ra
  |.  movt.d FRET1, FRET2, 0
  |.endif
  |1:
  |  jr ra
  |.  mov.d FRET1, FARG1
  |.endmacro
  |
  |->vm_floor:
  |  vm_round floor
  |->vm_ceil:
  |  vm_round ceil
  |->vm_trunc:
  |.if JIT
  |  vm_round trunc
  |.endif
  |
  |//-----------------------------------------------------------------------
  |//-- Miscellaneous functions --------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |//-----------------------------------------------------------------------
  |//-- FFI helper functions -----------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |// Handler for callback functions. Callback slot number in r1, g in r2.
  |->vm_ffi_callback:
  |.if FFI
  |.type CTSTATE, CTState, PC
  |  saveregs
  |  ld CTSTATE, GL:r2->ctype_state
  |   addli DISPATCH, r2, GG_G2DISP
  |  load_got lj_ccallback_enter
  |  st r1, CTSTATE->cb.slot
  |  st CARG1, CTSTATE->cb.gpr[0]
  |  st CARG2, CTSTATE->cb.gpr[1]
  |   sdc1 FARG1, CTSTATE->cb.fpr[0]
  |  st CARG3, CTSTATE->cb.gpr[2]
  |  st CARG4, CTSTATE->cb.gpr[3]
  |   sdc1 FARG2, CTSTATE->cb.fpr[1]
  |  addli TMP0, sp, CFRAME_SPACE+16
  |  st TMP0, CTSTATE->cb.stack
  |  st sp, zero
  |   move CARG2, sp
  |  move CARG1, CTSTATE
  |  call_intern lj_ccallback_enter	// (CTState *cts, void *cf)
  |  // Returns lua_State *.
  |  ld BASE, L:CRET1->base
  |  ld RC, L:CRET1->top
  |   move L, CRET1
  |     //lui TMP3, 0x59c0		// TOBIT = 2^52 + 2^51 (float).
  |  ld LFUNC:RB, FRAME_FUNC(BASE)
  |     mtc1 TMP3, TOBIT
  |    li_vmstate INTERP
  |     li TISNIL, LJ_TNIL
  |  sub RC, RC, BASE
  |    st_vmstate
  |     cvt.d.s TOBIT, TOBIT
  |  ins_callt
  |.endif
  |
  |->cont_ffi_callback:			// Return from FFI callback.
  |.if FFI
  |  load_got lj_ccallback_leave
  |  ld CTSTATE, DISPATCH_GL(ctype_state)(DISPATCH)
  |   st BASE, L->base
  |   st RB, L->top
  |  st L, CTSTATE->L
  |  move CARG2, RA
  |  move CARG1, CTSTATE
  |  call_intern lj_ccallback_leave	// (CTState *cts, TValue *o)
  |  ld CRET1, CTSTATE->cb.gpr[0]
  |   ldc1 FRET1, CTSTATE->cb.fpr[0]
  |  ld CRET2, CTSTATE->cb.gpr[1]
  |  ldc1 FRET2, CTSTATE->cb.fpr[1]
  |  b ->vm_leave_unw
  |.endif
  |
  |->vm_ffi_call:			// Call C function via FFI.
  |  // Caveat: needs special frame unwinding, see below.
  |.if FFI
  |  .type CCSTATE, CCallState, CARG1
  |  ld TMP1, CCSTATE->spadj
  |   lbu CARG2, CCSTATE->nsp
  |  move TMP2, sp
  |  sub sp, sp, TMP1
  |  st ra, -4(TMP2)
  |   shli CARG2, CARG2, 2
  |  st r16, -8(TMP2)
  |  st CCSTATE, -12(TMP2)
  |  move r16, TMP2
  |  addli TMP1, CCSTATE, offsetof(CCallState, stack)
  |  addli TMP2, sp, 16
  |  add TMP3, TMP1, CARG2
  |  beqz CARG2, >2
  |1:
  |   ld TMP0, 0(TMP1)
  |  addli TMP1, TMP1, 4
  |  cmpltu AT, TMP1, TMP3
  |   st TMP0, 0(TMP2)
  |  addli TMP2, TMP2, 4
  |  bnez AT, <1
  |2:
  |  ld CFUNCADDR, CCSTATE->func
  |  ld CARG2, CCSTATE->gpr[1]
  |  ld CARG3, CCSTATE->gpr[2]
  |  ld CARG4, CCSTATE->gpr[3]
  |  ldc1 FARG1, CCSTATE->fpr[0]
  |  ldc1 FARG2, CCSTATE->fpr[1]
  |  addlo AT, CCSTATE:CARG1->gpr[0]		// Do this last, since CCSTATE is CARG1.
  |  ld CARG1, AT
  |  jalr CFUNCADDR
  |  ld CCSTATE:TMP1, -12(r16)
  |  ld TMP2, -8(r16)
  |  ld ra, -4(r16)
  |  st CRET1, CCSTATE:TMP1->gpr[0]
  |  st CRET2, CCSTATE:TMP1->gpr[1]
  |  sdc1 FRET1, CCSTATE:TMP1->fpr[0]
  |  sdc1 FRET2, CCSTATE:TMP1->fpr[1]
  |  move sp, r16
  |  move r16, TMP2
  |  jr ra
  |.endif
  |// Note: vm_ffi_call must be the last function in this object file!
  |
  |//-----------------------------------------------------------------------
}

/* Generate the code for a single instruction. */
static void build_ins(BuildCtx *ctx, BCOp op, int defop)
{
  int vk = 0;
  |=>defop:

  switch (op) {

  /* -- Comparison ops ---------------------------------------------------- */

  /* Remember: all ops branch for a true comparison, fall through otherwise. */

  case BC_ISLT: case BC_ISGE: case BC_ISLE: case BC_ISGT:
    |  // RA = src1*8, RD = src2*8, JMP with RD = target
    |  add CARG2, BASE, RA
    |   add CARG3, BASE, RD
    |  ld TMP0, HI(CARG2)
    |   ld TMP1, HI(CARG3)
    |   ldc1 f0, 0(CARG2)
    |   ldc1 f2, 0(CARG3)
    |  cmpltui TMP0, TMP0, LJ_TISNUM
    |   cmpltui TMP1, TMP1, LJ_TISNUM
    |    ld2u TMP2, OFS_RD(PC)
    |  and TMP0, TMP0, TMP1
    |    addli PC, PC, 4
    |   //lui TMP1, (-(BCBIAS_J*4 >> 16) & 65535)
    |  beqz TMP0, ->vmeta_comp
    |    decode_RD4b TMP2
    |    add TMP2, TMP2, TMP1
    if (op == BC_ISLT || op == BC_ISGE) {
      |  c.olt.d f0, f2
    } else {
      |  c.ole.d f0, f2
    }
    if (op == BC_ISLT || op == BC_ISLE) {
      |  movf TMP2, r0
    } else {
      |  movt TMP2, r0
    }
    |  add PC, PC, TMP2
    |1:
    |  ins_next
    break;

  case BC_ISEQV: case BC_ISNEV:
    vk = op == BC_ISEQV;
    |  // RA = src1*8, RD = src2*8, JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  ld TMP0, HI(RA)
    |   ldc1 f0, 0(RA)
    |  add RD, BASE, RD
    |    ld2u TMP2, -4+OFS_RD(PC)
    |   ld TMP1, HI(RD)
    |   ldc1 f2, 0(RD)
    |    //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |  cmpltui AT, TMP0, LJ_TISNUM
    |  cmpltui CARG1, TMP1, LJ_TISNUM
    |    decode_RD4b TMP2
    |  and AT, AT, CARG1
    |   add TMP2, TMP2, TMP3
    |  beqz AT, >5
    |  c.eq.d f0, f2
    if (vk) {
      |  movf TMP2, r0
    } else {
      |  movt TMP2, r0
    }
    |1:
    |  add PC, PC, TMP2
    |  ins_next
    |5:  // Either or both types are not numbers.
    |  ld CARG2, LO(RA)
    |  ld CARG3, LO(RD)
    |.if FFI
    |  li TMP3, LJ_TCDATA
    |  beq TMP0, TMP3, ->vmeta_equal_cd
    |.endif
    |.  cmpltui AT, TMP0, LJ_TISPRI		// Not a primitive?
    |.if FFI
    |  beq TMP1, TMP3, ->vmeta_equal_cd
    |.endif
    |.  xor TMP3, CARG2, CARG3			// Same tv?
    |  xor TMP1, TMP1, TMP0			// Same type?
    |  cmpltui CARG1, TMP0, LJ_TISTABUD+1		// Table or userdata?
    |  movz TMP3, r0, AT			// Ignore tv if primitive.
    |  movn CARG1, r0, TMP1			// Tab/ud and same type?
    |  or AT, TMP1, TMP3			// Same type && (pri||same tv).
    |  cmoveqz_3 CARG1, AT, zero
    if (vk) {
      |  cmovnez_3 TMP2, AT, zero
    } else {
      |  cmoveqz_3 TMP2, AT, zero
    }
    |  beqz CARG1, <1	// Done if not tab/ud or not same type or same tv.
    |  // Different tables or userdatas. Need to check __eq metamethod.
    |  // Field metatable must be at same offset for GCtab and GCudata!
    |  ld TAB:TMP1, TAB:CARG2->metatable
    |  beqz TAB:TMP1, <1		// No metatable?
    |  lbu TMP1, TAB:TMP1->nomm
    |  andi TMP1, TMP1, 1<<MM_eq
    |  bnez TMP1, <1			// Or 'no __eq' flag set?
    |  addi CARG4, zero, 1-vk			// ne = 0 or 1.
    |  b ->vmeta_equal			// Handle __eq metamethod.
    break;

  case BC_ISEQS: case BC_ISNES:
    vk = op == BC_ISEQS;
    |  // RA = src*8, RD = str_const*8 (~), JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  ld TMP0, HI(RA)
    |   srl RD, RD, 1
    |  ld STR:TMP3, LO(RA)
    |   sub RD, KBASE, RD
    |    ld2u TMP2, -4+OFS_RD(PC)
    |.if FFI
    |  li AT, LJ_TCDATA
    |  beq TMP0, AT, ->vmeta_equal_cd
    |.endif
    |.  ld STR:TMP1, -4(RD)		// KBASE-4-str_const*4
    |  addli TMP0, TMP0, -LJ_TSTR
    |   decode_RD4b TMP2
    |  xor TMP1, STR:TMP1, STR:TMP3
    |  or TMP0, TMP0, TMP1
    |   //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |   add TMP2, TMP2, TMP3
    if (vk) {
      |  movn TMP2, r0, TMP0
    } else {
      |  movz TMP2, r0, TMP0
    }
    |  add PC, PC, TMP2
    |  ins_next
    break;

  case BC_ISEQN: case BC_ISNEN:
    vk = op == BC_ISEQN;
    |  // RA = src*8, RD = num_const*8, JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  ld TMP0, HI(RA)
    |   ldc1 f0, 0(RA)
    |  add RD, KBASE, RD
    |    ld2u TMP2, -4+OFS_RD(PC)
    |   ldc1 f2, 0(RD)
    |    //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |  cmpltui AT, TMP0, LJ_TISNUM
    |    decode_RD4b TMP2
    |.if FFI
    |  beqz AT, >5
    |.else
    |  beqz AT, >1
    |.endif
    |.   add TMP2, TMP2, TMP3
    |   c.eq.d f0, f2
    if (vk) {
      |  movf TMP2, r0
      |  add PC, PC, TMP2
      |1:
    } else {
      |  movt TMP2, r0
      |1:
      |  add PC, PC, TMP2
    }
    |  ins_next
    |.if FFI
    |5:
    |  li AT, LJ_TCDATA
    |  beq TMP0, AT, ->vmeta_equal_cd
    |  b <1
    |.endif
    break;

  case BC_ISEQP: case BC_ISNEP:
    vk = op == BC_ISEQP;
    |  // RA = src*8, RD = primitive_type*8 (~), JMP with RD = target
    |  add RA, BASE, RA
    |   srl TMP1, RD, 3
    |  ld TMP0, HI(RA)
    |    ld2u TMP2, OFS_RD(PC)
    |   not TMP1, TMP1
    |    addli PC, PC, 4
    |.if FFI
    |  li AT, LJ_TCDATA
    |  beq TMP0, AT, ->vmeta_equal_cd
    |.endif
    |.  xor TMP0, TMP0, TMP1
    |  decode_RD4b TMP2
    |  //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |  add TMP2, TMP2, TMP3
    if (vk) {
      |  movn TMP2, r0, TMP0
    } else {
      |  movz TMP2, r0, TMP0
    }
    |  add PC, PC, TMP2
    |  ins_next
    break;

  /* -- Unary test and copy ops ------------------------------------------- */

  case BC_ISTC: case BC_ISFC: case BC_IST: case BC_ISF:
    |  // RA = dst*8 or unused, RD = src*8, JMP with RD = target
    |  add RD, BASE, RD
    |   ld2u TMP2, OFS_RD(PC)
    |  ld TMP0, HI(RD)
    |   addli PC, PC, 4
    if (op == BC_IST || op == BC_ISF) {
      |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
      |   decode_RD4b TMP2
      |   //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
      |   add TMP2, TMP2, TMP3
      if (op == BC_IST) {
	|  movz TMP2, r0, TMP0
      } else {
	|  movn TMP2, r0, TMP0
      }
      |  add PC, PC, TMP2
    } else {
      |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
      |  ldc1 f0, 0(RD)
      |  add RA, BASE, RA
      if (op == BC_ISTC) {
	|  beqz TMP0, >1
      } else {
	|  bnez TMP0, >1
      }
      |   decode_RD4b TMP2
      |   //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
      |   add TMP2, TMP2, TMP3
      |  sdc1 f0, 0(RA)
      |   add PC, PC, TMP2
      |1:
    }
    |  ins_next
    break;

  /* -- Unary ops --------------------------------------------------------- */

  case BC_MOV:
    |  // RA = dst*8, RD = src*8
    |  add RD, BASE, RD
    |  add RA, BASE, RA
    |  ldc1 f0, 0(RD)
    |  ins_next1
    |  sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_NOT:
    |  // RA = dst*8, RD = src*8
    |  add RD, BASE, RD
    |   add RA, BASE, RA
    |  ld TMP0, HI(RD)
    |   li TMP1, LJ_TFALSE
    |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
    |  addli TMP1, TMP0, LJ_TTRUE
    |  ins_next1
    |  st TMP1, HI(RA)
    |  ins_next2
    break;
  case BC_UNM:
    |  // RA = dst*8, RD = src*8
    |  add CARG3, BASE, RD
    |   add RA, BASE, RA
    |  ld TMP0, HI(CARG3)
    |   ldc1 f0, 0(CARG3)
    |  cmpltui AT, TMP0, LJ_TISNUM
    |  neg.d f0, f0
    |  beqz AT, ->vmeta_unm
    |  ins_next1
    |   sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_LEN:
    |  // RA = dst*8, RD = src*8
    |  add CARG2, BASE, RD
    |   add RA, BASE, RA
    |  ld TMP0, HI(CARG2)
    |   ld CARG1, LO(CARG2)
    |  li AT, LJ_TSTR
    |  addi AT, LJ_TTAB
    |  cmpne TMP0, AT, >2
    |  ld CRET1, STR:CARG1->len
    |1:
    |  mtc1 CRET1, f0
    |  cvt.d.w f0, f0
    |  ins_next1
    |  sdc1 f0, 0(RA)
    |  ins_next2
    |2:
    |  cmpne TMP0, AT, ->vmeta_len
#if LJ_52
    |  ld TAB:TMP2, TAB:CARG1->metatable
    |  bnez TAB:TMP2, >9
    |3:
#endif
    |->BC_LEN_Z:
    |  load_got lj_tab_len
    |  call_intern lj_tab_len		// (GCtab *t)
    |  // Returns uint32_t (but less than 2^31).
    |  b <1
#if LJ_52
    |9:
    |  lbu TMP0, TAB:TMP2->nomm
    |  andi TMP0, TMP0, 1<<MM_len
    |  bnez TMP0, <3			// 'no __len' flag set: done.
    |  b ->vmeta_len
#endif
    break;

  /* -- Binary ops -------------------------------------------------------- */

    |.macro ins_arithpre
    ||vk = ((int)op - BC_ADDVN) / (BC_ADDNV-BC_ADDVN);
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  // RA = dst*8, RB = src1*8, RC = src2*8 | num_const*8
    ||switch (vk) {
    ||case 0:
    |   add CARG3, BASE, RB
    |    add CARG4, KBASE, RC
    |   ld TMP1, HI(CARG3)
    |   ldc1 f20, 0(CARG3)
    |    ldc1 f22, 0(CARG4)
    |   cmpltui AT, TMP1, LJ_TISNUM
    ||  break;
    ||case 1:
    |   add CARG4, BASE, RB
    |    add CARG3, KBASE, RC
    |   ld TMP1, HI(CARG4)
    |   ldc1 f22, 0(CARG4)
    |    ldc1 f20, 0(CARG3)
    |   cmpltui AT, TMP1, LJ_TISNUM
    ||  break;
    ||default:
    |   add CARG3, BASE, RB
    |    add CARG4, BASE, RC
    |   ld TMP1, HI(CARG3)
    |    ld TMP2, HI(CARG4)
    |   ldc1 f20, 0(CARG3)
    |    ldc1 f22, 0(CARG4)
    |   cmpltui AT, TMP1, LJ_TISNUM
    |   cmpltui TMP0, TMP2, LJ_TISNUM
    |   and AT, AT, TMP0
    ||  break;
    ||}
    |  beqz AT, ->vmeta_arith
    |.  add RA, BASE, RA
    |.endmacro
    |
    |.macro fpmod, a, b, c
    |->BC_MODVN_Z:
    |  div.d FARG1, b, c
    |  bal ->vm_floor			// floor(b/c)
    |  mul.d a, FRET1, c
    |  sub.d a, b, a			// b - floor(b/c)*c
    |.endmacro
    |
    |.macro ins_arith, ins
    |  ins_arithpre
    |.if "ins" == "fpmod_"
    |  b ->BC_MODVN_Z			// Avoid 3 copies. It's slow anyway.
    |.else
    |  ins f0, f20, f22
    |  ins_next1
    |  sdc1 f0, 0(RA)
    |  ins_next2
    |.endif
    |.endmacro

  case BC_ADDVN: case BC_ADDNV: case BC_ADDVV:
    |  ins_arith add.d
    break;
  case BC_SUBVN: case BC_SUBNV: case BC_SUBVV:
    |  ins_arith sub.d
    break;
  case BC_MULVN: case BC_MULNV: case BC_MULVV:
    |  ins_arith mul.d
    break;
  case BC_DIVVN: case BC_DIVNV: case BC_DIVVV:
    |  ins_arith div.d
    break;
  case BC_MODVN:
    |  ins_arith fpmod
    break;
  case BC_MODNV: case BC_MODVV:
    |  ins_arith fpmod_
    break;
  case BC_POW:
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG3, BASE, RB
    |   add CARG4, BASE, RC
    |  ld TMP1, HI(CARG3)
    |   ld TMP2, HI(CARG4)
    |  ldc1 FARG1, 0(CARG3)
    |   ldc1 FARG2, 0(CARG4)
    |  cmpltui AT, TMP1, LJ_TISNUM
    |  cmpltui TMP0, TMP2, LJ_TISNUM
    |  and AT, AT, TMP0
    |  load_got pow
    |  add RA, BASE, RA
    |  beqz AT, ->vmeta_arith
    |  call_extern
    |  ins_next1
    |  sdc1 FRET1, 0(RA)
    |  ins_next2
    break;

  case BC_CAT:
    |  // RA = dst*8, RB = src_start*8, RC = src_end*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  sub CARG3, RC, RB
    |   st BASE, L->base
    |  add CARG2, BASE, RC
    |  move MULTRES, RB
    |->BC_CAT_Z:
    |  load_got lj_meta_cat
    |  srl CARG3, CARG3, 3
    |   st sp, PC
    |  move CARG1, L
    |  call_intern lj_meta_cat		// (lua_State *L, TValue *top, int left)
    |  // Returns NULL (finished) or TValue * (metamethod).
    |  addlo AT, L:LREG->base
    |  ld BASE, AT
    |  bnez CRET1, ->vmeta_binop
    |  add RB, BASE, MULTRES
    |  ldc1 f0, 0(RB)
    |   add RA, BASE, RA
    |  ins_next1
    |   sdc1 f0, 0(RA)			// Copy result from RB to RA.
    |  ins_next2
    break;

  /* -- Constant ops ------------------------------------------------------ */

  case BC_KSTR:
    |  // RA = dst*8, RD = str_const*8 (~)
    |  srl TMP1, RD, 1
    |  sub TMP1, KBASE, TMP1
    |  ins_next1
    |  ld TMP0, -4(TMP1)		// KBASE-4-str_const*4
    |  add RA, BASE, RA
    |   li TMP2, LJ_TSTR
    |  st TMP0, LO(RA)
    |   st TMP2, HI(RA)
    |  ins_next2
    break;
  case BC_KCDATA:
    |.if FFI
    |  // RA = dst*8, RD = cdata_const*8 (~)
    |  srl TMP1, RD, 1
    |  sub TMP1, KBASE, TMP1
    |  ins_next1
    |  ld TMP0, -4(TMP1)		// KBASE-4-cdata_const*4
    |  add RA, BASE, RA
    |   li TMP2, LJ_TCDATA
    |  st TMP0, LO(RA)
    |   st TMP2, HI(RA)
    |  ins_next2
    |.endif
    break;
  case BC_KSHORT:
    |  // RA = dst*8, RD = int16_literal*8
    |  sra RD, INS, 16
    |  mtc1 RD, f0
    |  add RA, BASE, RA
    |  cvt.d.w f0, f0
    |  ins_next1
    |  sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_KNUM:
    |  // RA = dst*8, RD = num_const*8
    |  add RD, KBASE, RD
    |   add RA, BASE, RA
    |  ldc1 f0, 0(RD)
    |  ins_next1
    |   sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_KPRI:
    |  // RA = dst*8, RD = primitive_type*8 (~)
    |  srl TMP1, RD, 3
    |   add RA, BASE, RA
    |  not TMP0, TMP1
    |  ins_next1
    |   st TMP0, HI(RA)
    |  ins_next2
    break;
  case BC_KNIL:
    |  // RA = base*8, RD = end*8
    |  add RA, BASE, RA
    |  st TISNIL, HI(RA)
    |   addli RA, RA, 8
    |  add RD, BASE, RD
    |1:
    |  st TISNIL, HI(RA)
    |  slt AT, RA, RD
    |  addli RA, RA, 8
    |  bnez AT, <1
    |  ins_next_
    break;

  /* -- Upvalue and function ops ------------------------------------------ */

  case BC_UGET:
    |  // RA = dst*8, RD = uvnum*8
    |  ld LFUNC:RB, FRAME_FUNC(BASE)
    |   srl RD, RD, 1
    |   add RD, RD, LFUNC:RB
    |  ld UPVAL:RB, LFUNC:RD->uvptr
    |  ins_next1
    |  ld TMP1, UPVAL:RB->v
    |  ldc1 f0, 0(TMP1)
    |  add RA, BASE, RA
    |  sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_USETV:
    |  // RA = uvnum*8, RD = src*8
    |  ld LFUNC:RB, FRAME_FUNC(BASE)
    |    srl RA, RA, 1
    |   add RD, BASE, RD
    |    add RA, RA, LFUNC:RB
    |   ldc1 f0, 0(RD)
    |  ld UPVAL:RB, LFUNC:RA->uvptr
    |  lbu TMP3, UPVAL:RB->marked
    |   ld CARG2, UPVAL:RB->v
    |  andi TMP3, TMP3, LJ_GC_BLACK	// isblack(uv)
    |  lbu TMP0, UPVAL:RB->closed
    |   ld TMP2, HI(RD)
    |   sdc1 f0, 0(CARG2)
    |  li AT, LJ_GC_BLACK|1
    |  or TMP3, TMP3, TMP0
    |  addli TMP2, TMP2, -(LJ_TISNUM+1)
    |  beq TMP3, AT, >2			// Upvalue is closed and black?
    |1:
    |  ins_next
    |
    |2:  // Check if new value is collectable.
    |  cmpltui AT, TMP2, LJ_TISGCV - (LJ_TISNUM+1)
    |  addi AT, RD, LO
    |  ld TMP1, AT
    |  beqz AT, <1			// tvisgcv(v)
    |  lbu TMP3, GCOBJ:TMP1->gch.marked
    |  andi TMP3, TMP3, LJ_GC_WHITES	// iswhite(v)
    |  load_got lj_gc_barrieruv
    |  beqz TMP3, <1
    |  // Crossed a write barrier. Move the barrier forward.
    |  addli CARG1, DISPATCH, GG_DISP2G
    |  call_intern lj_gc_barrieruv	// (global_State *g, TValue *tv)
    |  b <1
    break;
  case BC_USETS:
    |  // RA = uvnum*8, RD = str_const*8 (~)
    |  ld LFUNC:RB, FRAME_FUNC(BASE)
    |    srl RA, RA, 1
    |   srl TMP1, RD, 1
    |    add RA, RA, LFUNC:RB
    |   sub TMP1, KBASE, TMP1
    |  ld UPVAL:RB, LFUNC:RA->uvptr
    |   ld STR:TMP1, -4(TMP1)		// KBASE-4-str_const*4
    |  lbu TMP2, UPVAL:RB->marked
    |   ld CARG2, UPVAL:RB->v
    |   lbu TMP3, STR:TMP1->marked
    |  andi AT, TMP2, LJ_GC_BLACK	// isblack(uv)
    |   lbu TMP2, UPVAL:RB->closed
    |   li TMP0, LJ_TSTR
    |   st STR:TMP1, LO(CARG2)
    |  addi AT, CARG2, HI
    |  st AT, TMP0
    |  bnez AT, >2
    |1:
    |  ins_next
    |
    |2:  // Check if string is white and ensure upvalue is closed.
    |  andi AT, TMP3, LJ_GC_WHITES	// iswhite(str)
    |  beqz TMP2, <1
    |  load_got lj_gc_barrieruv
    |  beqz AT, <1
    |  // Crossed a write barrier. Move the barrier forward.
    |  addli CARG1, DISPATCH, GG_DISP2G
    |  call_intern lj_gc_barrieruv	// (global_State *g, TValue *tv)
    |  b <1
    break;
  case BC_USETN:
    |  // RA = uvnum*8, RD = num_const*8
    |  ld LFUNC:RB, FRAME_FUNC(BASE)
    |   srl RA, RA, 1
    |    add RD, KBASE, RD
    |   add RA, RA, LFUNC:RB
    |    ldc1 f0, 0(RD)
    |  ld UPVAL:RB, LFUNC:RA->uvptr
    |  ins_next1
    |  ld TMP1, UPVAL:RB->v
    |  sdc1 f0, 0(TMP1)
    |  ins_next2
    break;
  case BC_USETP:
    |  // RA = uvnum*8, RD = primitive_type*8 (~)
    |  ld LFUNC:RB, FRAME_FUNC(BASE)
    |   srl RA, RA, 1
    |    srl TMP0, RD, 3
    |   add RA, RA, LFUNC:RB
    |    not TMP0, TMP0
    |  ld UPVAL:RB, LFUNC:RA->uvptr
    |  ins_next1
    |  ld TMP1, UPVAL:RB->v
    |  st TMP0, HI(TMP1)
    |  ins_next2
    break;

  case BC_UCLO:
    |  // RA = level*8, RD = target
    |  ld TMP2, L->openupval
    |  branch_RD			// Do this first since RD is not saved.
    |  load_got lj_func_closeuv
    |   st BASE, L->base
    |  move CARG1, L
    |  beqz TMP2, >1
    |  add CARG2, BASE, RA
    |  call_intern lj_func_closeuv	// (lua_State *L, TValue *level)
    |  ld BASE, L->base
    |1:
    |  ins_next
    break;

  case BC_FNEW:
    |  // RA = dst*8, RD = proto_const*8 (~) (holding function prototype)
    |  srl TMP1, RD, 1
    |  load_got lj_func_newL_gc
    |  sub TMP1, KBASE, TMP1
    |  addi CARG3, BASE, FRAME_FUNC
    |  ld CARG3, CARG3
    |  addi CARG2, TMP1, -4
    |  ld CARG2, CARG2
    |   addlo AT, L:LREG->base
    |   st AT, BASE
    |   st sp, PC
    |  // (lua_State *L, GCproto *pt, GCfuncL *parent)
    |  move CARG1, L
    |  call_intern lj_func_newL_gc
    |  // Returns GCfuncL *.
    |  ld BASE, L->base
    |   li TMP0, LJ_TFUNC
    |  ins_next1
    |  add RA, BASE, RA
    |  st TMP0, HI(RA)
    |  st LFUNC:CRET1, LO(RA)
    |  ins_next2
    break;

  /* -- Table ops --------------------------------------------------------- */

  case BC_TNEW:
  case BC_TDUP:
    |  // RA = dst*8, RD = (hbits|asize)*8 | tab_const*8 (~)
    |  ld TMP0, DISPATCH_GL(gc.total)(DISPATCH)
    |  ld TMP1, DISPATCH_GL(gc.threshold)(DISPATCH)
    |   addlo AT, L:LREG->base
    |   st AT, BASE
    |   st sp, PC
    |  cmpltu AT, TMP0, TMP1
    |  beqz AT, >5
    |1:
    if (op == BC_TNEW) {
      |  load_got lj_tab_new
      |  srl CARG2, RD, 3
      |  andi CARG2, CARG2, 0x7ff
      |  li TMP0, 0x801
      |  addli AT, CARG2, -0x7ff
      |   srl CARG3, RD, 14
      |  movz CARG2, TMP0, AT
      |  // (lua_State *L, int32_t asize, uint32_t hbits)
      |  move CARG1, L
      |  call_intern lj_tab_new
      |  // Returns Table *.
    } else {
      |  load_got lj_tab_dup
      |  shli TMP1, RD, 1
      |  sub TMP1, KBASE, TMP1
      |  move CARG1, L
      |  addi AT, TMP1, -4
      |  ld CARG2, AT
      |  call_intern lj_tab_dup		// (lua_State *L, Table *kt)
      |  // Returns Table *.
    }
    |  ld BASE, L->base
    |  ins_next1
    |  add RA, BASE, RA
    |   li TMP0, LJ_TTAB
    |  st TAB:CRET1, LO(RA)
    |   st TMP0, HI(RA)
    |  ins_next2
    |5:
    |  load_got lj_gc_step_fixtop
    |  move MULTRES, RD
    |  move CARG1, L
    |  call_intern lj_gc_step_fixtop	// (lua_State *L)
    |  move RD, MULTRES
    |  b <1
    break;

  case BC_GGET:
    |  // RA = dst*8, RD = str_const*8 (~)
  case BC_GSET:
    |  // RA = src*8, RD = str_const*8 (~)
    |  ld LFUNC:TMP2, FRAME_FUNC(BASE)
    |   srl TMP1, RD, 1
    |   sub TMP1, KBASE, TMP1
    |  ld TAB:RB, LFUNC:TMP2->env
    |  ld STR:RC, -4(TMP1)		// KBASE-4-str_const*4
    |  add RA, BASE, RA
    if (op == BC_GGET) {
      |  b ->BC_TGETS_Z
    } else {
      |  b ->BC_TSETS_Z
    }
    break;

  case BC_TGETV:
    |  // RA = dst*8, RB = table*8, RC = key*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG2, BASE, RB
    |   add CARG3, BASE, RC
    |  ld TMP1, HI(CARG2)
    |   ld TMP2, HI(CARG3)
    |    ld TAB:RB, LO(CARG2)
    |  li AT, LJ_TTAB
    |   ldc1 f0, 0(CARG3)
    |  add RA, BASE, RA
    |  cmpne TMP1, AT, ->vmeta_tgetv
    |  cmpltui AT, TMP2, LJ_TISNUM
    |  addi AT, zero, LJ_TSTR
    |  beqz AT, >5
    |
    |  // Convert number key to integer, check for integerness and range.
    |  cvt.w.d f2, f0
    |   ld TMP0, TAB:RB->asize
    |  mfc1 TMP2, f2
    |  cvt.d.w f4, f2
    |   ld TMP1, TAB:RB->array
    |  c.eq.d f0, f4
    |  cmpltu AT, TMP2, TMP0
    |  movf AT, r0
    |   shli TMP2, TMP2, 3
    |  add TMP2, TMP1, TMP2
    |  beqz AT, ->vmeta_tgetv		// Integer key and in array part?
    |  ld TMP0, HI(TMP2)
    |  ldc1 f0, 0(TMP2)
    |  beq TMP0, TISNIL, >2
    |1:
    |  ins_next1
    |   sdc1 f0, 0(RA)
    |  ins_next2
    |
    |2:  // Check for __index if table value is nil.
    |  ld TAB:TMP2, TAB:RB->metatable
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  lbu TMP0, TAB:TMP2->nomm
    |  andi TMP0, TMP0, 1<<MM_index
    |  bnez TMP0, <1			// 'no __index' flag set: done.
    |  b ->vmeta_tgetv
    |
    |5:
    |  addi AT, CARG3, LO
    |  ld STR:RC, AT
    |  cmpne TMP2, AT, ->vmeta_tgetv
    |  b ->BC_TGETS_Z			// String key?
    break;
  case BC_TGETS:
    |  // RA = dst*8, RB = table*8, RC = str_const*4 (~)
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RC4a RC, INS
    |  ld TMP0, HI(CARG2)
    |   decode_RC4b RC
    |  li AT, LJ_TTAB
    |   ld TAB:RB, LO(CARG2)
    |   sub CARG3, KBASE, RC
    |   ld STR:RC, -4(CARG3)		// KBASE-4-str_const*4
    |  add RA, BASE, RA
    |  cmpne TMP0, AT, ->vmeta_tgets1
    |->BC_TGETS_Z:
    |  // TAB:RB = GCtab *, STR:RC = GCstr *, RA = dst*8
    |  ld TMP0, TAB:RB->hmask
    |  ld TMP1, STR:RC->hash
    |  ld NODE:TMP2, TAB:RB->node
    |  and TMP1, TMP1, TMP0		// idx = str->hash & tab->hmask
    |  shli TMP0, TMP1, 5
    |  shli TMP1, TMP1, 3
    |  sub TMP1, TMP0, TMP1
    |  add NODE:TMP2, NODE:TMP2, TMP1	// node = tab->node + (idx*32-idx*8)
    |1:
    |  ld CARG1, offsetof(Node, key)+HI(NODE:TMP2)
    |   ld TMP0, offsetof(Node, key)+LO(NODE:TMP2)
    |    ld NODE:TMP1, NODE:TMP2->next
    |    ld CARG2, offsetof(Node, val)+HI(NODE:TMP2)
    |  addli CARG1, CARG1, -LJ_TSTR
    |   xor TMP0, TMP0, STR:RC
    |  or AT, CARG1, TMP0
    |  addlo AT, TAB:RB->metatable
    |  ld TAB:TMP3, AT
    |  bnez AT, >4
    |    addi AT, NODE:TMP2, offsetof(Node, val)+LO
    |    ld CARG1, AT
    |    beq CARG2, TISNIL, >5		// Key found, but nil value?
    |3:
    |  ins_next1
    |    st CARG2, HI(RA)
    |     st CARG1, LO(RA)
    |  ins_next2
    |
    |4:  // Follow hash chain.
    |  move NODE:TMP2, NODE:TMP1
    |  bnez NODE:TMP1, <1
    |  // End of hash chain: key not found, nil result.
    |
    |5:  // Check for __index if table value is nil.
    |  addi CARG2, zero, LJ_TNIL
    |  beqz TAB:TMP3, <3		// No metatable: done.
    |  addlo AT, TAB:TMP3->nomm
    |  ld1u TMP0, AT
    |  andi TMP0, TMP0, 1<<MM_index
    |  bnez TMP0, <3			// 'no __index' flag set: done.
    |  b ->vmeta_tgets
    break;
  case BC_TGETB:
    |  // RA = dst*8, RB = table*8, RC = index*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RDtoRC8 RC, RD
    |  ld CARG1, HI(CARG2)
    |  li AT, LJ_TTAB
    |   ld TAB:RB, LO(CARG2)
    |   add RA, BASE, RA
    |  shli TMP0, RC, 3
    |  cmpne CARG1, AT, ->vmeta_tgetb
    |  ld TMP1, TAB:RB->asize
    |   ld TMP2, TAB:RB->array
    |  cmpltu AT, TMP0, TMP1
    |  add RC, TMP2, RC
    |  beqz AT, ->vmeta_tgetb
    |  ld TMP1, HI(RC)
    |  ldc1 f0, 0(RC)
    |  beq TMP1, TISNIL, >5
    |1:
    |  ins_next1
    |   sdc1 f0, 0(RA)
    |  ins_next2
    |
    |5:  // Check for __index if table value is nil.
    |  ld TAB:TMP2, TAB:RB->metatable
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  addlo AT, TAB:TMP2->nomm
    |  ld1u TMP1, AT
    |  andi TMP1, TMP1, 1<<MM_index
    |  bnez TMP1, <1			// 'no __index' flag set: done.
    |  b ->vmeta_tgetb			// Caveat: preserve TMP0!
    break;

  case BC_TSETV:
    |  // RA = src*8, RB = table*8, RC = key*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG2, BASE, RB
    |   add CARG3, BASE, RC
    |  ld TMP1, HI(CARG2)
    |   ld TMP2, HI(CARG3)
    |    ld TAB:RB, LO(CARG2)
    |  li AT, LJ_TTAB
    |   ldc1 f0, 0(CARG3)
    |  add RA, BASE, RA
    |  cmpne TMP1, AT, ->vmeta_tsetv
    |  cmpltui AT, TMP2, LJ_TISNUM
    |  addi AT, zero, LJ_TSTR
    |  beqz AT, >5
    |
    |  // Convert number key to integer, check for integerness and range.
    |  cvt.w.d f2, f0
    |   ld TMP0, TAB:RB->asize
    |  mfc1 TMP2, f2
    |  cvt.d.w f4, f2
    |   ld TMP1, TAB:RB->array
    |  c.eq.d f0, f4
    |  cmpltu AT, TMP2, TMP0
    |  movf AT, r0
    |   shli TMP2, TMP2, 3
    |  add TMP1, TMP1, TMP2
    |  beqz AT, ->vmeta_tsetv		// Integer key and in array part?
    |   addlo AT, TAB:RB->marked
    |   ld1u TMP3, AT
    |  addi AT, TMP1, HI
    |  ld TMP0, AT
    |  ldc1 f0, 0(RA)
    |  beq TMP0, TISNIL, >3
    |1:
    |   andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  sdc1 f0, 0(TMP1)
    |   bnez AT, >7
    |2:
    |  ins_next
    |
    |3:  // Check for __newindex if previous value is nil.
    |  ld TAB:TMP2, TAB:RB->metatable
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  lbu TMP2, TAB:TMP2->nomm
    |  andi TMP2, TMP2, 1<<MM_newindex
    |  bnez TMP2, <1			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsetv
    |
    |5:
    |  addi AT, CARG3, LO
    |  ld STR:RC, AT
    |  cmpne TMP2, AT, ->vmeta_tsetv
    |  b ->BC_TSETS_Z			// String key?
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <2
    break;
  case BC_TSETS:
    |  // RA = src*8, RB = table*8, RC = str_const*8 (~)
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RC4a RC, INS
    |  ld TMP0, HI(CARG2)
    |   decode_RC4b RC
    |  li AT, LJ_TTAB
    |   sub CARG3, KBASE, RC
    |    ld TAB:RB, LO(CARG2)
    |   ld STR:RC, -4(CARG3)		// KBASE-4-str_const*4
    |  add RA, BASE, RA
    |  cmpne TMP0, AT, ->vmeta_tsets1
    |->BC_TSETS_Z:
    |  // TAB:RB = GCtab *, STR:RC = GCstr *, RA = BASE+src*8
    |  ld TMP0, TAB:RB->hmask
    |  ld TMP1, STR:RC->hash
    |  ld NODE:TMP2, TAB:RB->node
    |   sb r0, TAB:RB->nomm		// Clear metamethod cache.
    |  and TMP1, TMP1, TMP0		// idx = str->hash & tab->hmask
    |  shli TMP0, TMP1, 5
    |  shli TMP1, TMP1, 3
    |  sub TMP1, TMP0, TMP1
    |  add NODE:TMP2, NODE:TMP2, TMP1	// node = tab->node + (idx*32-idx*8)
    |   ldc1 f20, 0(RA)
    |1:
    |  ld CARG1, offsetof(Node, key)+HI(NODE:TMP2)
    |   ld TMP0, offsetof(Node, key)+LO(NODE:TMP2)
    |  li AT, LJ_TSTR
    |    ld NODE:TMP1, NODE:TMP2->next
    |   addli AT, NODE:TMP2, offsetof(Node, val)+HI
    |   ld CARG2, AT
    |  cmpne CARG1, AT, >5
    |    addlo AT, TAB:RB->marked
    |    ld1u TMP3, AT
    |   cmpne TMP0, STR:RC, >5
    |    addlo AT, TAB:RB->metatable
    |    ld TAB:TMP0, AT
    |    beq CARG2, TISNIL, >4		// Key found, but nil value?
    |2:
    |  andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  sdc1 f20, NODE:TMP2->val
    |  bnez AT, >7
    |3:
    |  ins_next
    |
    |4:  // Check for __newindex if previous value is nil.
    |  beqz TAB:TMP0, <2		// No metatable: done.
    |  lbu TMP0, TAB:TMP0->nomm
    |  andi TMP0, TMP0, 1<<MM_newindex
    |  bnez TMP0, <2			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsets
    |
    |5:  // Follow hash chain.
    |  move NODE:TMP2, NODE:TMP1
    |  bnez NODE:TMP1, <1
    |  // End of hash chain: key not found, add a new one
    |
    |  // But check for __newindex first.
    |  ld TAB:TMP2, TAB:RB->metatable
    |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
    |  beqz TAB:TMP2, >6		// No metatable: continue.
    |  lbu TMP0, TAB:TMP2->nomm
    |  andi TMP0, TMP0, 1<<MM_newindex
    |  addi AT, zero, LJ_TSTR
    |  beqz TMP0, ->vmeta_tsets		// 'no __newindex' flag NOT set: check.
    |6:
    |  load_got lj_tab_newkey
    |  st STR:RC, LO(CARG3)
    |  st AT, HI(CARG3)
    |   st BASE, L->base
    |  move CARG2, TAB:RB
    |   st PC, SAVE_PC
    |  move CARG1, L
    |  call_intern lj_tab_newkey	// (lua_State *L, GCtab *t, TValue *k
    |  // Returns TValue *.
    |  ld BASE, L->base
    |  sdc1 f20, 0(CRET1)
    |  b <3				// No 2nd write barrier needed.
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <3
    break;
  case BC_TSETB:
    |  // RA = src*8, RB = table*8, RC = index*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RDtoRC8 RC, RD
    |  ld CARG1, HI(CARG2)
    |  li AT, LJ_TTAB
    |   ld TAB:RB, LO(CARG2)
    |   add RA, BASE, RA
    |  shrui TMP0, RC, 3
    |  cmpne CARG1, AT, ->vmeta_tsetb
    |  ld TMP1, TAB:RB->asize
    |   ld TMP2, TAB:RB->array
    |  cmpltu AT, TMP0, TMP1
    |  add RC, TMP2, RC
    |  beqz AT, ->vmeta_tsetb
    |  ld TMP1, HI(RC)
    |   lbu TMP3, TAB:RB->marked
    |  ldc1 f0, 0(RA)
    |  beq TMP1, TISNIL, >5
    |1:
    |  andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  sdc1 f0, 0(RC)
    |  bnez AT, >7
    |2:
    |  ins_next
    |
    |5:  // Check for __newindex if previous value is nil.
    |  ld TAB:TMP2, TAB:RB->metatable
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  lbu TMP1, TAB:TMP2->nomm
    |  andi TMP1, TMP1, 1<<MM_newindex
    |  bnez TMP1, <1			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsetb			// Caveat: preserve TMP0!
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <2
    break;

  case BC_TSETM:
    |  // RA = base*8 (table at base-1), RD = num_const*8 (start index)
    |  add RA, BASE, RA
    |1:
    |   add TMP3, KBASE, RD
    |  ld TAB:CARG2, -8+LO(RA)		// Guaranteed to be a table.
    |    addli TMP0, MULTRES, -8
    |   ld TMP3, LO(TMP3)		// Integer constant is in lo-word.
    |    shrui CARG3, TMP0, 3
    |    beqz TMP0, >4			// Nothing to copy?
    |  add CARG3, CARG3, TMP3
    |  ld TMP2, TAB:CARG2->asize
    |   shli TMP1, TMP3, 3
    |    lbu TMP3, TAB:CARG2->marked
    |   ld CARG1, TAB:CARG2->array
    |  cmpltu AT, TMP2, CARG3
    |  add TMP2, RA, TMP0
    |  bnez AT, >5
    |   add TMP1, TMP1, CARG1
    |  andi TMP0, TMP3, LJ_GC_BLACK	// isblack(table)
    |3:  // Copy result slots to table.
    |   ldc1 f0, 0(RA)
    |    addli RA, RA, 8
    |  cmpltu AT, RA, TMP2
    |   sdc1 f0, 0(TMP1)
    |   addli TMP1, TMP1, 8
    |  bnez AT, <3
    |  bnez TMP0, >7
    |4:
    |  ins_next
    |
    |5:  // Need to resize array part.
    |  load_got lj_tab_reasize
    |   st BASE, L->base
    |   st PC, SAVE_PC
    |  move BASE, RD
    |  move CARG1, L
    |  call_intern lj_tab_reasize	// (lua_State *L, GCtab *t, int nasize)
    |  // Must not reallocate the stack.
    |  move RD, BASE
    |  addlo AT, L:LREG->base	// Reload BASE for lack of a saved register.
    |  ld BASE, AT	// Reload BASE for lack of a saved register.
    |  b <1
    |
    |7:  // Possible table write barrier for any value. Skip valiswhite check.
    |  barrierback TAB:CARG2, TMP3, TMP0, <4
    break;

  /* -- Calls and vararg handling ----------------------------------------- */

  case BC_CALLM:
    |  // RA = base*8, (RB = (nresults+1)*8,) RC = extra_nargs*8
    |  decode_RDtoRC8 NARGS8:RC, RD
    |  add NARGS8:RC, NARGS8:RC, MULTRES
    |  b ->BC_CALL_Z
    break;
  case BC_CALL:
    |  // RA = base*8, (RB = (nresults+1)*8,) RC = (nargs+1)*8
    |  decode_RDtoRC8 NARGS8:RC, RD
    |->BC_CALL_Z:
    |  move TMP2, BASE
    |  add BASE, BASE, RA
    |   li AT, LJ_TFUNC
    |  ld TMP0, HI(BASE)
    |   ld LFUNC:RB, LO(BASE)
    |   addli BASE, BASE, 8
    |  addli NARGS8:RC, NARGS8:RC, -8
    |  cmpne TMP0, AT, ->vmeta_call
    |  ins_call
    break;

  case BC_CALLMT:
    |  // RA = base*8, (RB = 0,) RC = extra_nargs*8
    |  add NARGS8:RD, NARGS8:RD, MULTRES	// BC_CALLT gets RC from RD.
    |  // Fall through. Assumes BC_CALLT follows.
    break;
  case BC_CALLT:
    |  // RA = base*8, (RB = 0,) RC = (nargs+1)*8
    |  add RA, BASE, RA
    |   li AT, LJ_TFUNC
    |  ld TMP0, HI(RA)
    |   ld LFUNC:RB, LO(RA)
    |   move NARGS8:RC, RD
    |    ld TMP1, FRAME_PC(BASE)
    |   addli RA, RA, 8
    |  addli NARGS8:RC, NARGS8:RC, -8
    |  cmpne TMP0, AT, ->vmeta_callt
    |->BC_CALLT_Z:
    |  andi TMP0, TMP1, FRAME_TYPE	// Caveat: preserve TMP0 until the 'or'.
    |   lbu TMP3, LFUNC:RB->ffid
    |  xori TMP2, TMP1, FRAME_VARG
    |  bnez TMP0, >7
    |1:
    |  st LFUNC:RB, FRAME_FUNC(BASE)	// Copy function down, but keep PC.
    |  cmpltui AT, TMP3, 2		// (> FF_C) Calling a fast function?
    |  move TMP2, BASE
    |  move TMP3, NARGS8:RC
    |  beqz NARGS8:RC, >3
    |2:
    |   ldc1 f0, 0(RA)
    |    addli RA, RA, 8
    |  addli TMP3, TMP3, -8
    |   sdc1 f0, 0(TMP2)
    |   addli TMP2, TMP2, 8
    |  bnez TMP3, <2
    |3:
    |  or TMP0, TMP0, AT
    |  beqz TMP0, >5
    |4:
    |  ins_callt
    |
    |5:  // Tailcall to a fast function with a Lua frame below.
    |  ld INS, -4(TMP1)
    |  decode_RA8a RA, INS
    |  decode_RA8b RA
    |  sub TMP1, BASE, RA
    |  ld LFUNC:TMP1, -8+FRAME_FUNC(TMP1)
    |  ld TMP1, LFUNC:TMP1->pc
    |  addi AT, TMP1, PC2PROTO(k)
    |  ld KBASE, AT
    |  b <4
    |
    |7:  // Tailcall from a vararg function.
    |  andi AT, TMP2, FRAME_TYPEP
    |  sub TMP2, BASE, TMP2		// Relocate BASE down.
    |  bnez AT, <1			// Vararg frame below?
    |  move BASE, TMP2
    |  ld TMP1, FRAME_PC(TMP2)
    |  andi TMP0, TMP1, FRAME_TYPE
    |  b <1
    break;

  case BC_ITERC:
    |  // RA = base*8, (RB = (nresults+1)*8, RC = (nargs+1)*8 ((2+1)*8))
    |  move TMP2, BASE
    |  add BASE, BASE, RA
    |   li AT, LJ_TFUNC
    |  ld TMP1, -24+HI(BASE)
    |   ld LFUNC:RB, -24+LO(BASE)
    |    ldc1 f2, -8(BASE)
    |    ldc1 f0, -16(BASE)
    |  st TMP1, HI(BASE)		// Copy callable.
    |   st LFUNC:RB, LO(BASE)
    |    sdc1 f2, 16(BASE)		// Copy control var.
    |    sdc1 f0, 8(BASE)		// Copy state.
    |   addli BASE, BASE, 8
    |  addi NARGS8:RC, zero, 16		// Iterators get 2 arguments.
    |  cmpne TMP1, AT, ->vmeta_call
    |  ins_call
    break;

  case BC_ITERN:
    |  // RA = base*8, (RB = (nresults+1)*8, RC = (nargs+1)*8 (2+1)*8)
    |.if JIT
    |  // NYI: add hotloop, record BC_ITERN.
    |.endif
    |  add RA, BASE, RA
    |  ld TAB:RB, -16+LO(RA)
    |  ld RC, -8+LO(RA)			// Get index from control var.
    |  ld TMP0, TAB:RB->asize
    |  ld TMP1, TAB:RB->array
    |   addli PC, PC, 4
    |1:  // Traverse array part.
    |  cmpltu AT, RC, TMP0
    |  shli TMP3, RC, 3
    |  beqz AT, >5			// Index points after array part?
    |  add TMP3, TMP1, TMP3
    |  ld TMP2, HI(TMP3)
    |   ldc1 f0, 0(TMP3)
    |    mtc1 RC, f2
    |     ld2u RD, -4+OFS_RD(PC)
    |  addli RC, RC, 1
    |  beq TMP2, TISNIL, <1		// Skip holes in array part.
    |    cvt.d.w f2, f2
    |     //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |   sdc1 f0, 8(RA)
    |     decode_RD4b RD
    |     add RD, RD, TMP3
    |   st RC, -8+LO(RA)		// Update control var.
    |     add PC, PC, RD
    |    sdc1 f2, 0(RA)
    |3:
    |  ins_next
    |
    |5:  // Traverse hash part.
    |  ld TMP1, TAB:RB->hmask
    |  sub RC, RC, TMP0
    |   ld TMP2, TAB:RB->node
    |6:
    |  cmpltu AT, TMP1, RC		// End of iteration? Branch to ITERL+1.
    |  shli TMP3, RC, 5
    |  bnez AT, <3
    |   shli RB, RC, 3
    |   sub TMP3, TMP3, RB
    |  add NODE:TMP3, TMP3, TMP2
    |  ld RB, HI(NODE:TMP3)
    |  ldc1 f0, 0(NODE:TMP3)
    |     ld2u RD, -4+OFS_RD(PC)
    |  addli RC, RC, 1
    |  beq RB, TISNIL, <6		// Skip holes in hash part.
    |   ldc1 f2, NODE:TMP3->key
    |     //lui TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |  sdc1 f0, 8(RA)
    |    add RC, RC, TMP0
    |     decode_RD4b RD
    |     add RD, RD, TMP3
    |   sdc1 f2, 0(RA)
    |     add PC, PC, RD
    |  addi AT, RA, -8+LO
    |  st AT, RC
    |  b <3
    break;

  case BC_ISNEXT:
    |  // RA = base*8, RD = target (points to ITERN)
    |  add RA, BASE, RA
    |  ld TMP0, -24+HI(RA)
    |  ld CFUNC:TMP1, -24+LO(RA)
    |   ld TMP2, -16+HI(RA)
    |    ld TMP3, -8+HI(RA)
    |  li AT, LJ_TFUNC
    |  addli TMP2, TMP2, -LJ_TTAB
    |  cmpne TMP0, AT, >5
    |  lbu TMP1, CFUNC:TMP1->ffid
    |  addli TMP3, TMP3, -LJ_TNIL
    |   srl TMP0, RD, 1
    |  or TMP2, TMP2, TMP3
    |  addli TMP1, TMP1, -FF_next_N
    |   add TMP0, PC, TMP0
    |  or TMP1, TMP1, TMP2
    |  //lui TMP2, (-(BCBIAS_J*4 >> 16) & 65535)
    |  bnez TMP1, >5
    |  add PC, TMP0, TMP2
    |  //lui TMP1, 0xfffe
    |  ori TMP1, TMP1, 0x7fff
    |  st r0, -8+LO(RA)			// Initialize control var.
    |  st TMP1, -8+HI(RA)
    |1:
    |  ins_next
    |5:  // Despecialize bytecode if any of the checks fail.
    |  addi TMP3, zero, BC_JMP
    |   addi TMP1, zero, BC_ITERC
    |  addi AT, PC, -4 + OFS_OP
    |  st1 AT, TMP3
    |   add PC, TMP0, TMP2
    |  addi AT, PC, OFS_OP
    |  st1 AT, TMP1
    |  b <1
    break;

  case BC_VARG:
    |  // RA = base*8, RB = (nresults+1)*8, RC = numparams*8
    |  ld TMP0, FRAME_PC(BASE)
    |  decode_RDtoRC8 RC, RD
    |   decode_RB8a RB, INS
    |  add RC, BASE, RC
    |   decode_RB8b RB
    |   add RA, BASE, RA
    |  addli RC, RC, FRAME_VARG
    |   add TMP2, RA, RB
    |  addli TMP3, BASE, -8		// TMP3 = vtop
    |  sub RC, RC, TMP0		// RC = vbase
    |  // Note: RC may now be even _above_ BASE if nargs was < numparams.
    |  sub TMP1, TMP3, RC
    |  beqz RB, >5			// Copy all varargs?
    |  addli TMP2, TMP2, -16
    |1:  // Copy vararg slots to destination slots.
    |  ld CARG1, HI(RC)
    |  cmpltu AT, RC, TMP3
    |   ld CARG2, LO(RC)
    |    addli RC, RC, 8
    |  movz CARG1, TISNIL, AT
    |  st CARG1, HI(RA)
    |   st CARG2, LO(RA)
    |  cmpltu AT, RA, TMP2
    |   addli RA, RA, 8
    |  bnez AT, <1
    |3:
    |  ins_next
    |
    |5:  // Copy all varargs.
    |  ld TMP0, L->maxstack
    |  addi MULTRES, zero, 8			// MULTRES = (0+1)*8
    |  blez TMP1, <3			// No vararg slots?
    |  add TMP2, RA, TMP1
    |  cmpltu AT, TMP0, TMP2
    |  addli MULTRES, TMP1, 8
    |  bnez AT, >7
    |6:
    |  ldc1 f0, 0(RC)
    |   addli RC, RC, 8
    |  sdc1 f0, 0(RA)
    |  cmpltu AT, RC, TMP3
    |  addli RA, RA, 8
    |  bnez AT, <6			// More vararg slots?
    |  b <3
    |
    |7:  // Grow stack for varargs.
    |  load_got lj_state_growstack
    |   st RA, L->top
    |  sub RA, RA, BASE
    |   st BASE, L->base
    |  sub BASE, RC, BASE		// Need delta, because BASE may change.
    |   st PC, SAVE_PC
    |  srl CARG2, TMP1, 3
    |  move CARG1, L
    |  call_intern lj_state_growstack	// (lua_State *L, int n)
    |  move RC, BASE
    |  ld BASE, L->base
    |  add RA, BASE, RA
    |  add RC, BASE, RC
    |  addli TMP3, BASE, -8
    |  b <6
    break;

  /* -- Returns ----------------------------------------------------------- */

  case BC_RETM:
    |  // RA = results*8, RD = extra_nresults*8
    |  add RD, RD, MULTRES		// MULTRES >= 8, so RD >= 8.
    |  // Fall through. Assumes BC_RET follows.
    break;

  case BC_RET:
    |  // RA = results*8, RD = (nresults+1)*8
    |  ld PC, FRAME_PC(BASE)
    |   add RA, BASE, RA
    |    move MULTRES, RD
    |1:
    |  andi TMP0, PC, FRAME_TYPE
    |  bnez TMP0, ->BC_RETV_Z
    |.  xori TMP1, PC, FRAME_VARG
    |
    |->BC_RET_Z:
    |  // BASE = base, RA = resultptr, RD = (nresults+1)*8, PC = return
    |   ld INS, -4(PC)
    |    addli TMP2, BASE, -8
    |    addli RC, RD, -8
    |  decode_RA8a TMP0, INS
    |   decode_RB8a RB, INS
    |  decode_RA8b TMP0
    |   decode_RB8b RB
    |   add TMP3, TMP2, RB
    |  sub BASE, TMP2, TMP0
    |  beqz RC, >3
    |2:
    |   ldc1 f0, 0(RA)
    |    addli RA, RA, 8
    |  addli RC, RC, -8
    |   sdc1 f0, 0(TMP2)
    |  addli TMP2, TMP2, 8
    |  bnez RC, <2
    |3:
    |  addli TMP3, TMP3, -8
    |5:
    |  cmpltu AT, TMP2, TMP3
    |  addi AT, BASE, FRAME_FUNC
    |  ld LFUNC:TMP1, AT
    |  bnez AT, >6
    |  ins_next1
    |  ld TMP1, LFUNC:TMP1->pc
    |  ld KBASE, PC2PROTO(k)(TMP1)
    |  ins_next2
    |
    |6:  // Fill up results with nil.
    |  st TISNIL, HI(TMP2)
    |  addli TMP2, TMP2, 8
    |  b <5
    |
    |->BC_RETV_Z:  // Non-standard return case.
    |  andi TMP2, TMP1, FRAME_TYPEP
    |  bnez TMP2, ->vm_return
    |  // Return from vararg function: relocate BASE down.
    |  sub BASE, BASE, TMP1
    |  addi AT, BASE, FRAME_PC
    |  ld PC, AT
    |  b <1
    break;

  case BC_RET0: case BC_RET1:
    |  // RA = results*8, RD = (nresults+1)*8
    |  ld PC, FRAME_PC(BASE)
    |   add RA, BASE, RA
    |    move MULTRES, RD
    |  andi TMP0, PC, FRAME_TYPE
    |  xori TMP1, PC, FRAME_VARG
    |  bnez TMP0, ->BC_RETV_Z
    |
    |  ld INS, -4(PC)
    |   addli TMP2, BASE, -8
    if (op == BC_RET1) {
      |  ldc1 f0, 0(RA)
    }
    |  decode_RB8a RB, INS
    |   decode_RA8a RA, INS
    |  decode_RB8b RB
    |   decode_RA8b RA
    if (op == BC_RET1) {
      |  sdc1 f0, 0(TMP2)
    }
    |   sub BASE, TMP2, RA
    |5:
    |  cmpltu AT, RD, RB
    |  addi AT, BASE, FRAME_FUNC
    |  ld LFUNC:TMP1, AT
    |  bnez AT, >6
    |  ins_next1
    |  ld TMP1, LFUNC:TMP1->pc
    |  ld KBASE, PC2PROTO(k)(TMP1)
    |  ins_next2
    |
    |6:  // Fill up results with nil.
    |  addli TMP2, TMP2, 8
    |  addli RD, RD, 8
    if (op == BC_RET1) {
      |  addi AT, TMP2, HI
      |  st AT, TISNIL
    } else {
      |  addi AT, TMP2, -8+HI
      |  st AT, TISNIL
    }
    |  b <5
    break;

  /* -- Loops and branches ------------------------------------------------ */

  case BC_FORL:
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_IFORL follows.
    break;

  case BC_JFORI:
  case BC_JFORL:
#if !LJ_HASJIT
    break;
#endif
  case BC_FORI:
  case BC_IFORL:
    |  // RA = base*8, RD = target (after end of loop or start of loop)
    vk = (op == BC_IFORL || op == BC_JFORL);
    |  add RA, BASE, RA
    if (vk) {
      |  ldc1 f0, FORL_IDX*8(RA)
      |  ldc1 f4, FORL_STEP*8(RA)
      |  ldc1 f2, FORL_STOP*8(RA)
      |   ld TMP3, FORL_STEP*8+HI(RA)
      |  add.d f0, f0, f4
      |  sdc1 f0, FORL_IDX*8(RA)
    } else {
      |  ld TMP1, FORL_IDX*8+HI(RA)
      |  ld TMP3, FORL_STEP*8+HI(RA)
      |  ld TMP2, FORL_STOP*8+HI(RA)
      |  cmpltui TMP1, TMP1, LJ_TISNUM
      |  cmpltui TMP0, TMP3, LJ_TISNUM
      |  cmpltui TMP2, TMP2, LJ_TISNUM
      |  and TMP1, TMP1, TMP0
      |  and TMP1, TMP1, TMP2
      |   ldc1 f0, FORL_IDX*8(RA)
      |  ldc1 f2, FORL_STOP*8(RA)
      |  beqz TMP1, ->vmeta_for
    }
    if (op != BC_JFORL) {
      |  srl RD, RD, 1
      |  //lui TMP0, (-(BCBIAS_J*4 >> 16) & 65535)
    }
    |  c.le.d 0, f0, f2
    |  c.le.d 1, f2, f0
    |  sdc1 f0, FORL_EXT*8(RA)
    if (op == BC_JFORI) {
      |  li TMP1, 1
      |  li TMP2, 1
      |   add TMP0, RD, TMP0
      |  slt TMP3, TMP3, r0
      |  movf TMP1, r0, 0
      |   add PC, PC, TMP0
      |  movf TMP2, r0, 1
      |   addi RD, PC, -4+OFS_RD
      |   ld2u RD, RD
      |  movn TMP1, TMP2, TMP3
      |  bnez TMP1, =>BC_JLOOP
      |.  decode_RD8b RD
    } else if (op == BC_JFORL) {
      |  li TMP1, 1
      |  li TMP2, 1
      |  slt TMP3, TMP3, r0
      |  movf TMP1, r0, 0
      |  movf TMP2, r0, 1
      |  movn TMP1, TMP2, TMP3
      |  bnez TMP1, =>BC_JLOOP
    } else {
      |  add TMP1, RD, TMP0
      |  slt TMP3, TMP3, r0
      |  move TMP2, TMP1
      if (op == BC_FORI) {
	|  movt TMP1, r0, 0
	|  movt TMP2, r0, 1
      } else {
	|  movf TMP1, r0, 0
	|  movf TMP2, r0, 1
      }
      |  movn TMP1, TMP2, TMP3
      |  add PC, PC, TMP1
    }
    |  ins_next
    break;

  case BC_ITERL:
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_IITERL follows.
    break;

  case BC_JITERL:
#if !LJ_HASJIT
    break;
#endif
  case BC_IITERL:
    |  // RA = base*8, RD = target
    |  add RA, BASE, RA
    |  ld TMP1, HI(RA)
    |  addi AT, RA, LO
    |  ld TMP2, AT
    |  beq TMP1, TISNIL, >1		// Stop if iterator returned nil.
    if (op == BC_JITERL) {
      |  st TMP1, -8+HI(RA)
      |  addi AT, RA, -8+LO
      |  st AT, TMP2
      |  b =>BC_JLOOP
    } else {
      |  branch_RD			// Otherwise save control var + branch.
      |  st TMP1, -8+HI(RA)
      |   st TMP2, -8+LO(RA)
    }
    |1:
    |  ins_next
    break;

  case BC_LOOP:
    |  // RA = base*8, RD = target (loop extent)
    |  // Note: RA/RD is only used by trace recorder to determine scope/extent
    |  // This opcode does NOT jump, it's only purpose is to detect a hot loop.
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_ILOOP follows.
    break;

  case BC_ILOOP:
    |  // RA = base*8, RD = target (loop extent)
    |  ins_next
    break;

  case BC_JLOOP:
    |.if JIT
    |  // RA = base*8 (ignored), RD = traceno*8
    |  ld TMP1, DISPATCH_J(trace)(DISPATCH)
    |  srl RD, RD, 1
    |   li AT, 0
    |  add TMP1, TMP1, RD
    |  // Traces on MIPS don't store the trace number, so use 0.
    |   st AT, DISPATCH_GL(vmstate)(DISPATCH)
    |  ld TRACE:TMP2, 0(TMP1)
    |   st BASE, DISPATCH_GL(jit_base)(DISPATCH)
    |   st L, DISPATCH_GL(jit_L)(DISPATCH)
    |  ld TMP2, TRACE:TMP2->mcode
    |  addli JGL, DISPATCH, GG_DISP2G+32768
    |  jr TMP2
    |.endif
    break;

  case BC_JMP:
    |  // RA = base*8 (only used by trace recorder), RD = target
    |  branch_RD
    |  ins_next
    break;

  /* -- Function headers -------------------------------------------------- */

  case BC_FUNCF:
    |.if JIT
    |  hotcall
    |.endif
  case BC_FUNCV:  /* NYI: compiled vararg functions. */
    |  // Fall through. Assumes BC_IFUNCF/BC_IFUNCV follow.
    break;

  case BC_JFUNCF:
#if !LJ_HASJIT
    break;
#endif
  case BC_IFUNCF:
    |  // BASE = new base, RA = BASE+framesize*8, RB = LFUNC, RC = nargs*8
    |  ld TMP2, L->maxstack
    |   lbu TMP1, -4+PC2PROTO(numparams)(PC)
    |    ld KBASE, -4+PC2PROTO(k)(PC)
    |  cmpltu AT, TMP2, RA
    |  shli TMP1, TMP1, 3
    |  bnez AT, ->vm_growstack_l
    if (op != BC_JFUNCF) {
      |  ins_next1
    }
    |2:
    |  cmpltu AT, NARGS8:RC, TMP1		// Check for missing parameters.
    |  add AT, BASE, NARGS8:RC
    |  bnez AT, >3
    if (op == BC_JFUNCF) {
      |  decode_RD8a RD, INS
      |  decode_RD8b RD
      |  b =>BC_JLOOP
    } else {
      |  ins_next2
    }
    |
    |3:  // Clear missing parameters.
    |  addi AT1, AT, HI
    |  st AT1, TISNIL
    |  addli NARGS8:RC, NARGS8:RC, 8
    |  b <2
    break;

  case BC_JFUNCV:
#if !LJ_HASJIT
    break;
#endif
    |  NYI  // NYI: compiled vararg functions
    break;  /* NYI: compiled vararg functions. */

  case BC_IFUNCV:
    |  // BASE = new base, RA = BASE+framesize*8, RB = LFUNC, RC = nargs*8
    |   add TMP1, BASE, RC
    |  ld TMP2, L->maxstack
    |  add TMP0, RA, RC
    |   st LFUNC:RB, LO(TMP1)		// Store copy of LFUNC.
    |   addli TMP3, RC, 8+FRAME_VARG
    |  cmpltu AT, TMP0, TMP2
    |    ld KBASE, -4+PC2PROTO(k)(PC)
    |  addi AT, TMP1, HI
    |  st AT, TMP3
    |  beqz AT, ->vm_growstack_l
    |  lbu TMP2, -4+PC2PROTO(numparams)(PC)
    |   move RA, BASE
    |   move RC, TMP1
    |  ins_next1
    |  addli BASE, TMP1, 8
    |  beqz TMP2, >3
    |1:
    |  ld TMP0, HI(RA)
    |   ld TMP3, LO(RA)
    |  cmpltu AT, RA, RC			// Less args than parameters?
    |  move CARG1, TMP0
    |  movz TMP0, TISNIL, AT		// Clear missing parameters.
    |  movn CARG1, TISNIL, AT		// Clear old fixarg slot (help the GC).
    |   st TMP3, 8+LO(TMP1)
    |    addli TMP2, TMP2, -1
    |  st TMP0, 8+HI(TMP1)
    |    addli TMP1, TMP1, 8
    |  st CARG1, HI(RA)
    |   addli RA, RA, 8
    |  bnez TMP2, <1
    |3:
    |  ins_next2
    break;

  case BC_FUNCC:
  case BC_FUNCCW:
    |  // BASE = new base, RA = BASE+framesize*8, RB = CFUNC, RC = nargs*8
    if (op == BC_FUNCC) {
      |  ld CFUNCADDR, CFUNC:RB->f
    } else {
      |  ld CFUNCADDR, DISPATCH_GL(wrapf)(DISPATCH)
    }
    |  add TMP1, RA, NARGS8:RC
    |  ld TMP2, L->maxstack
    |   add RC, BASE, NARGS8:RC
    |  st BASE, L->base
    |  cmpltu AT, TMP2, TMP1
    |   st RC, L->top
    |    li_vmstate C
    if (op == BC_FUNCCW) {
      |  ld CARG2, CFUNC:RB->f
    }
    |  move CARG1, L
    |  bnez AT, ->vm_growstack_c	// Need to grow stack.
    |   st_vmstate
    |  jalr CFUNCADDR			// (lua_State *L [, lua_CFunction f])
    |  // Returns nresults.
    |  ld BASE, L->base
    |   shli RD, CRET1, 3
    |  ld TMP1, L->top
    |    li_vmstate INTERP
    |  ld PC, FRAME_PC(BASE)		// Fetch PC of caller.
    |   sub RA, TMP1, RD		// RA = L->top - nresults*8
    |   st_vmstate
    |  b ->vm_returnc
    break;

  /* ---------------------------------------------------------------------- */

  default:
    fprintf(stderr, "Error: undefined opcode BC_%s\n", bc_names[op]);
    exit(2);
    break;
  }
}

static int build_backend(BuildCtx *ctx)
{
  int op;

  dasm_growpc(Dst, BC__MAX);

  build_subroutines(ctx);

  |.code_op
  for (op = 0; op < BC__MAX; op++)
    build_ins(ctx, (BCOp)op, op);

  return BC__MAX;
}

/* Emit pseudo frame-info for all assembler functions. */
static void emit_asm_debug(BuildCtx *ctx)
{
  int fcofs = (int)((uint8_t *)ctx->glob[GLOB_vm_ffi_call] - ctx->code);
  int i;
  switch (ctx->mode) {
  case BUILD_elfasm:
    fprintf(ctx->fp, "\t.section .debug_frame,\"\",@progbits\n");
    fprintf(ctx->fp,
	".Lframe0:\n"
	"\t.4byte .LECIE0-.LSCIE0\n"
	".LSCIE0:\n"
	"\t.4byte 0xffffffff\n"
	"\t.byte 0x1\n"
	"\t.string \"\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE0:\n\n");
    fprintf(ctx->fp,
	".LSFDE0:\n"
	"\t.4byte .LEFDE0-.LASFDE0\n"
	".LASFDE0:\n"
	"\t.4byte .Lframe0\n"
	"\t.4byte .Lbegin\n"
	"\t.4byte %d\n"
	"\t.byte 0xe\n\t.uleb128 %d\n"
	"\t.byte 0x9f\n\t.sleb128 1\n"
	"\t.byte 0x9e\n\t.sleb128 2\n",
	fcofs, CFRAME_SIZE);
    for (i = 23; i >= 16; i--)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+i, 26-i);
    for (i = 30; i >= 20; i -= 2)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+32+i, 42-i);
    fprintf(ctx->fp,
	"\t.align 2\n"
	".LEFDE0:\n\n");
#if LJ_HASFFI
    fprintf(ctx->fp,
	".LSFDE1:\n"
	"\t.4byte .LEFDE1-.LASFDE1\n"
	".LASFDE1:\n"
	"\t.4byte .Lframe0\n"
	"\t.4byte lj_vm_ffi_call\n"
	"\t.4byte %d\n"
	"\t.byte 0x9f\n\t.uleb128 1\n"
	"\t.byte 0x90\n\t.uleb128 2\n"
	"\t.byte 0xd\n\t.uleb128 0x10\n"
	"\t.align 2\n"
	".LEFDE1:\n\n", (int)ctx->codesz - fcofs);
#endif
    fprintf(ctx->fp, "\t.section .eh_frame,\"aw\",@progbits\n");
    fprintf(ctx->fp,
	"\t.globl lj_err_unwind_dwarf\n"
	".Lframe1:\n"
	"\t.4byte .LECIE1-.LSCIE1\n"
	".LSCIE1:\n"
	"\t.4byte 0\n"
	"\t.byte 0x1\n"
	"\t.string \"zPR\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.uleb128 6\n"			/* augmentation length */
	"\t.byte 0\n"
	"\t.4byte lj_err_unwind_dwarf\n"
	"\t.byte 0\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE1:\n\n");
    fprintf(ctx->fp,
	".LSFDE2:\n"
	"\t.4byte .LEFDE2-.LASFDE2\n"
	".LASFDE2:\n"
	"\t.4byte .LASFDE2-.Lframe1\n"
	"\t.4byte .Lbegin\n"
	"\t.4byte %d\n"
	"\t.uleb128 0\n"			/* augmentation length */
	"\t.byte 0xe\n\t.uleb128 %d\n"
	"\t.byte 0x9f\n\t.sleb128 1\n"
	"\t.byte 0x9e\n\t.sleb128 2\n",
	fcofs, CFRAME_SIZE);
    for (i = 23; i >= 16; i--)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+i, 26-i);
    for (i = 30; i >= 20; i -= 2)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+32+i, 42-i);
    fprintf(ctx->fp,
	"\t.align 2\n"
	".LEFDE2:\n\n");
#if LJ_HASFFI
    fprintf(ctx->fp,
	".Lframe2:\n"
	"\t.4byte .LECIE2-.LSCIE2\n"
	".LSCIE2:\n"
	"\t.4byte 0\n"
	"\t.byte 0x1\n"
	"\t.string \"zR\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.uleb128 1\n"			/* augmentation length */
	"\t.byte 0\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE2:\n\n");
    fprintf(ctx->fp,
	".LSFDE3:\n"
	"\t.4byte .LEFDE3-.LASFDE3\n"
	".LASFDE3:\n"
	"\t.4byte .LASFDE3-.Lframe2\n"
	"\t.4byte lj_vm_ffi_call\n"
	"\t.4byte %d\n"
	"\t.uleb128 0\n"			/* augmentation length */
	"\t.byte 0x9f\n\t.uleb128 1\n"
	"\t.byte 0x90\n\t.uleb128 2\n"
	"\t.byte 0xd\n\t.uleb128 0x10\n"
	"\t.align 2\n"
	".LEFDE3:\n\n", (int)ctx->codesz - fcofs);
#endif
    break;
  default:
    break;
  }
}

