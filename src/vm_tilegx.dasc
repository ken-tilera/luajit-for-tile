  |   st BASE, L->top			// Dummy frame length is ok.
  |  addli CARG3, BASE, 8
  |   st sp, PC
  |  move CARG1, L
  |  call_intern lj_tab_next		// (lua_State *L, GCtab *t, TValue *key)
