  |.ffunc_n math_abs
  |//.  abs.d FRET1, FARG1
  |->fff_resn:
  |  addi PC, BASE, FRAME_PC
  |  ld4s PC, PC
  |  addli RA, BASE, -8
  |  addi AT, BASE, -8
  |  st AT, AT2
  |  b ->fff_res1
  |
  |->fff_restv:
  |  // CARG3/CARG1 = TValue result.
  |  addi PC, BASE, FRAME_PC
  |  ld4s PC, PC
  |   addi AT, BASE, -8+HI
  |   st4 AT, CARG3
  |  addli RA, BASE, -8
  |   addi AT, BASE, -8+LO
  |   st4 AT, CARG1
  |->fff_res1:
  |  // RA = results, PC = return.
  |  addi RD, zero, (1+1)*8
  |->fff_res:
  |  // RA = results, RD = (nresults+1)*8, PC = return.
  |  andi TMP0, PC, FRAME_TYPE
  |  move MULTRES, RD
  |  bnez TMP0, ->vm_return
  |  addi INS, PC, -4
  |  ld4u INS, INS
  |  decode_RB8a RB, INS
  |  decode_RB8b RB
