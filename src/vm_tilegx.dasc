  |//-----------------------------------------------------------------------
  |
  |->fff_fallback:			// Call fast function fallback handler.
  |  // BASE = new base, RB = CFUNC, RC = nargs*8
  |  ld TMP3, CFUNC:RB->f
  |    add TMP1, BASE, NARGS8:RC
  |   ld PC, FRAME_PC(BASE)		// Fallback may overwrite PC.
  |    addli TMP0, TMP1, 8*LUA_MINSTACK
  |     addlo TMP2, L:LREG->maxstack
  |     ld TMP2, TMP2
  |   st sp, PC
  |  cmpltu AT, TMP2, TMP0
  |     st BASE, L->base
  |    st TMP1, L->top
  |  move CFUNCADDR, TMP3
  |  bnez AT, >5			// Need to grow stack.
  |  move CARG1, L
  |  jalr TMP3				// (lua_State *L)
  |  // Either throws an error, or recovers and returns -1, 0 or nresults+1.
  |  ld BASE, L->base
  |   shli RD, CRET1, 3
  |  addli RA, BASE, -8
  |  bgtz CRET1, ->fff_res		// Returned nresults+1?
  |1:  // Returned 0 or -1: retry fast path.
  |  ld TMP0, L->top
  |   ld LFUNC:RB, FRAME_FUNC(BASE)
  |  sub NARGS8:RC, TMP0, BASE
  |  bnez CRET1, ->vm_call_tail		// Returned -1?
  |  ins_callt				// Returned 0: retry fast path.
  |
  |// Reconstruct previous base for vmeta_call during tailcall.
  |->vm_call_tail:
  |  andi TMP0, PC, FRAME_TYPE
  |   li AT, -4
  |  and TMP1, PC, AT
  |  bnez TMP0, >3
  |  lbu TMP1, OFS_RA(PC)
  |  shli TMP1, TMP1, 3
  |  addli TMP1, TMP1, 8
  |3:
  |  sub TMP2, BASE, TMP1
  |  b ->vm_call_dispatch		// Resolve again for tailcall.
  |
  |5:  // Grow stack for fallback handler.
  |  load_got lj_state_growstack
  |  li CARG2, LUA_MINSTACK
  |  move CARG1, L
  |  call_intern lj_state_growstack	// (lua_State *L, int n)
  |  ld BASE, L->base
  |  move CRET1, zero			// Force retry.
  |  b <1
  |
  |->fff_gcstep:			// Call GC step function.
  |  // BASE = new base, RC = nargs*8
  |  move MULTRES, ra
  |  load_got lj_gc_step
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |  add TMP0, BASE, NARGS8:RC
  |   st sp, PC
  |   addlo AT, L:LREG->top
  |  st AT, TMP0
  |  move CARG1, L
  |  call_intern lj_gc_step		// (lua_State *L)
  |   addlo BASE, L:LREG->base
  |   ld BASE, BASE
  |  move ra, MULTRES
  |    ld TMP0, L->top
  |  ld CFUNC:RB, FRAME_FUNC(BASE)
  |  sub NARGS8:RC, TMP0, BASE
  |  jr ra
