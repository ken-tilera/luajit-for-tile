  |  // Decrement the hookcount for consistency, but always do the call.
  |  addi AT, DISPATCH, DISPATCH_GL(hookcount)
  |  ld TMP2, AT
  |  bnez AT, >5
  |  andi AT, TMP3, HOOK_ACTIVE
