  |  addi MULTRES, RB, -24+LO
  |  ld4s MULTRES, MULTRES
  |  b <4
  |
  |->vm_hotloop:			// Hot loop counter underflow.
  |.if JIT
  |  addi LFUNC:TMP1, BASE, FRAME_FUNC
  |  ld4s LFUNC:TMP1, LFUNC:TMP1
  |   addli CARG1, DISPATCH, GG_DISP2J
  |   addi AT, sp, SAVE_PC
  |   st AT, PC
  |  addlo TMP1, LFUNC:TMP1->pc
  |  ld4u TMP1, TMP1
  |   move CARG2, PC
  |   addli, AT, DISPATCH, DISPATCH_J(L)
  |   st AT, L
  |  addi TMP1, TMP1, PC2PROTO(framesize)
  |  ld1u TMP1, TMP1
  |  load_got lj_trace_hot
  |   addlo AT, L->base
  |   st AT, BASE
  |  shli TMP1, TMP1, 3
  |  add TMP1, BASE, TMP1
  |  addlo AT, L:LREG->top
  |  st AT, TMP1
  |  call_intern lj_trace_hot		// (jit_State *J, const BCIns *pc)
  |  b <3
  |.endif
  |
  |->vm_callhook:			// Dispatch target for call hooks.
  |.if JIT
  |  b >1
  |.endif
  |.  move CARG2, PC
  |
  |->vm_hotcall:			// Hot call counter underflow.
  |.if JIT
  |  ori CARG2, PC, 1
  |1:
  |.endif
  |  load_got lj_dispatch_call
  |  add TMP0, BASE, RC
  |   st sp, PC
  |   addlo AT, L:LREG->base
  |   st AT, BASE
  |  sub RA, RA, BASE
  |   addlo AT, L:LREG->top
  |   st AT, TMP0
  |  move CARG1, L
  |  call_intern lj_dispatch_call	// (lua_State *L, const BCIns *pc)
  |  // Returns ASMFunction.
  |  addlo BASE, L:LREG->base
  |  ld BASE, BASE
  |   addlo TMP0, L:LREG->top
  |   ld TMP0, TMP0
  |   addi AT, sp, SAVE_PC
  |   st AT, zero
  |  sub NARGS8:RC, TMP0, BASE
  |  add RA, BASE, RA
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |  addi AT, PC, -4
  |  ld4u INS, AT
  |  jr CRET1
  |
  |//-----------------------------------------------------------------------
  |//-- Trace exit handler -------------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |.macro savex_, a, b
  |  //sdc1 f..a, 16+a*8(sp)
  |  addli AT, sp, 16+32*8+a*4
  |  st AT, r..a
  |  addli AT, sp, 16+32*8+b*4
  |  st AT, r..b
  |.endmacro
  |
  |->vm_exit_handler:
  |.if JIT
  |  addli sp, sp, -(16+32*8+32*4)
  |  savex_ 0, 1
  |  savex_ 2, 3
  |  savex_ 4, 5
  |  savex_ 6, 7
  |  savex_ 8, 9
  |  savex_ 10, 11
  |  savex_ 12, 13
  |  savex_ 14, 15
  |  savex_ 16, 17
  |  savex_ 18, 19
  |  savex_ 20, 21
  |  savex_ 22, 23
  |  savex_ 24, 25
  |  savex_ 26, 27
  |  //sdc1 f28, 16+28*8(sp)
  |  addli AT, sp, 16+32*8+28*4
  |  st AT, r28
  |  //sdc1 f30, 16+30*8(sp)
  |  addli AT, sp, 16+32*8+30*4
  |  st AT, r30
  |  addli AT, sp, 16+32*8+31*4
  |  st AT, r0
  |  li_vmstate EXIT
  |   addli TMP2, sp, 16+32*8+32*4	// Recompute original value of sp.
  |  addli DISPATCH, JGL, -GG_DISP2G-32768
  |  ld TMP1, TMP2			// Load exit number.
  |  st_vmstate
  |   addli AT, sp, 16+32*8+29*4
  |   st AT, TMP2
  |  addi L, DISPATCH, DISPATCH_GL(jit_L)
  |  ld L, L
  |  addi BASE, DISPATCH, DISPATCH_GL(jit_base)
  |  ld BASE, BASE
  |  load_got lj_trace_exit
  |  addli AT, DISPATCH, DISPATCH_J(L)
  |  st AT, L
  |  addli AT, DISPATCH, DISPATCH_J(parent)
  |  st AT, lr
  |  addli AT, DISPATCH, DISPATCH_J(exitno)
  |  st4 AT, TMP1
  |  addli CARG1, DISPATCH, GG_DISP2J
  |  addlo AT, L->base
  |  st AT, BASE
  |  addli CARG2, sp, 16
  |  call_intern lj_trace_exit		// (jit_State *J, ExitState *ex)
  |  // Returns MULTRES (unscaled) or negated error code.
  |  addlo TMP1, L->cframe
  |  ld TMP1, TMP1
  |  addi AT, zero, -4
  |   addlo BASE, L->base
  |   ld BASE, BASE
  |  and sp, TMP1, AT
  |   ld PC, sp
  |  addi AT, L, SAVE_L
  |  st AT, L
  |  b >1
  |.endif
  |->vm_exit_interp:
  |.if JIT
  |  // CRET1 = MULTRES or negated error code, BASE, PC and JGL set.
  |  addi L, sp, SAVE_L
  |  ld L, L
  |  addli DISPATCH, JGL, -GG_DISP2G-32768
  |1:
  |  addi LFUNC:TMP1, BASE, FRAME_FUNC
  |  ld4s LFUNC:TMP1, LFUNC:TMP1
  |  bltz CRET1, >3			// Check for error from exit.
  |    //lui TMP3, 0x59c0			// TOBIT = 2^52 + 2^51 (float).
  |  shli MULTRES, CRET1, 3
  |    addi TISNIL, zero, LJ_TNIL
  |  st MULTRES, SAVE_MULTRES
  |    //mtc1 TMP3, TOBIT
  |  addlo TMP1, LFUNC:TMP1->pc
  |  ld4u TMP1, TMP1
  |   addi AT, DISPATCH, DISPATCH_GL(jit_L)
  |   st4 AT, zero
  |  addi KBASE, TMP1, PC2PROTO(k)
  |  ld4u KBASE, KBASE
  |    //cvt.d.s TOBIT, TOBIT
  |  // Modified copy of ins_next which handles function header dispatch, too.
  |  ld INS, PC
  |   addli PC, PC, 4
  |    // Assumes TISNIL == ~LJ_VMST_INTERP == -1
  |    addi AT, DISPATCH, DISPATCH_GL(vmstate)
  |    st4 AT, TISNIL
  |  decode_OP4a TMP1, INS
  |  decode_OP4b TMP1
  |    cmpltui TMP2, TMP1, BC_FUNCF*4	// Function header?
  |  add TMP0, DISPATCH, TMP1
  |   decode_RD8a RD, INS
  |  ld AT, TMP0
  |   decode_RA8a RA, INS
  |  decode_RA8b RA
  |    beqz TMP2, >2
  |  decode_RD8b RD
  |  jr AT
  |2:
  |  addli RC, MULTRES, -8
  |  add RA, RA, BASE
  |  jr AT
  |
  |3:  // Rethrow error from the right C frame.
  |  load_got lj_err_throw
  |  //negu CARG2, CRET1
  |  move CARG1, L
  |  call_intern lj_err_throw		// (lua_State *L, int errcode)
  |.endif
  |
  |//-----------------------------------------------------------------------
  |//-- Math helper functions ----------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |// Modifies AT, TMP0, FRET1, FRET2, f4. Keeps all others incl. FARG1.
  |.macro vm_round, func
  |  //lui TMP0, 0x4330			// Hiword of 2^52 (double).
  |  //mtc1 r0, f4
  |  //mtc1 TMP0, f5
  |  //abs.d FRET2, FARG1			// |x|
  |   // mfc1 AT, f13
  |  //c.olt.d 0, FRET2, f4
  |   //add.d FRET1, FRET2, f4		// (|x| + 2^52) - 2^52
  |  //bc1f 0, >1				// Truncate only if |x| < 2^52.
  |//.  sub.d FRET1, FRET1, f4
  |    cmplts AT, AT, zero
  |//.if "func" == "ceil"
  |   //lui TMP0, 0xbff0			// Hiword of -1 (double). Preserves -0.
  |//.else
  |   //lui TMP0, 0x3ff0			// Hiword of +1 (double).
  |//.endif
  |.if "func" == "trunc"
  |//   mtc1 TMP0, f5
  |//  c.olt.d 0, FRET2, FRET1		// |x| < result?
  |//   sub.d FRET2, FRET1, f4
  |//  movt.d FRET1, FRET2, 0		// If yes, subtract +1.
  |//  neg.d FRET2, FRET1
  |  jr lr
  |//.  movn.d FRET1, FRET2, AT		// Merge sign bit back in.
  |.else
  |  //neg.d FRET2, FRET1
  |   //mtc1 TMP0, f5
  |  //movn.d FRET1, FRET2, AT		// Merge sign bit back in.
  |//.if "func" == "ceil"
  | // c.olt.d 0, FRET1, FARG1		// x > result?
  |//.else
  | // c.olt.d 0, FARG1, FRET1		// x < result?
  |//.endif
  | //  sub.d FRET2, FRET1, f4		// If yes, subtract +-1.
  |  jr lr
  |//.  movt.d FRET1, FRET2, 0
  |.endif
  |1:
  |  jr lr
  |//.  mov.d FRET1, FARG1
  |.endmacro
  |
  |->vm_floor:
  |  vm_round floor
  |->vm_ceil:
  |  vm_round ceil
  |->vm_trunc:
  |.if JIT
  |  vm_round trunc
  |.endif
  |
  |//-----------------------------------------------------------------------
  |//-- Miscellaneous functions --------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |//-----------------------------------------------------------------------
  |//-- FFI helper functions -----------------------------------------------
  |//-----------------------------------------------------------------------
  |
  |// Handler for callback functions. Callback slot number in r1, g in r2.
  |->vm_ffi_callback:
  |.if FFI
  |.type CTSTATE, CTState, PC
  |  saveregs
  |  addlo CTSTATE, GL:r2->ctype_state
  |  ld4u CTSTATE, CTSTATE
  |   addli DISPATCH, r2, GG_G2DISP
  |  load_got lj_ccallback_enter
  |  addlo AT, CTSTATE->cb.slot
  |  st AT, r1
  |  addlo AT, CTSTATE->cb.gpr[0]
  |  st AT, CARG1
  |  addlo AT, CTSTATE->cb.gpr[1]
  |  st AT, CARG2
  |   //sdc1 FARG1, CTSTATE->cb.fpr[0]
  |  addlo AT, CTSTATE->cb.gpr[2]
  |  st AT, CARG3
  |  addlo AT, CTSTATE->cb.gpr[3]
  |  st AT, CARG4
  |   //sdc1 FARG2, CTSTATE->cb.fpr[1]
  |  addli TMP0, sp, CFRAME_SPACE+16
  |  addlo AT, CTSTATE->cb.stack
  |  st AT, TMP0
  |  addi AT, sp, SAVE_PC
  |  st AT, zero
  |   move CARG2, sp
  |  move CARG1, CTSTATE
  |  call_intern lj_ccallback_enter	// (CTState *cts, void *cf)
  |  // Returns lua_State *.
  |  addlo BASE, L:CRET1->base
  |  ld BASE, BASE
  |  addlo RC, L:CRET1->top
  |  ld RC, RC
  |   move L, CRET1
  |     //lui TMP3, 0x59c0		// TOBIT = 2^52 + 2^51 (float).
  |  addi LFUNC:RB, BASE, FRAME_FUNC
  |  ld4s LFUNC:RB, LFUNC:RB
  |     //mtc1 TMP3, TOBIT
  |    li_vmstate INTERP
  |     addi TISNIL, zero, LJ_TNIL
  |  sub RC, RC, BASE
  |    st_vmstate
  |     //cvt.d.s TOBIT, TOBIT
  |  ins_callt
  |.endif
  |
  |->cont_ffi_callback:			// Return from FFI callback.
  |.if FFI
  |  load_got lj_ccallback_leave
  |  addli CTSTATE, DISPATCH, DISPATCH_GL(ctype_state)
  |  ld4u CTSTATE, CTSTATE
  |   addlo AT, L->base
  |   st AT, BASE
  |   addlo AT, L->top
  |   st AT, RB
  |  addlo AT, CTSTATE->L
  |  st AT, L
  |  move CARG2, RA
  |  move CARG1, CTSTATE
  |  call_intern lj_ccallback_leave	// (CTState *cts, TValue *o)
  |  addlo CRET1, CTSTATE->cb.gpr[0]
  |  ld CRET1, CRET1
  |   //ldc1 FRET1, CTSTATE->cb.fpr[0]
  |  addlo CRET2, CTSTATE->cb.gpr[1]
  |  ld CRET2, CRET2
  |  //ldc1 FRET2, CTSTATE->cb.fpr[1]
  |  b ->vm_leave_unw
  |.endif
  |
  |->vm_ffi_call:			// Call C function via FFI.
  |  // Caveat: needs special frame unwinding, see below.
  |.if FFI
  |  .type CCSTATE, CCallState, CARG1
  |  addlo TMP1, CCSTATE->spadj
  |  ld TMP1, TMP1
  |   addlo CARG2, CCSTATE->nsp
  |   ld1u CARG2, CARG2
  |  move TMP2, sp
  |  sub sp, sp, TMP1
  |  addi AT, TMP2, -4
  |  st4 AT, lr
  |   shli CARG2, CARG2, 2
  |  addi AT, TMP2, -8
  |  st4 AT, r16
  |  addi AT, TMP2, -12
  |  st4 AT, CCSTATE
  |  move r16, TMP2
  |  addli TMP1, CCSTATE, offsetof(CCallState, stack)
  |  addli TMP2, sp, 16
  |  add TMP3, TMP1, CARG2
  |  beqz CARG2, >2
  |1:
  |   ld TMP0, TMP1
  |  addli TMP1, TMP1, 4
  |  cmpltu AT, TMP1, TMP3
  |   st TMP2, TMP0
  |  addli TMP2, TMP2, 4
  |  bnez AT, <1
  |2:
  |  addlo CFUNCADDR, CCSTATE->func
  |  ld CFUNCADDR, CFUNCADDR
  |  addlo CARG2, CCSTATE->gpr[1]
  |  ld CARG2, CARG2
  |  addlo CARG3, CCSTATE->gpr[2]
  |  ld CARG3, CARG3
  |  addlo CARG4, CCSTATE->gpr[3]
  |  ld CARG4, CARG4
  |  //ldc1 FARG1, CCSTATE->fpr[0]
  |  //ldc1 FARG2, CCSTATE->fpr[1]
  |  addlo AT, CCSTATE:CARG1->gpr[0]		// Do this last, since CCSTATE is CARG1.
  |  ld CARG1, AT
  |  jalr CFUNCADDR
  |  addi CCSTATE:TMP1, r16, -12
  |  ld CCSTATE:TMP1, CCSTATE:TMP1
  |  addi TMP2, r16, -8
  |  ld TMP2, TMP2
  |  addi lr, r16, -4
  |  ld lr,lr
  |  addlo AT, CCSTATE:TMP1->gpr[0]
  |  st AT, CRET1
  |  addlo AT, CCSTATE:TMP1->gpr[1]
  |  st AT, CRET2
  |  //sdc1 FRET1, CCSTATE:TMP1->fpr[0]
  |  //sdc1 FRET2, CCSTATE:TMP1->fpr[1]
  |  move sp, r16
  |  move r16, TMP2
  |  jr lr
  |.endif
  |// Note: vm_ffi_call must be the last function in this object file!
  |
  |//-----------------------------------------------------------------------
}

/* Generate the code for a single instruction. */
static void build_ins(BuildCtx *ctx, BCOp op, int defop)
{
  int vk = 0;
  |=>defop:

  switch (op) {

  /* -- Comparison ops ---------------------------------------------------- */

  /* Remember: all ops branch for a true comparison, fall through otherwise. */

  case BC_ISLT: case BC_ISGE: case BC_ISLE: case BC_ISGT:
    |  // RA = src1*8, RD = src2*8, JMP with RD = target
    |  add CARG2, BASE, RA
    |   add CARG3, BASE, RD
    |  addi TMP0, CARG2, HI
    |  ld4s TMP0, TMP0
    |   addi TMP1, CARG3, HI
    |   ld4s TMP1, TMP1
    |   ld AT2, CARG2
    |   ld AT3, CARG3
    |  moveli AT, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT, AT, (short)(LJ_TISNUM & 0xFFFF)
    |  cmpltu TMP0, TMP0, AT
    |   cmpltu TMP1, TMP1, AT
    |    addi TMP2, PC, OFS_RD
    |    ld2u TMP2, TMP2
    |  and TMP0, TMP0, TMP1
    |    addi PC, PC, 4
    |   moveli TMP1, (-(BCBIAS_J*4 >> 16) & 65535)
    |   shl16insli TMP1, TMP1, 0
    |  beqz TMP0, ->vmeta_comp
    |    decode_RD4b TMP2
    |    add TMP2, TMP2, TMP1
    |  fdouble_add_flags AT, AT2, AT3
    if (op == BC_ISLT || op == BC_ISGE) {
      |  bfextu AT, AT, 26, 26
    } else {
      |  bfextu AT, AT, 27, 27
    }
    if (op == BC_ISLT || op == BC_ISLE) {
      |  cmoveqz TMP2, AT, zero
    } else {
      |  cmovnez TMP2, AT, zero
    }
    |  add PC, PC, TMP2
    |1:
    |  ins_next
    break;

  case BC_ISEQV: case BC_ISNEV:
    vk = op == BC_ISEQV;
    |  // RA = src1*8, RD = src2*8, JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |   ld AT2, RA
    |  add RD, BASE, RD
    |    addi TMP2, PC, -4+OFS_RD
    |    ld2u TMP2, TMP2
    |   addi TMP1, RD, HI
    |   ld4s TMP1, TMP1
    |   ld AT3, RD
    |    moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |    shl16insli TMP3, TMP3, 0
    |  moveli AT, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT, AT, (short)(LJ_TISNUM & 0xFFFF)
    |  cmpltu AT1, TMP0, AT
    |  cmpltu CARG1, TMP1, AT
    |    decode_RD4b TMP2
    |  and AT, AT1, CARG1
    |   add TMP2, TMP2, TMP3
    |  beqz AT, >5
    |  fdouble_add_flags AT, AT2, AT3
    |  bfextu AT, AT, 30, 30
    if (vk) {
      |  cmoveqz TMP2, AT, zero
    } else {
      |  cmovnez TMP2, AT, zero
    }
    |1:
    |  add PC, PC, TMP2
    |  ins_next
    |5:  // Either or both types are not numbers.
    |  addi CARG2, RA, LO
    |  ld4s CARG2, CARG2
    |  addi CARG3, RD, LO
    |  ld4s CARG3, CARG3
    |.if FFI
    |  addi TMP3, zero, LJ_TCDATA
    |  cmpeq AT1, TMP0, TMP3
    |  bnez AT1, ->vmeta_equal_cd
    |.endif
    |  cmpltui AT, TMP0, LJ_TISPRI		// Not a primitive?
    |.if FFI
    |  cmpeq AT1, TMP1, TMP3
    |  bnez AT1, ->vmeta_equal_cd
    |.endif
    |  xor TMP3, CARG2, CARG3			// Same tv?
    |  xor TMP1, TMP1, TMP0			// Same type?
    |  cmpltui CARG1, TMP0, LJ_TISTABUD+1		// Table or userdata?
    |  cmoveqz TMP3, AT, zero			// Ignore tv if primitive.
    |  cmovnez CARG1, TMP1, zero			// Tab/ud and same type?
    |  or AT, TMP1, TMP3			// Same type && (pri||same tv).
    |  cmoveqz CARG1, AT, zero
    if (vk) {
      |  cmovnez TMP2, AT, zero
    } else {
      |  cmoveqz TMP2, AT, zero
    }
    |  beqz CARG1, <1	// Done if not tab/ud or not same type or same tv.
    |  // Different tables or userdatas. Need to check __eq metamethod.
    |  // Field metatable must be at same offset for GCtab and GCudata!
    |  addlo TAB:TMP1, TAB:CARG2->metatable
    |  ld TAB:TMP1, TAB:TMP1
    |  beqz TAB:TMP1, <1		// No metatable?
    |  addlo TMP1, TAB:TMP1->nomm
    |  ld1u TMP1, TMP1
    |  andi TMP1, TMP1, 1<<MM_eq
    |  bnez TMP1, <1			// Or 'no __eq' flag set?
    |  addi CARG4, zero, 1-vk			// ne = 0 or 1.
    |  b ->vmeta_equal			// Handle __eq metamethod.
    break;

  case BC_ISEQS: case BC_ISNES:
    vk = op == BC_ISEQS;
    |  // RA = src*8, RD = str_const*8 (~), JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |   shrui RD, RD, 1
    |  addi STR:TMP3, RA, LO
    |  ld4u STR:TMP3, STR:TMP3
    |   sub RD, KBASE, RD
    |    addi TMP2, PC, -4+OFS_RD
    |    ld2u TMP2, TMP2
    |.if FFI
    |  addi AT, zero, LJ_TCDATA
    |  cmpeq AT, TMP0, AT
    |  bnez AT, ->vmeta_equal_cd
    |.endif
    |  addi STR:TMP1, RD, -4
    |  ld4u STR:TMP1, STR:TMP1
    |  addli TMP0, TMP0, -LJ_TSTR
    |   decode_RD4b TMP2
    |  xor TMP1, STR:TMP1, STR:TMP3
    |  or TMP0, TMP0, TMP1
    |   moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |   shl16insli TMP3, TMP3, 0
    |   add TMP2, TMP2, TMP3
    if (vk) {
      |  cmovnez TMP2, TMP0, zero
    } else {
      |  cmoveqz TMP2, TMP0, zero
    }
    |  add PC, PC, TMP2
    |  ins_next
    break;

  case BC_ISEQN: case BC_ISNEN:
    vk = op == BC_ISEQN;
    |  // RA = src*8, RD = num_const*8, JMP with RD = target
    |  add RA, BASE, RA
    |   addli PC, PC, 4
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |   ld AT2, RA
    |  add RD, KBASE, RD
    |    addi TMP2, PC, -4+OFS_RD
    |    ld2u TMP2, TMP2
    |   ld AT3, RD
    |    moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |    shl16insli TMP3, TMP3, 0
    |  moveli AT, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT, AT, (short)(LJ_TISNUM & 0xFFFF)
    |  cmpltu AT, TMP0, AT
    |    decode_RD4b TMP2
    |   add TMP2, TMP2, TMP3
    |.if FFI
    |  beqz AT, >5
    |.else
    |  beqz AT, >1
    |.endif
    |  fdouble_add_flags AT, AT2, AT3
    |  bfextu AT, AT, 30, 30
    if (vk) {
      |  cmoveqz TMP2, AT, zero
      |  add PC, PC, TMP2
      |1:
    } else {
      |  cmovnez TMP2, AT, zero
      |1:
      |  add PC, PC, TMP2
    }
    |  ins_next
    |.if FFI
    |5:
    |  addi AT, zero, LJ_TCDATA
    |  cmpeq AT, TMP0, AT
    |  bnez AT, ->vmeta_equal_cd
    |  b <1
    |.endif
    break;

  case BC_ISEQP: case BC_ISNEP:
    vk = op == BC_ISEQP;
    |  // RA = src*8, RD = primitive_type*8 (~), JMP with RD = target
    |  add RA, BASE, RA
    |   shrui TMP1, RD, 3
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |    addi TMP2, PC, OFS_RD
    |    ld2u TMP2, TMP2
    |   nor TMP1, TMP1, zero
    |    addli PC, PC, 4
    |.if FFI
    |  addi AT, zero, LJ_TCDATA
    |  cmpeq AT, TMP0, AT
    |  bnez AT, ->vmeta_equal_cd
    |.endif
    |  xor TMP0, TMP0, TMP1
    |  decode_RD4b TMP2
    |  moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |  shl16insli TMP3, TMP3, 0
    |  add TMP2, TMP2, TMP3
    if (vk) {
      |  cmovnez, TMP2, TMP0, zero
    } else {
      |  cmoveqz, TMP2, TMP0, zero
    }
    |  add PC, PC, TMP2
    |  ins_next
    break;

  /* -- Unary test and copy ops ------------------------------------------- */

  case BC_ISTC: case BC_ISFC: case BC_IST: case BC_ISF:
    |  // RA = dst*8 or unused, RD = src*8, JMP with RD = target
    |  add RD, BASE, RD
    |   addi TMP2, PC, OFS_RD
    |   ld2u TMP2, TMP2
    |  addi TMP0, RD, HI
    |  ld4s TMP0, TMP0
    |   addi PC, PC, 4
    if (op == BC_IST || op == BC_ISF) {
      |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
      |   decode_RD4b TMP2
      |   moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
      |   shl16insli TMP3, TMP3, 0
      |   add TMP2, TMP2, TMP3
      if (op == BC_IST) {
	|  cmoveqz TMP2, TMP0, zero
      } else {
	|  cmovnez TMP2, TMP0, zero
      }
      |  add PC, PC, TMP2
    } else {
      |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
      |  ld AT2, RD
      |  add RA, BASE, RA
      if (op == BC_ISTC) {
	|  beqz TMP0, >1
      } else {
	|  bnez TMP0, >1
      }
      |   decode_RD4b TMP2
      |   moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
      |   shl16insli TMP3, TMP3, 0
      |   add TMP2, TMP2, TMP3
      |   st RA, AT2
      |   add PC, PC, TMP2
      |1:
    }
    |  ins_next
    break;

  /* -- Unary ops --------------------------------------------------------- */

  case BC_MOV:
    |  // RA = dst*8, RD = src*8
    |  add RD, BASE, RD
    |  add RA, BASE, RA
    |  ld AT2, RD
    |  ins_next1
    |  st RA, AT2
    |  ins_next2
    break;
  case BC_NOT:
    |  // RA = dst*8, RD = src*8
    |  add RD, BASE, RD
    |   add RA, BASE, RA
    |  addi TMP0, RD, HI
    |  ld4s TMP0, TMP0
    |   addi TMP1, zero, LJ_TFALSE
    |  cmpltui TMP0, TMP0, LJ_TISTRUECOND
    |  addli TMP1, TMP0, LJ_TTRUE
    |  ins_next1
    |  addi AT, RA, HI
    |  st4 AT, TMP1
    |  ins_next2
    break;
  case BC_UNM:
    |  // RA = dst*8, RD = src*8
    |  add CARG3, BASE, RD
    |   add RA, BASE, RA
    |  addi TMP0, CARG3, HI
    |  ld4s TMP0, TMP0
    |   //ldc1 f0, 0(CARG3)
    |  cmpltui AT, TMP0, LJ_TISNUM
    |  //neg.d f0, f0
    |  beqz AT, ->vmeta_unm
    |  ins_next1
    |   //sdc1 f0, 0(RA)
    |  ins_next2
    break;
  case BC_LEN:
    |  // RA = dst*8, RD = src*8
    |  add CARG2, BASE, RD
    |   add RA, BASE, RA
    |  addi TMP0, CARG2, HI
    |  ld4s TMP0, TMP0
    |   addi CARG1, CARG2, LO
    |   ld4s CARG1, CARG1
    |  addi AT1, zero, LJ_TSTR
    |  addi AT, zero, LJ_TTAB
    |  cmpne AT1, TMP0, AT1
    |  bnez AT1, >2
    |  addlo CRET1, STR:CARG1->len
    |  ld CRET1, CRET1
    |1:
    |  //mtc1 CRET1, f0
    |  //cvt.d.w f0, f0
    |  ins_next1
    |  //sdc1 f0, 0(RA)
    |  ins_next2
    |2:
    |  cmpne AT, TMP0, AT
    |  bnez AT, ->vmeta_len
#if LJ_52
    |  addlo TAB:TMP2, TAB:CARG1->metatable
    |  ld TAB:TMP2, TAB:TMP2
    |  bnez TAB:TMP2, >9
    |3:
#endif
    |->BC_LEN_Z:
    |  load_got lj_tab_len
    |  call_intern lj_tab_len		// (GCtab *t)
    |  // Returns uint32_t (but less than 2^31).
    |  b <1
#if LJ_52
    |9:
    |  addlo TMP0, TAB:TMP2->nomm
    |  ld1u TMP0, TMP0
    |  andi TMP0, TMP0, 1<<MM_len
    |  bnez TMP0, <3			// 'no __len' flag set: done.
    |  b ->vmeta_len
#endif
    break;

  /* -- Binary ops -------------------------------------------------------- */

    |.macro ins_arithpre
    ||vk = ((int)op - BC_ADDVN) / (BC_ADDNV-BC_ADDVN);
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  // RA = dst*8, RB = src1*8, RC = src2*8 | num_const*8
    ||switch (vk) {
    ||case 0:
    |   add CARG3, BASE, RB
    |    add CARG4, KBASE, RC
    |   addi TMP1, CARG3, HI
    |   ld4s TMP1, TMP1
    |   ld AT2, CARG3
    |   ld AT3, CARG4
    |  moveli AT, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT, AT, (short)(LJ_TISNUM & 0xFFFF)
    |   cmpltu AT, TMP1, AT
    ||  break;
    ||case 1:
    |   add CARG4, BASE, RB
    |    add CARG3, KBASE, RC
    |   addi TMP1, CARG4, HI
    |   ld4s TMP1, TMP1
    |   ld AT3, CARG4
    |    ld AT2, CARG3
    |  moveli AT, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT, AT, (short)(LJ_TISNUM & 0xFFFF)
    |   cmpltu AT, TMP1, AT
    ||  break;
    ||default:
    |   add CARG3, BASE, RB
    |    add CARG4, BASE, RC
    |   addi TMP1, CARG3, HI
    |   ld4s TMP1, TMP1
    |    addi TMP2, CARG4, HI
    |    ld4s TMP2, TMP2
    |   ld AT2, CARG3
    |    ld AT3, CARG4
    |  moveli AT1, (short)((LJ_TISNUM >> 16) & 0xFFFF)
    |  shl16insli AT1, AT1, (short)(LJ_TISNUM & 0xFFFF)
    |   cmpltu AT, TMP1, AT1
    |   cmpltu TMP0, TMP2, AT1
    |   and AT, AT, TMP0
    ||  break;
    ||}
    |  add RA, BASE, RA
    |  beqz AT, ->vmeta_arith
    |.endmacro
    |
    |.macro fpmod, a, b, c
    |->BC_MODVN_Z:
    |  //div.d FARG1, b, c
    |  jal ->vm_floor			// floor(b/c)
    |  //mul.d a, FRET1, c
    |  //sub.d a, b, a			// b - floor(b/c)*c
    |.endmacro
    |
    |.macro ins_arith, ins
    |  ins_arithpre
    |.if "ins" == "fpmod_"
    |  b ->BC_MODVN_Z			// Avoid 3 copies. It's slow anyway.
    |.elif "ins" == "add"
    |  fdouble_add_flags AT, AT2, AT3
    |  fdouble_unpack_min AT1, AT2, AT3
    |  fdouble_unpack_max AT3, AT2, AT3
    |  fdouble_addsub AT3, AT1, AT
    |  fdouble_pack1 AT2, AT3, AT
    |  fdouble_pack2 AT2, AT3, zero
    |.elif "ins" == "sub"
    |  fdouble_sub_flags AT, AT2, AT3
    |  fdouble_unpack_min AT1, AT2, AT3
    |  fdouble_unpack_max AT3, AT2, AT3
    |  fdouble_addsub AT3, AT1, AT
    |  fdouble_pack1 AT2, AT3, AT
    |  fdouble_pack2 AT2, AT3, zero
    |.elif "ins" == "mul"
    |  fdouble_unpack_max AT, AT2, zero
    |  fdouble_unpack_max AT1, AT3, zero
    |  fdouble_mul_flags AT2, AT2, AT3
    |  mul_hu_lu AT3, AT, AT1
    |  mul_lu_lu AT4, AT, AT1
    |  mula_hu_lu AT3, AT1, AT
    |  mul_hu_hu AT, AT, AT1
    |  shli AT5, AT3, 32
    |  shrui AT3, AT3, 32
    |  add AT4, AT4, AT5
    |  add AT3, AT, AT3
    |  cmpltu AT5, AT4, AT5
    |  add AT3, AT3, AT5
    |  fdouble_pack1 AT2, AT3, AT2
    |  fdouble_pack2 AT2, AT3, AT4
    |.else  // div
    |  move CARG1, AT2
    |  move CARG2, AT3
    |  load_got __divdf3
    |  call_intern __divdf3
    |  move AT2, CRET1
    |.endif
    |  ins_next1
    |  st RA, AT2
    |  ins_next2
    |.endmacro

  case BC_ADDVN: case BC_ADDNV: case BC_ADDVV:
    |  ins_arith add
    break;
  case BC_SUBVN: case BC_SUBNV: case BC_SUBVV:
    |  ins_arith sub
    break;
  case BC_MULVN: case BC_MULNV: case BC_MULVV:
    |  ins_arith mul
    break;
  case BC_DIVVN: case BC_DIVNV: case BC_DIVVV:
    |  ins_arith div
    break;
  case BC_MODVN:
    |  //ins_arith fpmod
    break;
  case BC_MODNV: case BC_MODVV:
    |  //ins_arith fpmod_
    break;
  case BC_POW:
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG3, BASE, RB
    |   add CARG4, BASE, RC
    |  addi TMP1, CARG3, HI
    |  ld4s TMP1, TMP1
    |   addi TMP2, CARG4, HI
    |   ld4s TMP2, TMP2
    |  //ldc1 FARG1, 0(CARG3)
    |   //ldc1 FARG2, 0(CARG4)
    |  cmpltui AT, TMP1, LJ_TISNUM
    |  cmpltui TMP0, TMP2, LJ_TISNUM
    |  and AT, AT, TMP0
    |  load_got pow
    |  add RA, BASE, RA
    |  beqz AT, ->vmeta_arith
    |  call_extern
    |  ins_next1
    |  //sdc1 FRET1, 0(RA)
    |  ins_next2
    break;

  case BC_CAT:
    |  // RA = dst*8, RB = src_start*8, RC = src_end*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  sub CARG3, RC, RB
    |   addlo AT, L->base
    |   st AT, BASE
    |  add CARG2, BASE, RC
    |  move MULTRES, RB
    |->BC_CAT_Z:
    |  load_got lj_meta_cat
    |  shrui CARG3, CARG3, 3
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  move CARG1, L
    |  call_intern lj_meta_cat		// (lua_State *L, TValue *top, int left)
    |  // Returns NULL (finished) or TValue * (metamethod).
    |  addlo AT, L:LREG->base
    |  ld BASE, AT
    |  bnez CRET1, ->vmeta_binop
    |  add RB, BASE, MULTRES
    |  ld AT2, RB
    |   add RA, BASE, RA
    |  ins_next1
    |   st RA, AT2			// Copy result from RB to RA.
    |  ins_next2
    break;

  /* -- Constant ops ------------------------------------------------------ */

  case BC_KSTR:
    |  // RA = dst*8, RD = str_const*8 (~)
    |  shrui TMP1, RD, 1
    |  sub TMP1, KBASE, TMP1
    |  ins_next1
    |  addi TMP0, TMP1, -4
    |  ld4u TMP0, TMP0
    |  add RA, BASE, RA
    |   addi TMP2, zero, LJ_TSTR
    |  addi AT, RA, LO
    |  st4 AT, TMP0
    |  addi AT, RA, HI
    |   st4 AT, TMP2
    |  ins_next2
    break;
  case BC_KCDATA:
    |.if FFI
    |  // RA = dst*8, RD = cdata_const*8 (~)
    |  shrui TMP1, RD, 1
    |  sub TMP1, KBASE, TMP1
    |  ins_next1
    |  addi TMP0, TMP1, -4
    |  ld4u TMP0, TMP0
    |  add RA, BASE, RA
    |   addi TMP2, zero, LJ_TCDATA
    |  addi AT, RA, LO
    |  st4 AT, TMP0
    |  addi AT, RA, HI
    |   st4 AT, TMP2
    |  ins_next2
    |.endif
    break;
  case BC_KSHORT:
    |  // RA = dst*8, RD = int16_literal*8
    |  bfexts RD, INS, 16, 31
    |  cmpltsi AT, RD, 0
    |  sub AT1, zero, RD
    |  moveli AT2, 539
    |  cmovnez RD, AT, AT1
    |  shli AT2, AT2, 8
    |  shli AT1, RD, 4
    |  bfins AT2, AT, 20, 20
    |  fdouble_pack1 RD, AT1, AT2
    |  fdouble_pack2 RD, AT1, zero
    |  add RA, BASE, RA
    |  ins_next1
    |  st RA, RD
    |  ins_next2
    break;
  case BC_KNUM:
    |  // RA = dst*8, RD = num_const*8
    |  add RD, KBASE, RD
    |   add RA, BASE, RA
    |  ld AT, RD
    |  ins_next1
    |   st RA, AT
    |  ins_next2
    break;
  case BC_KPRI:
    |  // RA = dst*8, RD = primitive_type*8 (~)
    |  shrui TMP1, RD, 3
    |   add RA, BASE, RA
    |  nor TMP0, TMP1, zero
    |  ins_next1
    |   addi AT, RA, HI
    |   st4 AT, TMP0
    |  ins_next2
    break;
  case BC_KNIL:
    |  // RA = base*8, RD = end*8
    |  add RA, BASE, RA
    |  addi AT, RA, HI
    |  st4 AT, TISNIL
    |   addi RA, RA, 8
    |  add RD, BASE, RD
    |1:
    |  addi AT, RA, HI
    |  st4 AT, TISNIL
    |  cmplts AT, RA, RD
    |  addi RA, RA, 8
    |  bnez AT, <1
    |  ins_next_
    break;

  /* -- Upvalue and function ops ------------------------------------------ */

  case BC_UGET:
    |  // RA = dst*8, RD = uvnum*8
    |  addi LFUNC:RB, BASE, FRAME_FUNC
    |  ld4s LFUNC:RB, LFUNC:RB
    |   shrui RD, RD, 1
    |   add RD, RD, LFUNC:RB
    |  addlo UPVAL:RB, LFUNC:RD->uvptr
    |  ld UPVAL:RB, UPVAL:RB
    |  ins_next1
    |  addlo TMP1, UPVAL:RB->v
    |  ld TMP1, TMP1
    |  ld AT2, TMP1
    |  add RA, BASE, RA
    |  st RA, AT2
    |  ins_next2
    break;
  case BC_USETV:
    |  // RA = uvnum*8, RD = src*8
    |  addi LFUNC:RB, BASE, FRAME_FUNC
    |  ld4s LFUNC:RB, LFUNC:RB
    |    shrui RA, RA, 1
    |   add RD, BASE, RD
    |    add RA, RA, LFUNC:RB
    |   ld AT2, RD
    |  addlo UPVAL:RB, LFUNC:RA->uvptr
    |  ld UPVAL:RB, UPVAL:RB
    |  addlo TMP3, UPVAL:RB->marked
    |  ld1u TMP3, TMP3
    |   addlo CARG2, UPVAL:RB->v
    |   ld CARG2, CARG2
    |  andi TMP3, TMP3, LJ_GC_BLACK	// isblack(uv)
    |  addlo TMP0, UPVAL:RB->closed
    |  ld1u TMP0, TMP0
    |   addi TMP2, RD, HI
    |   ld4s TMP2, TMP2
    |   st CARG2, AT2
    |  addi AT, zero, LJ_GC_BLACK|1
    |  or TMP3, TMP3, TMP0
    |  addli TMP2, TMP2, -(LJ_TISNUM+1)
    |  cmpeq AT, TMP3, AT
    |  bnez AT, >2			// Upvalue is closed and black?
    |1:
    |  ins_next
    |
    |2:  // Check if new value is collectable.
    |  cmpltui AT, TMP2, LJ_TISGCV - (LJ_TISNUM+1)
    |  addi AT, RD, LO
    |  ld4s TMP1, AT
    |  beqz AT, <1			// tvisgcv(v)
    |  addlo TMP3, GCOBJ:TMP1->gch.marked
    |  ld1u TMP3, TMP3
    |  andi TMP3, TMP3, LJ_GC_WHITES	// iswhite(v)
    |  load_got lj_gc_barrieruv
    |  beqz TMP3, <1
    |  // Crossed a write barrier. Move the barrier forward.
    |  addli CARG1, DISPATCH, GG_DISP2G
    |  call_intern lj_gc_barrieruv	// (global_State *g, TValue *tv)
    |  b <1
    break;
  case BC_USETS:
    |  // RA = uvnum*8, RD = str_const*8 (~)
    |  addi LFUNC:RB, BASE, FRAME_FUNC
    |  ld4s LFUNC:RB, LFUNC:RB
    |    shrui RA, RA, 1
    |   shrui TMP1, RD, 1
    |    add RA, RA, LFUNC:RB
    |   sub TMP1, KBASE, TMP1
    |  addlo UPVAL:RB, LFUNC:RA->uvptr
    |  ld UPVAL:RB, UPVAL:RB
    |   addi STR:TMP1, TMP1, -4
    |   ld STR:TMP1, STR:TMP1
    |  addlo TMP2, UPVAL:RB->marked
    |  ld1u TMP2, TMP2
    |   addlo CARG2, UPVAL:RB->v
    |   ld CARG2, CARG2
    |   addlo TMP3, STR:TMP1->marked
    |   ld1u TMP3, TMP3
    |  andi AT, TMP2, LJ_GC_BLACK	// isblack(uv)
    |   addlo TMP2, UPVAL:RB->closed
    |   ld1u TMP2, TMP2
    |   addi TMP0, zero, LJ_TSTR
    |   addi AT, CARG2, LO
    |   st4 AT, STR:TMP1
    |  addi AT, CARG2, HI
    |  st4 AT, TMP0
    |  bnez AT, >2
    |1:
    |  ins_next
    |
    |2:  // Check if string is white and ensure upvalue is closed.
    |  andi AT, TMP3, LJ_GC_WHITES	// iswhite(str)
    |  beqz TMP2, <1
    |  load_got lj_gc_barrieruv
    |  beqz AT, <1
    |  // Crossed a write barrier. Move the barrier forward.
    |  addli CARG1, DISPATCH, GG_DISP2G
    |  call_intern lj_gc_barrieruv	// (global_State *g, TValue *tv)
    |  b <1
    break;
  case BC_USETN:
    |  // RA = uvnum*8, RD = num_const*8
    |  addi LFUNC:RB, BASE, FRAME_FUNC
    |  ld4s LFUNC:RB, LFUNC:RB
    |   shrui RA, RA, 1
    |    add RD, KBASE, RD
    |   add RA, RA, LFUNC:RB
    |    //ldc1 f0, 0(RD)
    |  addlo UPVAL:RB, LFUNC:RA->uvptr
    |  ld UPVAL:RB, UPVAL:RB
    |  ins_next1
    |  addlo TMP1, UPVAL:RB->v
    |  ld TMP1, TMP1
    |  //sdc1 f0, 0(TMP1)
    |  ins_next2
    break;
  case BC_USETP:
    |  // RA = uvnum*8, RD = primitive_type*8 (~)
    |  addi LFUNC:RB, BASE, FRAME_FUNC
    |  ld4s LFUNC:RB, LFUNC:RB
    |   shrui RA, RA, 1
    |    shrui TMP0, RD, 3
    |   add RA, RA, LFUNC:RB
    |    nor TMP0, TMP0, zero
    |  addlo UPVAL:RB, LFUNC:RA->uvptr
    |  ld UPVAL:RB, UPVAL:RB
    |  ins_next1
    |  addlo TMP1, UPVAL:RB->v
    |  ld TMP1, TMP1
    |  addi AT, TMP1, HI
    |  st4 AT, TMP0
    |  ins_next2
    break;

  case BC_UCLO:
    |  // RA = level*8, RD = target
    |  addlo TMP2, L->openupval
    |  ld4u TMP2, TMP2
    |  branch_RD			// Do this first since RD is not saved.
    |  load_got lj_func_closeuv
    |   addlo AT, L->base
    |   st AT, BASE
    |  move CARG1, L
    |  beqz TMP2, >1
    |  add CARG2, BASE, RA
    |  call_intern lj_func_closeuv	// (lua_State *L, TValue *level)
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |1:
    |  ins_next
    break;

  case BC_FNEW:
    |  // RA = dst*8, RD = proto_const*8 (~) (holding function prototype)
    |  shrui TMP1, RD, 1
    |  load_got lj_func_newL_gc
    |  sub TMP1, KBASE, TMP1
    |  addi CARG3, BASE, FRAME_FUNC
    |  ld4s CARG3, CARG3
    |  addi CARG2, TMP1, -4
    |  ld4s CARG2, CARG2
    |   addlo AT, L:LREG->base
    |   st AT, BASE
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  // (lua_State *L, GCproto *pt, GCfuncL *parent)
    |  move CARG1, L
    |  call_intern lj_func_newL_gc
    |  // Returns GCfuncL *.
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |   addi TMP0, zero, LJ_TFUNC
    |  ins_next1
    |  add RA, BASE, RA
    |  addi AT, RA, HI
    |  st4 AT, TMP0
    |  addi AT, RA, LO
    |  st4 AT, LFUNC:CRET1
    |  ins_next2
    break;

  /* -- Table ops --------------------------------------------------------- */

  case BC_TNEW:
  case BC_TDUP:
    |  // RA = dst*8, RD = (hbits|asize)*8 | tab_const*8 (~)
    |  addli TMP0, DISPATCH, DISPATCH_GL(gc.total)
    |  ld4u TMP0, TMP0
    |  addli TMP1, DISPATCH, DISPATCH_GL(gc.threshold)
    |  ld4u TMP1, TMP1
    |   addlo AT, L:LREG->base
    |   st AT, BASE
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  cmpltu AT, TMP0, TMP1
    |  beqz AT, >5
    |1:
    if (op == BC_TNEW) {
      |  load_got lj_tab_new
      |  shrui CARG2, RD, 3
      |  addli AT, zero, 0x7ff
      |  and CARG2, CARG2, AT
      |  addli TMP0, zero, 0x801
      |  addli AT, CARG2, -0x7ff
      |   shrui CARG3, RD, 14
      |  cmoveqz CARG2, AT, TMP0
      |  // (lua_State *L, int32_t asize, uint32_t hbits)
      |  move CARG1, L
      |  call_intern lj_tab_new
      |  // Returns Table *.
    } else {
      |  load_got lj_tab_dup
      |  shli TMP1, RD, 1
      |  sub TMP1, KBASE, TMP1
      |  move CARG1, L
      |  addi CARG2, TMP1, -4
      |  ld4u CARG2, CARG2
      |  call_intern lj_tab_dup		// (lua_State *L, Table *kt)
      |  // Returns Table *.
    }
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |  ins_next1
    |  add RA, BASE, RA
    |   addi TMP0, zero, LJ_TTAB
    |  addi AT, RA, LO
    |  st4 AT, TAB:CRET1
    |  addi AT, RA, HI
    |   st4 AT, TMP0
    |  ins_next2
    |5:
    |  load_got lj_gc_step_fixtop
    |  move MULTRES, RD
    |  move CARG1, L
    |  call_intern lj_gc_step_fixtop	// (lua_State *L)
    |  move RD, MULTRES
    |  b <1
    break;

  case BC_GGET:
    |  // RA = dst*8, RD = str_const*8 (~)
  case BC_GSET:
    |  // RA = src*8, RD = str_const*8 (~)
    |  addi LFUNC:TMP2, BASE, FRAME_FUNC
    |  ld4s LFUNC:TMP2, LFUNC:TMP2
    |   shrui TMP1, RD, 1
    |   sub TMP1, KBASE, TMP1
    |  addlo TAB:RB, LFUNC:TMP2->env
    |  ld4u TAB:RB, TAB:RB
    |  addi STR:RC, TMP1, -4
    |  ld4u STR:RC, STR:RC
    |  add RA, BASE, RA
    if (op == BC_GGET) {
      |  b ->BC_TGETS_Z
    } else {
      |  b ->BC_TSETS_Z
    }
    break;

  case BC_TGETV:
    |  // RA = dst*8, RB = table*8, RC = key*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG2, BASE, RB
    |   add CARG3, BASE, RC
    |  addi TMP1, CARG2, HI
    |  ld4s TMP1, TMP1
    |   addi TMP2, CARG3, HI
    |   ld4s TMP2, TMP2
    |    addi TAB:RB, CARG2, LO
    |    ld4s TAB:RB, TAB:RB
    |  addi AT, zero, LJ_TTAB
    |   //ldc1 f0, 0(CARG3)
    |  add RA, BASE, RA
    |  cmpne AT, TMP1, AT
    |  bnez AT, ->vmeta_tgetv
    |  cmpltui AT, TMP2, LJ_TISNUM
    |  addi AT, zero, LJ_TSTR
    |  beqz AT, >5
    |
    |  // Convert number key to integer, check for integerness and range.
    |  //cvt.w.d f2, f0
    |   addlo TMP0, TAB:RB->asize
    |   ld TMP0, TMP0
    |  //mfc1 TMP2, f2
    |  //cvt.d.w f4, f2
    |   addlo TMP1, TAB:RB->array
    |   ld TMP1, TMP1
    |  //c.eq.d f0, f4
    |  cmpltu AT, TMP2, TMP0
    |  //movf AT, r0
    |   shli TMP2, TMP2, 3
    |  add TMP2, TMP1, TMP2
    |  beqz AT, ->vmeta_tgetv		// Integer key and in array part?
    |  addi TMP0, TMP2, HI
    |  ld4s TMP0, TMP0
    |  //ldc1 f0, 0(TMP2)
    |  cmpeq AT, TMP0, TISNIL
    |  bnez AT, >2
    |1:
    |  ins_next1
    |   //sdc1 f0, 0(RA)
    |  ins_next2
    |
    |2:  // Check for __index if table value is nil.
    |  addlo TAB:TMP2, TAB:RB->metatable
    |  ld TAB:TMP2, TAB:TMP2
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  addlo TMP0, TAB:TMP2->nomm
    |  ld1u TMP0, TMP0
    |  andi TMP0, TMP0, 1<<MM_index
    |  bnez TMP0, <1			// 'no __index' flag set: done.
    |  b ->vmeta_tgetv
    |
    |5:
    |  addi AT, CARG3, LO
    |  ld4s STR:RC, AT
    |  cmpne AT, TMP2, AT
    |  bnez AT, ->vmeta_tgetv
    |  b ->BC_TGETS_Z			// String key?
    break;
  case BC_TGETS:
    |  // RA = dst*8, RB = table*8, RC = str_const*4 (~)
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RC4a RC, INS
    |  addi TMP0, CARG2, HI
    |  ld4s TMP0, TMP0
    |   decode_RC4b RC
    |  addi AT, zero, LJ_TTAB
    |   addi TAB:RB, CARG2, LO
    |   ld4s TAB:RB, TAB:RB
    |   sub CARG3, KBASE, RC
    |   addi STR:RC, CARG3, -4
    |   ld4u STR:RC, STR:RC
    |  add RA, BASE, RA
    |  cmpne AT, TMP0, AT
    |  bnez AT, ->vmeta_tgets1
    |->BC_TGETS_Z:
    |  // TAB:RB = GCtab *, STR:RC = GCstr *, RA = dst*8
    |  addlo TMP0, TAB:RB->hmask
    |  ld4u TMP0, TMP0
    |  addlo TMP1, STR:RC->hash
    |  ld4u TMP1, TMP1
    |  addlo NODE:TMP2, TAB:RB->node
    |  ld4u NODE:TMP2, NODE:TMP2
    |  and TMP1, TMP1, TMP0		// idx = str->hash & tab->hmask
    |  shli TMP0, TMP1, 5
    |  shli TMP1, TMP1, 3
    |  sub TMP1, TMP0, TMP1
    |  add NODE:TMP2, NODE:TMP2, TMP1	// node = tab->node + (idx*32-idx*8)
    |1:
    |  addi CARG1, NODE:TMP2, offsetof(Node, key)+HI
    |  ld4s CARG1, CARG1
    |   addi TMP0, NODE:TMP2, offsetof(Node, key)+LO
    |   ld4s TMP0, TMP0
    |    addlo NODE:TMP1, NODE:TMP2->next
    |    ld4s NODE:TMP1, NODE:TMP1
    |    addi CARG2, NODE:TMP2, offsetof(Node, val)+HI
    |    ld4s CARG2, CARG2
    |  addli CARG1, CARG1, 5
    |   xor TMP0, TMP0, STR:RC
    |  or AT, CARG1, TMP0
    |  addlo TAB:TMP3, TAB:RB->metatable
    |  ld4u TAB:TMP3, TAB:TMP3
    |  bnez AT, >4
    |    addi CARG1, NODE:TMP2, offsetof(Node, val)+LO
    |    ld4s CARG1, CARG1
    |    cmpeq AT, CARG2, TISNIL
    |    bnez AT, >5		// Key found, but nil value?
    |3:
    |  ins_next1
    |    addi AT, RA, HI
    |    st4 AT, CARG2
    |    addi AT, RA, LO
    |     st4 AT, CARG1
    |  ins_next2
    |
    |4:  // Follow hash chain.
    |  move NODE:TMP2, NODE:TMP1
    |  bnez NODE:TMP1, <1
    |  // End of hash chain: key not found, nil result.
    |
    |5:  // Check for __index if table value is nil.
    |  addi CARG2, zero, LJ_TNIL
    |  beqz TAB:TMP3, <3		// No metatable: done.
    |  addlo TMP0, TAB:TMP3->nomm
    |  ld1u TMP0, TMP0
    |  andi TMP0, TMP0, 1<<MM_index
    |  bnez TMP0, <3			// 'no __index' flag set: done.
    |  b ->vmeta_tgets
    break;
  case BC_TGETB:
    |  // RA = dst*8, RB = table*8, RC = index*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RDtoRC8 RC, RD
    |  addi CARG1, CARG2, HI
    |  ld4s CARG1, CARG1
    |  addi AT, zero, LJ_TTAB
    |   addi TAB:RB, CARG2, LO
    |   ld4s TAB:RB, TAB:RB
    |   add RA, BASE, RA
    |  shli TMP0, RC, 3
    |  cmpne AT, CARG1, AT
    |  bnez AT, ->vmeta_tgetb
    |  addlo TMP1, TAB:RB->asize
    |  ld TMP1, TMP1
    |   addlo TMP2, TAB:RB->array
    |   ld TMP2, TMP2
    |  cmpltu AT, TMP0, TMP1
    |  add RC, TMP2, RC
    |  beqz AT, ->vmeta_tgetb
    |  addi TMP1, RC, HI
    |  ld4s TMP1, TMP1
    |  //ldc1 f0, 0(RC)
    |  cmpeq AT, TMP1, TISNIL
    |  bnez AT, >5
    |1:
    |  ins_next1
    |   //sdc1 f0, 0(RA)
    |  ins_next2
    |
    |5:  // Check for __index if table value is nil.
    |  addlo TAB:TMP2, TAB:RB->metatable
    |  ld TAB:TMP2, TAB:TMP2
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  addlo AT, TAB:TMP2->nomm
    |  ld1u TMP1, AT
    |  andi TMP1, TMP1, 1<<MM_index
    |  bnez TMP1, <1			// 'no __index' flag set: done.
    |  b ->vmeta_tgetb			// Caveat: preserve TMP0!
    break;

  case BC_TSETV:
    |  // RA = src*8, RB = table*8, RC = key*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |   decode_RDtoRC8 RC, RD
    |  add CARG2, BASE, RB
    |   add CARG3, BASE, RC
    |  addi TMP1, CARG2, HI
    |  ld4s TMP1, TMP1
    |   addi TMP2, CARG3, HI
    |   ld4s TMP2, TMP2
    |    addi TAB:RB, CARG2, LO
    |    ld4s TAB:RB, TAB:RB
    |  addi AT, zero, LJ_TTAB
    |   //ldc1 f0, 0(CARG3)
    |  add RA, BASE, RA
    |  cmpne AT, TMP1, AT
    |  bnez AT, ->vmeta_tsetv
    |  cmpltui AT, TMP2, LJ_TISNUM
    |  addi AT, zero, LJ_TSTR
    |  beqz AT, >5
    |
    |  // Convert number key to integer, check for integerness and range.
    |  //cvt.w.d f2, f0
    |   addlo TMP0, TAB:RB->asize
    |   ld TMP0, TMP0
    |  //mfc1 TMP2, f2
    |  //cvt.d.w f4, f2
    |   addlo TMP1, TAB:RB->array
    |   ld TMP1, TMP1
    |  //c.eq.d f0, f4
    |  cmpltu AT, TMP2, TMP0
    |  //movf AT, r0
    |   shli TMP2, TMP2, 3
    |  add TMP1, TMP1, TMP2
    |  beqz AT, ->vmeta_tsetv		// Integer key and in array part?
    |   addlo AT, TAB:RB->marked
    |   ld1u TMP3, AT
    |  addi AT, TMP1, HI
    |  ld4s TMP0, AT
    |  //ldc1 f0, 0(RA)
    |  cmpeq AT, TMP0, TISNIL
    |  bnez AT, >3
    |1:
    |   andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  //sdc1 f0, 0(TMP1)
    |   bnez AT, >7
    |2:
    |  ins_next
    |
    |3:  // Check for __newindex if previous value is nil.
    |  addlo TAB:TMP2, TAB:RB->metatable
    |  ld TAB:TMP2, TAB:TMP2
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  addlo TMP2, TAB:TMP2->nomm
    |  ld1u TMP2, TMP2
    |  andi TMP2, TMP2, 1<<MM_newindex
    |  bnez TMP2, <1			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsetv
    |
    |5:
    |  addi AT, CARG3, LO
    |  ld4s STR:RC, AT
    |  cmpne AT, TMP2, AT
    |  bnez AT, ->vmeta_tsetv
    |  b ->BC_TSETS_Z			// String key?
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <2
    break;
  case BC_TSETS:
    |  // RA = src*8, RB = table*8, RC = str_const*8 (~)
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RC4a RC, INS
    |  addi TMP0, CARG2, HI
    |  ld4s TMP0, TMP0
    |   decode_RC4b RC
    |  addi AT, zero, LJ_TTAB
    |   sub CARG3, KBASE, RC
    |    addi TAB:RB, CARG2, LO
    |    ld4s TAB:RB, TAB:RB
    |   addi STR:RC, CARG3, -4		// KBASE-4-str_const*4
    |   ld4s STR:RC, STR:RC
    |  add RA, BASE, RA
    |  cmpne AT, TMP0, AT
    |  bnez AT, ->vmeta_tsets1
    |->BC_TSETS_Z:
    |  // TAB:RB = GCtab *, STR:RC = GCstr *, RA = BASE+src*8
    |  addlo TMP0, TAB:RB->hmask
    |  ld4u TMP0, TMP0
    |  addlo TMP1, STR:RC->hash
    |  ld4u TMP1, TMP1
    |  addlo NODE:TMP2, TAB:RB->node
    |  ld4u NODE:TMP2, NODE:TMP2
    |   addlo AT, TAB:RB->nomm		// Clear metamethod cache.
    |   st1 AT, zero
    |  and TMP1, TMP1, TMP0		// idx = str->hash & tab->hmask
    |  shli TMP0, TMP1, 5
    |  shli TMP1, TMP1, 3
    |  sub TMP1, TMP0, TMP1
    |  add NODE:TMP2, NODE:TMP2, TMP1	// node = tab->node + (idx*32-idx*8)
    |   ld AT2, RA
    |1:
    |  addi CARG1, NODE:TMP2, offsetof(Node, key)+HI
    |  ld4s CARG1, CARG1
    |   addi TMP0, NODE:TMP2, offsetof(Node, key)+LO
    |   ld4s TMP0, TMP0
    |  addi AT, zero, LJ_TSTR
    |    addlo NODE:TMP1, NODE:TMP2->next
    |    ld4u NODE:TMP1, NODE:TMP1
    |   addli AT, NODE:TMP2, offsetof(Node, val)+HI
    |   ld4s CARG2, AT
    |  cmpne AT, CARG1, AT
    |  bnez AT, >5
    |    addlo AT, TAB:RB->marked
    |    ld1u TMP3, AT
    |   cmpne AT, TMP0, STR:RC
    |   bnez AT, >5
    |    addlo AT, TAB:RB->metatable
    |    ld4u TAB:TMP0, AT
    |    cmpeq AT, CARG2, TISNIL
    |    bnez AT, >4		// Key found, but nil value?
    |2:
    |  andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  addlo AT1, NODE:TMP2->val
    |  st AT1, AT2
    |  bnez AT, >7
    |3:
    |  ins_next
    |
    |4:  // Check for __newindex if previous value is nil.
    |  beqz TAB:TMP0, <2		// No metatable: done.
    |  addlo TMP0, TAB:TMP0->nomm
    |  ld1u TMP0, TMP0
    |  andi TMP0, TMP0, 1<<MM_newindex
    |  bnez TMP0, <2			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsets
    |
    |5:  // Follow hash chain.
    |  move NODE:TMP2, NODE:TMP1
    |  bnez NODE:TMP1, <1
    |  // End of hash chain: key not found, add a new one
    |
    |  // But check for __newindex first.
    |  addlo TAB:TMP2, TAB:RB->metatable
    |  ld4u TAB:TMP2, TAB:TMP2
    |  addli CARG3, DISPATCH, DISPATCH_GL(tmptv)
    |  beqz TAB:TMP2, >6		// No metatable: continue.
    |  addlo TMP0, TAB:TMP2->nomm
    |  ld1u TMP0, TMP0
    |  andi TMP0, TMP0, 1<<MM_newindex
    |  addi AT, zero, LJ_TSTR
    |  beqz TMP0, ->vmeta_tsets		// 'no __newindex' flag NOT set: check.
    |6:
    |  load_got lj_tab_newkey
    |  addi AT1, CARG3, LO
    |  st4 AT1, STR:RC
    |  addi AT1, CARG3, HI
    |  st4 AT1, AT
    |   addlo AT, L->base
    |   st AT, BASE
    |  move CARG2, TAB:RB
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  move CARG1, L
    |  call_intern lj_tab_newkey	// (lua_State *L, GCtab *t, TValue *k
    |  // Returns TValue *.
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |  st CRET1, AT2
    |  b <3				// No 2nd write barrier needed.
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <3
    break;
  case BC_TSETB:
    |  // RA = src*8, RB = table*8, RC = index*8
    |  decode_RB8a RB, INS
    |  decode_RB8b RB
    |  add CARG2, BASE, RB
    |   decode_RDtoRC8 RC, RD
    |  addi CARG1, CARG2, HI
    |  ld4s CARG1, CARG1
    |  addi AT, zero, LJ_TTAB
    |   addi TAB:RB, CARG2, LO
    |   ld4s TAB:RB, TAB:RB
    |   add RA, BASE, RA
    |  shrui TMP0, RC, 3
    |  cmpne AT, CARG1, AT
    |  bnez AT, ->vmeta_tsetb
    |  addlo TMP1, TAB:RB->asize
    |  ld TMP1, TMP1
    |   addlo TMP2, TAB:RB->array
    |   ld TMP2, TMP2
    |  cmpltu AT, TMP0, TMP1
    |  add RC, TMP2, RC
    |  beqz AT, ->vmeta_tsetb
    |  addi TMP1, RC, HI
    |  ld4s TMP1, TMP1
    |   addlo TMP3, TAB:RB->marked
    |   ld1u TMP3, TMP3
    |  //ldc1 f0, 0(RA)
    |  cmpeq AT, TMP1, TISNIL
    |  bnez AT, >5
    |1:
    |  andi AT, TMP3, LJ_GC_BLACK	// isblack(table)
    |  //sdc1 f0, 0(RC)
    |  bnez AT, >7
    |2:
    |  ins_next
    |
    |5:  // Check for __newindex if previous value is nil.
    |  addlo TAB:TMP2, TAB:RB->metatable
    |  ld TAB:TMP2, TAB:TMP2
    |  beqz TAB:TMP2, <1		// No metatable: done.
    |  addlo TMP1, TAB:TMP2->nomm
    |  ld1u TMP1, TMP1
    |  andi TMP1, TMP1, 1<<MM_newindex
    |  bnez TMP1, <1			// 'no __newindex' flag set: done.
    |  b ->vmeta_tsetb			// Caveat: preserve TMP0!
    |
    |7:  // Possible table write barrier for the value. Skip valiswhite check.
    |  barrierback TAB:RB, TMP3, TMP0, <2
    break;

  case BC_TSETM:
    |  // RA = base*8 (table at base-1), RD = num_const*8 (start index)
    |  add RA, BASE, RA
    |1:
    |   add TMP3, KBASE, RD
    |  addi TAB:CARG2, RA, -8+LO
    |  ld4s TAB:CARG2, TAB:CARG2
    |    addli TMP0, MULTRES, -8
    |   addi TMP3, TMP3, LO
    |   ld4s TMP3, TMP3
    |    shrui CARG3, TMP0, 3
    |    beqz TMP0, >4			// Nothing to copy?
    |  add CARG3, CARG3, TMP3
    |  addlo TMP2, TAB:CARG2->asize
    |  ld TMP2, TMP2
    |   shli TMP1, TMP3, 3
    |    addlo TMP3, TAB:CARG2->marked
    |    ld1u TMP3, TMP3
    |   addlo CARG1, TAB:CARG2->array
    |   ld CARG1, CARG1
    |  cmpltu AT, TMP2, CARG3
    |  add TMP2, RA, TMP0
    |  bnez AT, >5
    |   add TMP1, TMP1, CARG1
    |  andi TMP0, TMP3, LJ_GC_BLACK	// isblack(table)
    |3:  // Copy result slots to table.
    |   //ldc1 f0, 0(RA)
    |    addli RA, RA, 8
    |  cmpltu AT, RA, TMP2
    |   //sdc1 f0, 0(TMP1)
    |   addli TMP1, TMP1, 8
    |  bnez AT, <3
    |  bnez TMP0, >7
    |4:
    |  ins_next
    |
    |5:  // Need to resize array part.
    |  load_got lj_tab_reasize
    |   addlo AT, L->base
    |   st AT, BASE
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  move BASE, RD
    |  move CARG1, L
    |  call_intern lj_tab_reasize	// (lua_State *L, GCtab *t, int nasize)
    |  // Must not reallocate the stack.
    |  move RD, BASE
    |  addlo AT, L:LREG->base	// Reload BASE for lack of a saved register.
    |  ld BASE, AT	// Reload BASE for lack of a saved register.
    |  b <1
    |
    |7:  // Possible table write barrier for any value. Skip valiswhite check.
    |  barrierback TAB:CARG2, TMP3, TMP0, <4
    break;

  /* -- Calls and vararg handling ----------------------------------------- */

  case BC_CALLM:
    |  // RA = base*8, (RB = (nresults+1)*8,) RC = extra_nargs*8
    |  decode_RDtoRC8 NARGS8:RC, RD
    |  add NARGS8:RC, NARGS8:RC, MULTRES
    |  b ->BC_CALL_Z
    break;
  case BC_CALL:
    |  // RA = base*8, (RB = (nresults+1)*8,) RC = (nargs+1)*8
    |  decode_RDtoRC8 NARGS8:RC, RD
    |->BC_CALL_Z:
    |  move TMP2, BASE
    |  add BASE, BASE, RA
    |   addi AT, zero, LJ_TFUNC
    |  addi TMP0, BASE, HI
    |  ld4s TMP0, TMP0
    |   addi LFUNC:RB, BASE, LO
    |   ld4s LFUNC:RB, LFUNC:RB
    |   addli BASE, BASE, 8
    |  addli NARGS8:RC, NARGS8:RC, -8
    |  cmpne AT, TMP0, AT
    |  bnez AT, ->vmeta_call
    |  ins_call
    break;

  case BC_CALLMT:
    |  // RA = base*8, (RB = 0,) RC = extra_nargs*8
    |  add NARGS8:RD, NARGS8:RD, MULTRES	// BC_CALLT gets RC from RD.
    |  // Fall through. Assumes BC_CALLT follows.
    break;
  case BC_CALLT:
    |  // RA = base*8, (RB = 0,) RC = (nargs+1)*8
    |  add RA, BASE, RA
    |   addi AT, zero, LJ_TFUNC
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |   addi LFUNC:RB, RA, LO
    |   ld4s LFUNC:RB, LFUNC:RB
    |   move NARGS8:RC, RD
    |    addi TMP1, BASE, FRAME_PC
    |    ld4s TMP1, TMP1
    |   addli RA, RA, 8
    |  addli NARGS8:RC, NARGS8:RC, -8
    |  cmpne AT, TMP0, AT
    |  bnez AT, ->vmeta_callt
    |->BC_CALLT_Z:
    |  andi TMP0, TMP1, FRAME_TYPE	// Caveat: preserve TMP0 until the 'or'.
    |   addlo TMP3, LFUNC:RB->ffid
    |   ld1u TMP3, TMP3
    |  xori TMP2, TMP1, FRAME_VARG
    |  bnez TMP0, >7
    |1:
    |  addi AT, BASE, FRAME_FUNC
    |  st4 AT, LFUNC:RB
    |  cmpltui AT, TMP3, 2		// (> FF_C) Calling a fast function?
    |  move TMP2, BASE
    |  move TMP3, NARGS8:RC
    |  beqz NARGS8:RC, >3
    |2:
    |   //ldc1 f0, 0(RA)
    |    addli RA, RA, 8
    |  addli TMP3, TMP3, -8
    |   //sdc1 f0, 0(TMP2)
    |   addli TMP2, TMP2, 8
    |  bnez TMP3, <2
    |3:
    |  or TMP0, TMP0, AT
    |  beqz TMP0, >5
    |4:
    |  ins_callt
    |
    |5:  // Tailcall to a fast function with a Lua frame below.
    |  addi INS, TMP1, -4
    |  ld4u INS, INS
    |  decode_RA8a RA, INS
    |  decode_RA8b RA
    |  sub TMP1, BASE, RA
    |  addi LFUNC:TMP1, TMP1, -8+FRAME_FUNC
    |  ld4s LFUNC:TMP1, LFUNC:TMP1
    |  addlo TMP1, LFUNC:TMP1->pc
    |  ld4u TMP1, TMP1
    |  addi AT, TMP1, PC2PROTO(k)
    |  ld4u KBASE, AT
    |  b <4
    |
    |7:  // Tailcall from a vararg function.
    |  andi AT, TMP2, FRAME_TYPEP
    |  sub TMP2, BASE, TMP2		// Relocate BASE down.
    |  bnez AT, <1			// Vararg frame below?
    |  move BASE, TMP2
    |  addi TMP1, TMP2, FRAME_PC
    |  ld4s TMP1, TMP1
    |  andi TMP0, TMP1, FRAME_TYPE
    |  b <1
    break;

  case BC_ITERC:
    |  // RA = base*8, (RB = (nresults+1)*8, RC = (nargs+1)*8 ((2+1)*8))
    |  move TMP2, BASE
    |  add BASE, BASE, RA
    |   addi AT, zero, LJ_TFUNC
    |  addi TMP1, BASE, -24+HI
    |  ld4s TMP1, TMP1
    |   addi LFUNC:RB, BASE, -24+LO
    |   ld4s LFUNC:RB, LFUNC:RB
    |    //ldc1 f2, -8(BASE)
    |    //ldc1 f0, -16(BASE)
    |  addi AT, BASE, HI
    |  st4 AT, TMP1
    |   addi AT, BASE, LO
    |   st4 AT, LFUNC:RB
    |    //sdc1 f2, 16(BASE)		// Copy control var.
    |    //sdc1 f0, 8(BASE)		// Copy state.
    |   addli BASE, BASE, 8
    |  addi NARGS8:RC, zero, 16		// Iterators get 2 arguments.
    |  cmpne AT, TMP1, AT
    |  bnez AT, ->vmeta_call
    |  ins_call
    break;

  case BC_ITERN:
    |  // RA = base*8, (RB = (nresults+1)*8, RC = (nargs+1)*8 (2+1)*8)
    |.if JIT
    |  // NYI: add hotloop, record BC_ITERN.
    |.endif
    |  add RA, BASE, RA
    |  addi TAB:RB, RA, -16+LO
    |  ld4s TAB:RB, TAB:RB
    |  addi RC, RA, -8+LO
    |  ld4s RC, RC
    |  addlo TMP0, TAB:RB->asize
    |  ld TMP0, TMP0
    |  addlo TMP1, TAB:RB->array
    |  ld TMP1, TMP1
    |   addli PC, PC, 4
    |1:  // Traverse array part.
    |  cmpltu AT, RC, TMP0
    |  shli TMP3, RC, 3
    |  beqz AT, >5			// Index points after array part?
    |  add TMP3, TMP1, TMP3
    |  addi TMP2, TMP3, HI
    |  ld4s TMP2, TMP2
    |   //ldc1 f0, 0(TMP3)
    |    //mtc1 RC, f2
    |     addi RD, PC, -4+OFS_RD
    |     ld2u RD, RD
    |  addli RC, RC, 1
    |  cmpeq AT, TMP2, TISNIL
    |  bnez AT, <1		// Skip holes in array part.
    |    //cvt.d.w f2, f2
    |     moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |     shl16insli TMP3, TMP3, 0
    |   //sdc1 f0, 8(RA)
    |     decode_RD4b RD
    |     add RD, RD, TMP3
    |   addi AT, RA, -8+LO
    |   st4 AT, RC
    |     add PC, PC, RD
    |    //sdc1 f2, 0(RA)
    |3:
    |  ins_next
    |
    |5:  // Traverse hash part.
    |  addlo TMP1, TAB:RB->hmask
    |  ld TMP1, TMP1
    |  sub RC, RC, TMP0
    |   addlo TMP2, TAB:RB->node
    |   ld TMP2, TMP2
    |6:
    |  cmpltu AT, TMP1, RC		// End of iteration? Branch to ITERL+1.
    |  shli TMP3, RC, 5
    |  bnez AT, <3
    |   shli RB, RC, 3
    |   sub TMP3, TMP3, RB
    |  add NODE:TMP3, TMP3, TMP2
    |  addi RB, NODE:TMP3, HI
    |  ld4s RB, RB
    |  //ldc1 f0, 0(NODE:TMP3)
    |     addi RD, PC, -4+OFS_RD
    |     ld2u RD, RD
    |  addli RC, RC, 1
    |  cmpeq AT, RB, TISNIL
    |  bnez AT, <6		// Skip holes in hash part.
    |   //ldc1 f2, NODE:TMP3->key
    |     moveli TMP3, (-(BCBIAS_J*4 >> 16) & 65535)
    |     shl16insli TMP3, TMP3, 0
    |  //sdc1 f0, 8(RA)
    |    add RC, RC, TMP0
    |     decode_RD4b RD
    |     add RD, RD, TMP3
    |   //sdc1 f2, 0(RA)
    |     add PC, PC, RD
    |  addi AT, RA, -8+LO
    |  st4 AT, RC
    |  b <3
    break;

  case BC_ISNEXT:
    |  // RA = base*8, RD = target (points to ITERN)
    |  add RA, BASE, RA
    |  addi TMP0, RA, -24+HI
    |  ld4s TMP0, TMP0
    |  addi CFUNC:TMP1, RA, -24+LO
    |  ld4s CFUNC:TMP1, CFUNC:TMP1
    |   addi TMP2, RA, -16+HI
    |   ld4s TMP2, TMP2
    |    addi TMP3, RA, -8+HI
    |    ld4s TMP3, TMP3
    |  addi AT, zero, LJ_TFUNC
    |  addli TMP2, TMP2, 12 //-LJ_TTAB
    |  cmpne AT, TMP0, AT
    |  bnez AT, >5
    |  addlo TMP1, CFUNC:TMP1->ffid
    |  ld1u TMP1, TMP1
    |  addli TMP3, TMP3, -LJ_TNIL
    |   shrui TMP0, RD, 1
    |  or TMP2, TMP2, TMP3
    |  addli TMP1, TMP1, -FF_next_N
    |   add TMP0, PC, TMP0
    |  or TMP1, TMP1, TMP2
    |  moveli TMP2, (-(BCBIAS_J*4 >> 16) & 65535)
    |  shl16insli TMP2, TMP2, 0
    |  bnez TMP1, >5
    |  add PC, TMP0, TMP2
    |  //lui TMP1, 0xfffe
    |  addli AT, zero, 0x7fff
    |  or TMP1, TMP1, AT
    |  addi AT, RA, -8+LO
    |  st4 AT, zero
    |  addi AT, RA, -8+HI
    |  st4 AT, TMP1
    |1:
    |  ins_next
    |5:  // Despecialize bytecode if any of the checks fail.
    |  addi TMP3, zero, BC_JMP
    |   addi TMP1, zero, BC_ITERC
    |  addi AT, PC, -4 + OFS_OP
    |  st1 AT, TMP3
    |   add PC, TMP0, TMP2
    |  addi AT, PC, OFS_OP
    |  st1 AT, TMP1
    |  b <1
    break;

  case BC_VARG:
    |  // RA = base*8, RB = (nresults+1)*8, RC = numparams*8
    |  addi TMP0, BASE, FRAME_PC
    |  ld4s TMP0, TMP0
    |  decode_RDtoRC8 RC, RD
    |   decode_RB8a RB, INS
    |  add RC, BASE, RC
    |   decode_RB8b RB
    |   add RA, BASE, RA
    |  addli RC, RC, FRAME_VARG
    |   add TMP2, RA, RB
    |  addli TMP3, BASE, -8		// TMP3 = vtop
    |  sub RC, RC, TMP0		// RC = vbase
    |  // Note: RC may now be even _above_ BASE if nargs was < numparams.
    |  sub TMP1, TMP3, RC
    |  beqz RB, >5			// Copy all varargs?
    |  addli TMP2, TMP2, -16
    |1:  // Copy vararg slots to destination slots.
    |  addi CARG1, RC, HI
    |  ld4s CARG1, CARG1
    |  cmpltu AT, RC, TMP3
    |   addi CARG2, RC, LO
    |   ld4s CARG2, CARG2
    |    addli RC, RC, 8
    |  cmoveqz CARG1, AT, TISNIL
    |  addi AT, RA, HI
    |  st4 AT, CARG1
    |  addi AT, RA, LO
    |   st4 AT, CARG2
    |  cmpltu AT, RA, TMP2
    |   addli RA, RA, 8
    |  bnez AT, <1
    |3:
    |  ins_next
    |
    |5:  // Copy all varargs.
    |  addlo TMP0, L->maxstack
    |  ld4u TMP0, TMP0
    |  addi MULTRES, zero, 8			// MULTRES = (0+1)*8
    |  blez TMP1, <3			// No vararg slots?
    |  add TMP2, RA, TMP1
    |  cmpltu AT, TMP0, TMP2
    |  addli MULTRES, TMP1, 8
    |  bnez AT, >7
    |6:
    |  //ldc1 f0, 0(RC)
    |   addli RC, RC, 8
    |  //sdc1 f0, 0(RA)
    |  cmpltu AT, RC, TMP3
    |  addli RA, RA, 8
    |  bnez AT, <6			// More vararg slots?
    |  b <3
    |
    |7:  // Grow stack for varargs.
    |  load_got lj_state_growstack
    |   addlo AT, L->top
    |   st AT, RA
    |  sub RA, RA, BASE
    |   addlo AT, L->base
    |   st AT, BASE
    |  sub BASE, RC, BASE		// Need delta, because BASE may change.
    |   addi AT, sp, SAVE_PC
    |   st AT, PC
    |  shrui CARG2, TMP1, 3
    |  move CARG1, L
    |  call_intern lj_state_growstack	// (lua_State *L, int n)
    |  move RC, BASE
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |  add RA, BASE, RA
    |  add RC, BASE, RC
    |  addli TMP3, BASE, -8
    |  b <6
    break;

  /* -- Returns ----------------------------------------------------------- */

  case BC_RETM:
    |  // RA = results*8, RD = extra_nresults*8
    |  add RD, RD, MULTRES		// MULTRES >= 8, so RD >= 8.
    |  // Fall through. Assumes BC_RET follows.
    break;

  case BC_RET:
    |  // RA = results*8, RD = (nresults+1)*8
    |  addi PC, BASE, FRAME_PC
    |  ld4s PC, PC
    |   add RA, BASE, RA
    |    move MULTRES, RD
    |1:
    |  andi TMP0, PC, FRAME_TYPE
    |  xori TMP1, PC, FRAME_VARG
    |  bnez TMP0, ->BC_RETV_Z
    |
    |->BC_RET_Z:
    |  // BASE = base, RA = resultptr, RD = (nresults+1)*8, PC = return
    |   addi INS, PC, -4
    |   ld4u INS, INS
    |    addi TMP2, BASE, -8
    |    addi RC, RD, -8
    |  decode_RA8a TMP0, INS
    |   decode_RB8a RB, INS
    |  decode_RA8b TMP0
    |   decode_RB8b RB
    |   add TMP3, TMP2, RB
    |  sub BASE, TMP2, TMP0
    |  beqz RC, >3
    |2:
    |   ld AT, RA
    |    addli RA, RA, 8
    |  addli RC, RC, -8
    |   st TMP2, AT
    |  addli TMP2, TMP2, 8
    |  bnez RC, <2
    |3:
    |  addli TMP3, TMP3, -8
    |5:
    |  cmpltu AT, TMP2, TMP3
    |  addi LFUNC:TMP1, BASE, FRAME_FUNC
    |  ld4s LFUNC:TMP1, LFUNC:TMP1
    |  bnez AT, >6
    |  ins_next1
    |  addlo TMP1, LFUNC:TMP1->pc
    |  ld4u TMP1, TMP1
    |  addi KBASE, TMP1, PC2PROTO(k)
    |  ld4u KBASE, KBASE
    |  ins_next2
    |
    |6:  // Fill up results with nil.
    |  addi AT, TMP2, HI
    |  st4 AT, TISNIL
    |  addli TMP2, TMP2, 8
    |  b <5
    |
    |->BC_RETV_Z:  // Non-standard return case.
    |  andi TMP2, TMP1, FRAME_TYPEP
    |  bnez TMP2, ->vm_return
    |  // Return from vararg function: relocate BASE down.
    |  sub BASE, BASE, TMP1
    |  addi PC, BASE, FRAME_PC
    |  ld4s PC, PC
    |  b <1
    break;

  case BC_RET0: case BC_RET1:
    |  // RA = results*8, RD = (nresults+1)*8
    |  addi PC, BASE, FRAME_PC
    |  ld4s PC, PC
    |   add RA, BASE, RA
    |    move MULTRES, RD
    |  andi TMP0, PC, FRAME_TYPE
    |  xori TMP1, PC, FRAME_VARG
    |  bnez TMP0, ->BC_RETV_Z
    |
    |  addi INS, PC, -4
    |  ld4u INS, INS
    |   addli TMP2, BASE, -8
    if (op == BC_RET1) {
      |  ld AT2, RA
    }
    |  decode_RB8a RB, INS
    |   decode_RA8a RA, INS
    |  decode_RB8b RB
    |   decode_RA8b RA
    if (op == BC_RET1) {
      |  st TMP2, AT2
    }
    |   sub BASE, TMP2, RA
    |5:
    |  cmpltu AT, RD, RB
    |  addi LFUNC:TMP1, BASE, FRAME_FUNC
    |  ld4s LFUNC:TMP1, LFUNC:TMP1
    |  bnez AT, >6
    |  ins_next1
    |  addlo TMP1, LFUNC:TMP1->pc
    |  ld4u TMP1, TMP1
    |  addi KBASE, TMP1, PC2PROTO(k)
    |  ld4u KBASE, KBASE
    |  ins_next2
    |
    |6:  // Fill up results with nil.
    |  addli TMP2, TMP2, 8
    |  addli RD, RD, 8
    if (op == BC_RET1) {
      |  addi AT, TMP2, HI
      |  st4 AT, TISNIL
    } else {
      |  addi AT, TMP2, -8+HI
      |  st4 AT, TISNIL
    }
    |  b <5
    break;

  /* -- Loops and branches ------------------------------------------------ */

  case BC_FORL:
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_IFORL follows.
    break;

  case BC_JFORI:
  case BC_JFORL:
#if !LJ_HASJIT
    break;
#endif
  case BC_FORI:
  case BC_IFORL:
    |  // RA = base*8, RD = target (after end of loop or start of loop)
    vk = (op == BC_IFORL || op == BC_JFORL);
    |  add RA, BASE, RA
    if (vk) {
      |  //ldc1 f0, FORL_IDX*8(RA)
      |  //ldc1 f4, FORL_STEP*8(RA)
      |  //ldc1 f2, FORL_STOP*8(RA)
      |   addi TMP3, RA, FORL_STEP*8+HI
      |   ld4s TMP3, TMP3
      |  //add.d f0, f0, f4
      |  //sdc1 f0, FORL_IDX*8(RA)
    } else {
      |  addi TMP1, RA, FORL_IDX*8+HI
      |  ld4s TMP1, TMP1
      |  addi TMP3, RA, FORL_STEP*8+HI
      |  ld4s TMP3, TMP3
      |  addi TMP2, RA, FORL_STOP*8+HI
      |  ld4s TMP2, TMP2
      |  cmpltui TMP1, TMP1, LJ_TISNUM
      |  cmpltui TMP0, TMP3, LJ_TISNUM
      |  cmpltui TMP2, TMP2, LJ_TISNUM
      |  and TMP1, TMP1, TMP0
      |  and TMP1, TMP1, TMP2
      |   //ldc1 f0, FORL_IDX*8(RA)
      |  //ldc1 f2, FORL_STOP*8(RA)
      |  beqz TMP1, ->vmeta_for
    }
    if (op != BC_JFORL) {
      |  shrui RD, RD, 1
      |  moveli TMP0, (-(BCBIAS_J*4 >> 16) & 65535)
      |  shl16insli TMP0, TMP0, 0
    }
    |  //c.le.d 0, f0, f2
    |  //c.le.d 1, f2, f0
    |  //sdc1 f0, FORL_EXT*8(RA)
    if (op == BC_JFORI) {
      |  addi TMP1, zero, 1
      |  addi TMP2, zero, 1
      |   add TMP0, RD, TMP0
      |  cmplts TMP3, TMP3, zero
      |  //movf TMP1, r0, 0
      |   add PC, PC, TMP0
      |  //movf TMP2, r0, 1
      |   addi RD, PC, -4+OFS_RD
      |   ld2u RD, RD
      |  cmovnez TMP1, TMP3, TMP2
      |  decode_RD8b RD
      |  bnez TMP1, =>BC_JLOOP
    } else if (op == BC_JFORL) {
      |  addi TMP1, zero, 1
      |  addi TMP2, zero, 1
      |  cmplts TMP3, TMP3, zero
      |  //movf TMP1, r0, 0
      |  //movf TMP2, r0, 1
      |  //movn TMP1, TMP2, TMP3
      |  bnez TMP1, =>BC_JLOOP
    } else {
      |  add TMP1, RD, TMP0
      |  cmplts TMP3, TMP3, zero
      |  move TMP2, TMP1
      if (op == BC_FORI) {
	|  //movt TMP1, r0, 0
	|  //movt TMP2, r0, 1
      } else {
	|  //movf TMP1, r0, 0
	|  //movf TMP2, r0, 1
      }
      |  cmovnez TMP1, TMP3, TMP2
      |  add PC, PC, TMP1
    }
    |  ins_next
    break;

  case BC_ITERL:
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_IITERL follows.
    break;

  case BC_JITERL:
#if !LJ_HASJIT
    break;
#endif
  case BC_IITERL:
    |  // RA = base*8, RD = target
    |  add RA, BASE, RA
    |  addi TMP1, RA, HI
    |  ld4s TMP1, TMP1
    |  addi AT, RA, LO
    |  ld4s TMP2, AT
    |  cmpeq AT, TMP1, TISNIL
    |  bnez AT, >1		// Stop if iterator returned nil.
    if (op == BC_JITERL) {
      |  addi AT, RA, -8+HI
      |  st4 AT, TMP1
      |  addi AT, RA, -8+LO
      |  st4 AT, TMP2
      |  b =>BC_JLOOP
    } else {
      |  branch_RD			// Otherwise save control var + branch.
      |  addi AT, RA, -8+HI
      |  st4 AT, TMP1
      |   addi AT, RA, -8+LO
      |   st4 AT, TMP2
    }
    |1:
    |  ins_next
    break;

  case BC_LOOP:
    |  // RA = base*8, RD = target (loop extent)
    |  // Note: RA/RD is only used by trace recorder to determine scope/extent
    |  // This opcode does NOT jump, it's only purpose is to detect a hot loop.
    |.if JIT
    |  hotloop
    |.endif
    |  // Fall through. Assumes BC_ILOOP follows.
    break;

  case BC_ILOOP:
    |  // RA = base*8, RD = target (loop extent)
    |  ins_next
    break;

  case BC_JLOOP:
    |.if JIT
    |  // RA = base*8 (ignored), RD = traceno*8
    |  addli TMP1, DISPATCH, DISPATCH_J(trace)
    |  ld TMP1, TMP1
    |  shrui RD, RD, 1
    |   move AT, zero
    |  add TMP1, TMP1, RD
    |  // Traces on MIPS don't store the trace number, so use 0.
    |   addli AT1, DISPATCH, DISPATCH_GL(vmstate)
    |   st4 AT1, AT
    |  ld TRACE:TMP2, TMP1
    |   addli AT, DISPATCH, DISPATCH_GL(jit_base)
    |   st4 AT, BASE
    |   addli AT, DISPATCH, DISPATCH_GL(jit_L)
    |   st4 AT, L
    |  addlo TMP2, TRACE:TMP2->mcode
    |  ld TMP2, TMP2
    |  addli JGL, DISPATCH, GG_DISP2G+32768
    |  jr TMP2
    |.endif
    break;

  case BC_JMP:
    |  // RA = base*8 (only used by trace recorder), RD = target
    |  branch_RD
    |  ins_next
    break;

  /* -- Function headers -------------------------------------------------- */

  case BC_FUNCF:
    |.if JIT
    |  hotcall
    |.endif
  case BC_FUNCV:  /* NYI: compiled vararg functions. */
    |  // Fall through. Assumes BC_IFUNCF/BC_IFUNCV follow.
    break;

  case BC_JFUNCF:
#if !LJ_HASJIT
    break;
#endif
  case BC_IFUNCF:
    |  // BASE = new base, RA = BASE+framesize*8, RB = LFUNC, RC = nargs*8
    |  addlo TMP2, L->maxstack
    |  ld4u TMP2, TMP2
    |   addi TMP1, PC, -4+PC2PROTO(numparams)
    |   ld1u TMP1, TMP1
    |    addi KBASE, PC, -4+PC2PROTO(k)
    |    ld4u KBASE, KBASE
    |  cmpltu AT, TMP2, RA
    |  shli TMP1, TMP1, 3
    |  bnez AT, ->vm_growstack_l
    if (op != BC_JFUNCF) {
      |  ins_next1
    }
    |2:
    |  cmpltu AT, NARGS8:RC, TMP1		// Check for missing parameters.
    |  add AT, BASE, NARGS8:RC
    |  bnez AT, >3
    if (op == BC_JFUNCF) {
      |  decode_RD8a RD, INS
      |  decode_RD8b RD
      |  b =>BC_JLOOP
    } else {
      |  ins_next2
    }
    |
    |3:  // Clear missing parameters.
    |  addi AT1, AT, HI
    |  st4 AT1, TISNIL
    |  addli NARGS8:RC, NARGS8:RC, 8
    |  b <2
    break;

  case BC_JFUNCV:
#if !LJ_HASJIT
    break;
#endif
    |  NYI  // NYI: compiled vararg functions
    break;  /* NYI: compiled vararg functions. */

  case BC_IFUNCV:
    |  // BASE = new base, RA = BASE+framesize*8, RB = LFUNC, RC = nargs*8
    |   add TMP1, BASE, RC
    |  addlo TMP2, L->maxstack
    |  ld4u TMP2, TMP2
    |  add TMP0, RA, RC
    |   addi AT, TMP1, LO
    |   st4 AT, LFUNC:RB
    |   addli TMP3, RC, 8+FRAME_VARG
    |  cmpltu AT1, TMP0, TMP2
    |    addi KBASE, PC, -4+PC2PROTO(k)
    |    ld4u KBASE, KBASE
    |  addi AT, TMP1, HI
    |  st4 AT, TMP3
    |  beqz AT1, ->vm_growstack_l
    |  addi TMP2, PC, -4+PC2PROTO(numparams)
    |  ld1u TMP2, TMP2
    |   move RA, BASE
    |   move RC, TMP1
    |  ins_next1
    |  addli BASE, TMP1, 8
    |  beqz TMP2, >3
    |1:
    |  addi TMP0, RA, HI
    |  ld4s TMP0, TMP0
    |   addi TMP3, RA, LO
    |   ld4s TMP3, TMP3
    |  cmpltu AT, RA, RC			// Less args than parameters?
    |  move CARG1, TMP0
    |  cmoveqz TMP0, AT, TISNIL
    |  cmovnez CARG1, AT, TISNIL
    |   addi AT, TMP1, 8+LO
    |   st4 AT, TMP3
    |    addli TMP2, TMP2, -1
    |  addi AT, TMP1, 8+HI
    |  st4 AT, TMP0
    |    addli TMP1, TMP1, 8
    |  addi AT, RA, HI
    |  st4 AT, CARG1
    |   addli RA, RA, 8
    |  bnez TMP2, <1
    |3:
    |  ins_next2
    break;

  case BC_FUNCC:
  case BC_FUNCCW:
    |  // BASE = new base, RA = BASE+framesize*8, RB = CFUNC, RC = nargs*8
    if (op == BC_FUNCC) {
      |  addlo CFUNCADDR, CFUNC:RB->f
      |  ld CFUNCADDR, CFUNCADDR
    } else {
      |  addli CFUNCADDR, DISPATCH, DISPATCH_GL(wrapf)
      |  ld CFUNCADDR, CFUNCADDR
    }
    |  add TMP1, RA, NARGS8:RC
    |  addlo TMP2, L->maxstack
    |  ld4u TMP2, TMP2
    |   add RC, BASE, NARGS8:RC
    |  addlo AT, L->base
    |  st AT, BASE
    |  cmpltu AT, TMP2, TMP1
    |   addlo AT1, L->top
    |   st AT1, RC
    |    li_vmstate C
    if (op == BC_FUNCCW) {
      |  addlo CARG2, CFUNC:RB->f
      |  ld CARG2, CARG2
    }
    |  move CARG1, L
    |  bnez AT, ->vm_growstack_c	// Need to grow stack.
    |   st_vmstate
    |  jalr CFUNCADDR			// (lua_State *L [, lua_CFunction f])
    |  // Returns nresults.
    |  addlo BASE, L->base
    |  ld BASE, BASE
    |   shli RD, CRET1, 3
    |  addlo TMP1, L->top
    |  ld TMP1, TMP1
    |    li_vmstate INTERP
    |  addi PC, BASE, FRAME_PC
    |  ld4s PC, PC
    |   sub RA, TMP1, RD		// RA = L->top - nresults*8
    |   st_vmstate
    |  b ->vm_returnc
    break;

  /* ---------------------------------------------------------------------- */

  default:
    fprintf(stderr, "Error: undefined opcode BC_%s\n", bc_names[op]);
    exit(2);
    break;
  }
}

static int build_backend(BuildCtx *ctx)
{
  int op;

  dasm_growpc(Dst, BC__MAX);

  build_subroutines(ctx);

  |.code_op
  for (op = 0; op < BC__MAX; op++)
    build_ins(ctx, (BCOp)op, op);

  return BC__MAX;
}

/* Emit pseudo frame-info for all assembler functions. */
static void emit_asm_debug(BuildCtx *ctx)
{
  int fcofs = (int)((uint8_t *)ctx->glob[GLOB_vm_ffi_call] - ctx->code);
  int i;
  switch (ctx->mode) {
  case BUILD_elfasm:
    fprintf(ctx->fp, "\t.section .debug_frame,\"\",@progbits\n");
    fprintf(ctx->fp,
	".Lframe0:\n"
	"\t.4byte .LECIE0-.LSCIE0\n"
	".LSCIE0:\n"
	"\t.4byte 0xffffffff\n"
	"\t.byte 0x1\n"
	"\t.string \"\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE0:\n\n");
    fprintf(ctx->fp,
	".LSFDE0:\n"
	"\t.4byte .LEFDE0-.LASFDE0\n"
	".LASFDE0:\n"
	"\t.4byte .Lframe0\n"
	"\t.4byte .Lbegin\n"
	"\t.4byte %d\n"
	"\t.byte 0xe\n\t.uleb128 %d\n"
	"\t.byte 0x9f\n\t.sleb128 1\n"
	"\t.byte 0x9e\n\t.sleb128 2\n",
	fcofs, CFRAME_SIZE);
    for (i = 23; i >= 16; i--)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+i, 26-i);
    for (i = 30; i >= 20; i -= 2)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+32+i, 42-i);
    fprintf(ctx->fp,
	"\t.align 2\n"
	".LEFDE0:\n\n");
#if LJ_HASFFI
    fprintf(ctx->fp,
	".LSFDE1:\n"
	"\t.4byte .LEFDE1-.LASFDE1\n"
	".LASFDE1:\n"
	"\t.4byte .Lframe0\n"
	"\t.4byte lj_vm_ffi_call\n"
	"\t.4byte %d\n"
	"\t.byte 0x9f\n\t.uleb128 1\n"
	"\t.byte 0x90\n\t.uleb128 2\n"
	"\t.byte 0xd\n\t.uleb128 0x10\n"
	"\t.align 2\n"
	".LEFDE1:\n\n", (int)ctx->codesz - fcofs);
#endif
    fprintf(ctx->fp, "\t.section .eh_frame,\"aw\",@progbits\n");
    fprintf(ctx->fp,
	"\t.globl lj_err_unwind_dwarf\n"
	".Lframe1:\n"
	"\t.4byte .LECIE1-.LSCIE1\n"
	".LSCIE1:\n"
	"\t.4byte 0\n"
	"\t.byte 0x1\n"
	"\t.string \"zPR\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.uleb128 6\n"			/* augmentation length */
	"\t.byte 0\n"
	"\t.4byte lj_err_unwind_dwarf\n"
	"\t.byte 0\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE1:\n\n");
    fprintf(ctx->fp,
	".LSFDE2:\n"
	"\t.4byte .LEFDE2-.LASFDE2\n"
	".LASFDE2:\n"
	"\t.4byte .LASFDE2-.Lframe1\n"
	"\t.4byte .Lbegin\n"
	"\t.4byte %d\n"
	"\t.uleb128 0\n"			/* augmentation length */
	"\t.byte 0xe\n\t.uleb128 %d\n"
	"\t.byte 0x9f\n\t.sleb128 1\n"
	"\t.byte 0x9e\n\t.sleb128 2\n",
	fcofs, CFRAME_SIZE);
    for (i = 23; i >= 16; i--)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+i, 26-i);
    for (i = 30; i >= 20; i -= 2)
      fprintf(ctx->fp, "\t.byte %d\n\t.uleb128 %d\n", 0x80+32+i, 42-i);
    fprintf(ctx->fp,
	"\t.align 2\n"
	".LEFDE2:\n\n");
#if LJ_HASFFI
    fprintf(ctx->fp,
	".Lframe2:\n"
	"\t.4byte .LECIE2-.LSCIE2\n"
	".LSCIE2:\n"
	"\t.4byte 0\n"
	"\t.byte 0x1\n"
	"\t.string \"zR\"\n"
	"\t.uleb128 0x1\n"
	"\t.sleb128 -4\n"
	"\t.byte 31\n"
	"\t.uleb128 1\n"			/* augmentation length */
	"\t.byte 0\n"
	"\t.byte 0xc\n\t.uleb128 29\n\t.uleb128 0\n"
	"\t.align 2\n"
	".LECIE2:\n\n");
    fprintf(ctx->fp,
	".LSFDE3:\n"
	"\t.4byte .LEFDE3-.LASFDE3\n"
	".LASFDE3:\n"
	"\t.4byte .LASFDE3-.Lframe2\n"
	"\t.4byte lj_vm_ffi_call\n"
	"\t.4byte %d\n"
	"\t.uleb128 0\n"			/* augmentation length */
	"\t.byte 0x9f\n\t.uleb128 1\n"
	"\t.byte 0x90\n\t.uleb128 2\n"
	"\t.byte 0xd\n\t.uleb128 0x10\n"
	"\t.align 2\n"
	".LEFDE3:\n\n", (int)ctx->codesz - fcofs);
#endif
    break;
  default:
    break;
  }
}

